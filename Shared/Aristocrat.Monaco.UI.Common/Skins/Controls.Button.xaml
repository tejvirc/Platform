                    <ControlTemplate.Triggers>
                        <Trigger Property="IsPressed"
                                 Value="True"
                                 AutomationProperties.AutomationId="Trigger38_880761DA">
                            <Setter Property="Background"
                                    TargetName="Border">
                                <Setter.Value>
                                    <LinearGradientBrush StartPoint="0,0"
                                                         EndPoint="1,1"
                                                         AutomationProperties.AutomationId="LinearGradientBrush39_880761DA">
                                        <GradientStop Color="{DynamicResource Color3}"
                                                      Offset="0"
                                                      AutomationProperties.AutomationId="GradientStop40_880761DA" />
                                        <GradientStop Color="{DynamicResource Color2}"
                                                      Offset=".05"
                                                      AutomationProperties.AutomationId="GradientStop41_880761DA" />
                                    </LinearGradientBrush>
                                </Setter.Value>
                            </Setter>
                            <Setter Property="Foreground"
                                    Value="{DynamicResource LightBrush5}" />
                        </Trigger>
                        <Trigger Property="IsEnabled"
                                 Value="False"
                                 AutomationProperties.AutomationId="Trigger42_880761DA">
                            <Setter Property="Opacity"
                                    TargetName="DisabledVisualElement"
                                    Value="0.7" />
                            <Setter Property="Opacity"
                                    TargetName="PART_ContentPresenter"
                                    Value="0.7" />
                            <Setter Property="Foreground"
                                    Value="{DynamicResource DarkBrush1}" />
                        </Trigger>
                        <Trigger Property="IsEnabled"
                                 Value="True"
                                 AutomationProperties.AutomationId="Trigger43_880761DA">
                            <Setter Property="Opacity"
                                    TargetName="DisabledVisualElement"
                                    Value="0" />
                            <Setter Property="Opacity"
                                    TargetName="PART_ContentPresenter"
                                    Value="1" />
                            <Setter Property="Foreground"
                                    Value="{DynamicResource DarkBrush3}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="MonacoGridNavButton"
           TargetType="Button"
           BasedOn="{StaticResource MonacoButton1}">
        <Setter Property="Margin"
                Value="5" />
        <Setter Property="Width"
                Value="50" />
        <Setter Property="Height"
                Value="50" />
    </Style>
    <Style x:Key="MonacoInfoButton"
           TargetType="Button">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Grid AutomationProperties.AutomationId="Grid44_880761DA">
                        <Rectangle Margin="8, 0, 0, 0"
                                   Width="36"
                                   Height="24"
                                   Fill="Transparent"
                                   AutomationProperties.AutomationId="Rectangle45_880761DA"></Rectangle>
                        <Ellipse Height="22"
                                 Width="22"
                                 Fill="White"
                                 AutomationProperties.AutomationId="Ellipse46_880761DA" />
                        <Ellipse Height="20"
                                 Width="20"
                                 Fill="{DynamicResource Brush2}"
                                 AutomationProperties.AutomationId="Ellipse47_880761DA" />
                        <TextBlock Text="i"
                                   Foreground="White"
                                   FontSize="14"
                                   VerticalAlignment="Center"
                                   HorizontalAlignment="Center"
                                   AutomationProperties.AutomationId="TextBlock48_880761DA" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="MonacoWarningButton"
           TargetType="Button">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Grid AutomationProperties.AutomationId="Grid49_880761DA">
                        <Polygon Points="15,-4 30,22, 0,22"
                                 Fill="Yellow"
                                 Stroke="Black"
                                 StrokeThickness="1"
                                 VerticalAlignment="Center"
                                 HorizontalAlignment="Center"
                                 AutomationProperties.AutomationId="Polygon50_880761DA" />
                        <TextBlock Text="!"
                                   Foreground="Black"
                                   FontSize="14"
                                   FontWeight="Black"
                                   VerticalAlignment="Center"
                                   HorizontalAlignment="Center"
                                   AutomationProperties.AutomationId="TextBlock51_880761DA" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="MonacoErrorScreenButton"
           TargetType="Button">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Border Background="Transparent"
                            AutomationProperties.AutomationId="Border52_880761DA">
                        <ContentPresenter AutomationProperties.AutomationId="ContentPresenter53_880761DA" />
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>