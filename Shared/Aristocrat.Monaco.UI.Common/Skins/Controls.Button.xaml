<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controls="http://metro.mahapps.com/winfx/xaml/controls">
    <!-- Button Style -->
    <Style x:Key="MonacoButton"
           TargetType="{x:Type Button}">
        <Setter Property="Background">
            <Setter.Value>
                <LinearGradientBrush StartPoint=".5,1"
                                     EndPoint=".5,0"
                                     AutomationProperties.AutomationId="LinearGradientBrush0_880761DA">
                    <GradientStop Color="{DynamicResource Color6}"
                                  Offset="0"
                                  AutomationProperties.AutomationId="GradientStop1_880761DA" />
                    <GradientStop Color="{DynamicResource LightColor2}"
                                  Offset=".3"
                                  AutomationProperties.AutomationId="GradientStop2_880761DA" />
                    <GradientStop Color="{DynamicResource LightColor4}"
                                  Offset=".5"
                                  AutomationProperties.AutomationId="GradientStop3_880761DA" />
                </LinearGradientBrush>
            </Setter.Value>
        </Setter>
        <Setter Property="BorderThickness"
                Value="0" />
        <Setter Property="controls:ControlsHelper.ContentCharacterCasing"
                Value="Normal" />
        <Setter Property="controls:ControlsHelper.CornerRadius"
                Value="3" />
        <Setter Property="FontFamily"
                Value="{DynamicResource MonacoFontFamily}" />
        <Setter Property="FontSize"
                Value="{DynamicResource ButtonFontSize}" />
        <Setter Property="FontWeight"
                Value="Normal" />
        <Setter Property="Foreground"
                Value="{DynamicResource DarkBrush3}" />
        <Setter Property="Margin"
                Value="0 0 15 0" />
        <Setter Property="MinHeight"
                Value="25" />
        <Setter Property="MinWidth"
                Value="100" />
        <Setter Property="Padding"
                Value="10,12" />
        <Setter Property="SnapsToDevicePixels"
                Value="True" />
        <Setter Property="Effect">
            <Setter.Value>
                <DropShadowEffect Color="{DynamicResource LightColor2}"
                                  ShadowDepth="0"
                                  Opacity="1"
                                  BlurRadius="15"
                                  AutomationProperties.AutomationId="DropShadowEffect4_880761DA" />
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Grid AutomationProperties.AutomationId="Grid5_880761DA">
                        <Border x:Name="Border"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                Background="{TemplateBinding Background}"
                                CornerRadius="{Binding (controls:ControlsHelper.CornerRadius), Mode=OneWay, RelativeSource={RelativeSource TemplatedParent}}"
                                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                AutomationProperties.AutomationId="Border6_880761DA" />
                        <Border x:Name="DisabledVisualElement"
                                Background="{DynamicResource ControlsDisabledBrush}"
                                CornerRadius="{Binding (controls:ControlsHelper.CornerRadius), Mode=OneWay, RelativeSource={RelativeSource TemplatedParent}}"
                                IsHitTestVisible="False"
                                Opacity="0"
                                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                AutomationProperties.AutomationId="Border7_880761DA" />
                        <controls:ContentControlEx x:Name="PART_ContentPresenter"
                                                   ContentCharacterCasing="{Binding (controls:ControlsHelper.ContentCharacterCasing), RelativeSource={RelativeSource TemplatedParent}}"
                                                   ContentTemplate="{TemplateBinding ContentTemplate}"
                                                   Content="{TemplateBinding Content}"
                                                   ContentStringFormat="{TemplateBinding ContentStringFormat}"
                                                   HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                   Margin="{TemplateBinding BorderThickness}"
                                                   Padding="{TemplateBinding Padding}"
                                                   RecognizesAccessKey="True"
                                                   SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                                   VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsPressed"
                                 Value="True"
                                 AutomationProperties.AutomationId="Trigger8_880761DA">
                            <Setter Property="Background"
                                    TargetName="Border">
                                <Setter.Value>
                                    <LinearGradientBrush StartPoint="1,1"
                                                         EndPoint="0,0"
                                                         AutomationProperties.AutomationId="LinearGradientBrush9_880761DA">
                                        <GradientStop Color="{DynamicResource Color1}"
                                                      Offset="0"
                                                      AutomationProperties.AutomationId="GradientStop10_880761DA" />
                                        <GradientStop Color="{DynamicResource Color2}"
                                                      Offset=".4"
                                                      AutomationProperties.AutomationId="GradientStop11_880761DA" />
                                        <GradientStop Color="{DynamicResource Color3}"
                                                      Offset=".9"
                                                      AutomationProperties.AutomationId="GradientStop12_880761DA" />
                                        <GradientStop Color="{DynamicResource Color4}"
                                                      Offset=".95"
                                                      AutomationProperties.AutomationId="GradientStop13_880761DA" />
                                    </LinearGradientBrush>
                                </Setter.Value>
                            </Setter>
                        </Trigger>
                        <Trigger Property="IsEnabled"
                                 Value="False"
                                 AutomationProperties.AutomationId="Trigger14_880761DA">
                            <Setter Property="Opacity"
                                    TargetName="DisabledVisualElement"
                                    Value="0.7" />
                            <Setter Property="Opacity"
                                    TargetName="PART_ContentPresenter"
                                    Value="0.7" />
                            <Setter Property="Foreground"
                                    Value="{DynamicResource DarkBrush1}" />
                            <Setter Property="Effect"
                                    Value="{x:Null}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Style.Triggers>
            <Trigger Property="IsPressed"
                     Value="True"
                     AutomationProperties.AutomationId="Trigger15_880761DA">
                <Setter Property="Foreground"
                        Value="{DynamicResource LightBrush5}" />
                <Setter Property="Effect"
                        Value="{x:Null}" />
            </Trigger>
        </Style.Triggers>
    </Style>
    <!-- Button 2 Style -->
    <Style x:Key="MonacoButton2"
           TargetType="Button">
        <Setter Property="Background">
            <Setter.Value>
                <LinearGradientBrush StartPoint=".5,1"
                                     EndPoint=".5,0"
                                     AutomationProperties.AutomationId="LinearGradientBrush16_880761DA">
                    <GradientStop Color="{DynamicResource LightColor2}"
                                  Offset="0"
                                  AutomationProperties.AutomationId="GradientStop17_880761DA" />
                    <GradientStop Color="{DynamicResource LightColor5}"
                                  Offset=".5"
                                  AutomationProperties.AutomationId="GradientStop18_880761DA" />
                </LinearGradientBrush>
            </Setter.Value>
        </Setter>
        <Setter Property="Foreground"
                Value="{DynamicResource DarkBrush1}" />
        <Setter Property="BorderThickness"
                Value="0" />
        <Setter Property="controls:ControlsHelper.ContentCharacterCasing"
                Value="Upper" />
        <Setter Property="controls:ControlsHelper.CornerRadius"
                Value="0" />
        <Setter Property="FontFamily"
                Value="{DynamicResource MonacoFontFamily}" />
        <Setter Property="FontSize"
                Value="{DynamicResource ButtonFontSize}" />
        <Setter Property="Padding"
                Value="10,13" />
        <Setter Property="FontWeight"
                Value="Normal" />
        <Setter Property="SnapsToDevicePixels"
                Value="True" />
        <Setter Property="Effect">
            <Setter.Value>
                <DropShadowEffect Color="{DynamicResource HighlightColor1}"
                                  ShadowDepth="0"
                                  Opacity="1"
                                  BlurRadius="20"
                                  AutomationProperties.AutomationId="DropShadowEffect19_880761DA" />
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Grid AutomationProperties.AutomationId="Grid20_880761DA">
                        <Border x:Name="Border"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                Background="{TemplateBinding Background}"
                                CornerRadius="{Binding (controls:ControlsHelper.CornerRadius), Mode=OneWay, RelativeSource={RelativeSource TemplatedParent}}"
                                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                AutomationProperties.AutomationId="Border21_880761DA" />
                        <Border x:Name="DisabledVisualElement"
                                Background="{DynamicResource ControlsDisabledBrush}"
                                CornerRadius="{Binding (controls:ControlsHelper.CornerRadius), Mode=OneWay, RelativeSource={RelativeSource TemplatedParent}}"
                                IsHitTestVisible="False"
                                Opacity="0"
                                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                AutomationProperties.AutomationId="Border22_880761DA" />
                        <controls:ContentControlEx x:Name="PART_ContentPresenter"
                                                   ContentCharacterCasing="{Binding (controls:ControlsHelper.ContentCharacterCasing), RelativeSource={RelativeSource TemplatedParent}}"
                                                   ContentTemplate="{TemplateBinding ContentTemplate}"
                                                   Content="{TemplateBinding Content}"
                                                   ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}"
                                                   ContentStringFormat="{TemplateBinding ContentStringFormat}"
                                                   HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                   VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                                                   Margin="{TemplateBinding BorderThickness}"
                                                   Padding="{TemplateBinding Padding}"
                                                   RecognizesAccessKey="True"
                                                   SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled"
                                 Value="False"
                                 AutomationProperties.AutomationId="Trigger23_880761DA">
                            <Setter Property="Opacity"
                                    TargetName="DisabledVisualElement"
                                    Value="0.7" />
                            <Setter Property="Opacity"
                                    TargetName="PART_ContentPresenter"
                                    Value="0.7" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Style.Triggers>
            <Trigger Property="IsPressed"
                     Value="True"
                     AutomationProperties.AutomationId="Trigger24_880761DA">
                <Setter Property="Background">
                    <Setter.Value>
                        <LinearGradientBrush StartPoint="1,1"
                                             EndPoint="0,0"
                                             AutomationProperties.AutomationId="LinearGradientBrush25_880761DA">
                            <GradientStop Color="{DynamicResource Color1}"
                                          Offset="0"
                                          AutomationProperties.AutomationId="GradientStop26_880761DA" />
                            <GradientStop Color="{DynamicResource Color2}"
                                          Offset=".4"
                                          AutomationProperties.AutomationId="GradientStop27_880761DA" />
                            <GradientStop Color="{DynamicResource Color3}"
                                          Offset=".9"
                                          AutomationProperties.AutomationId="GradientStop28_880761DA" />
                            <GradientStop Color="{DynamicResource Color4}"
                                          Offset=".95"
                                          AutomationProperties.AutomationId="GradientStop29_880761DA" />
                        </LinearGradientBrush>
                    </Setter.Value>
                </Setter>
                <Setter Property="Foreground"
                        Value="{DynamicResource LightBrush3}" />
                <Setter Property="Effect"
                        Value="{x:Null}" />
            </Trigger>
            <Trigger Property="IsEnabled"
                     Value="False"
                     AutomationProperties.AutomationId="Trigger30_880761DA">
                <Setter Property="Foreground"
                        Value="{DynamicResource DarkBrush1}" />
                <Setter Property="Effect"
                        Value="{x:Null}" />
            </Trigger>
        </Style.Triggers>
    </Style>
    <!-- Button 1 Style -->
