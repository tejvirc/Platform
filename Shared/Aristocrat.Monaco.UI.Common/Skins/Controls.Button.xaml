                <LinearGradientBrush StartPoint=".5,0"
                                     EndPoint=".5,1"
                                     AutomationProperties.AutomationId="LinearGradientBrush31_880761DA">
                    <GradientStop Color="{DynamicResource Color6}"
                                  Offset="0"
                                  AutomationProperties.AutomationId="GradientStop32_880761DA" />
                    <GradientStop Color="{DynamicResource LightColor1}"
                                  Offset=".4"
                                  AutomationProperties.AutomationId="GradientStop33_880761DA" />
                    <GradientStop Color="{DynamicResource LightColor3}"
                                  Offset=".6"
                                  AutomationProperties.AutomationId="GradientStop34_880761DA" />
                </LinearGradientBrush>
            </Setter.Value>
        </Setter>
        <Setter Property="BorderThickness"
