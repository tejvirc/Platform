<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="SliderRepeatButton"
           TargetType="RepeatButton">
        <Setter Property="SnapsToDevicePixels"
                Value="true" />
        <Setter Property="OverridesDefaultStyle"
                Value="true" />
        <Setter Property="IsTabStop"
                Value="false" />
        <Setter Property="Focusable"
                Value="false" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="RepeatButton">
                    <Border Background="Transparent"
                            AutomationProperties.AutomationId="Border0_332BE05C" />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="SliderRepeatButton1"
           TargetType="RepeatButton">
        <Setter Property="Background">
            <Setter.Value>
                <LinearGradientBrush StartPoint=".5,1"
                                     EndPoint=".5,0"
                                     AutomationProperties.AutomationId="LinearGradientBrush1_332BE05C">
                    <GradientStop Color="{DynamicResource LightColor2}"
                                  Offset="0"
                                  AutomationProperties.AutomationId="GradientStop2_332BE05C" />
                    <GradientStop Color="{DynamicResource LightColor5}"
                                  Offset=".5"
                                  AutomationProperties.AutomationId="GradientStop3_332BE05C" />
                </LinearGradientBrush>
            </Setter.Value>
        </Setter>
        <Setter Property="SnapsToDevicePixels"
                Value="true" />
        <Setter Property="OverridesDefaultStyle"
                Value="true" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="RepeatButton">
                    <Border SnapsToDevicePixels="True"
                            Background="{TemplateBinding Background}"
                            BorderThickness="1"
                            BorderBrush="{TemplateBinding Background}"
                            Height="12"
                            AutomationProperties.AutomationId="Border4_332BE05C" />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="SliderThumb"
           TargetType="Thumb">
        <Setter Property="Background">
            <Setter.Value>
                <LinearGradientBrush StartPoint=".5,1"
                                     EndPoint=".5,0"
                                     AutomationProperties.AutomationId="LinearGradientBrush5_332BE05C">
                    <GradientStop Color="{DynamicResource LightColor2}"
                                  Offset="0"
                                  AutomationProperties.AutomationId="GradientStop6_332BE05C" />
                    <GradientStop Color="{DynamicResource LightColor5}"
                                  Offset=".5"
                                  AutomationProperties.AutomationId="GradientStop7_332BE05C" />
                </LinearGradientBrush>
            </Setter.Value>
        </Setter>
        <Setter Property="SnapsToDevicePixels"
                Value="true" />
        <Setter Property="OverridesDefaultStyle"
                Value="true" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Thumb">
                    <StackPanel Orientation="Vertical"
                                AutomationProperties.AutomationId="StackPanel8_332BE05C">
                        <Rectangle Height="40"
                                   Width="30"
                                   Fill="{TemplateBinding Background}"
                                   Margin="0,32,0,0"
                                   AutomationProperties.AutomationId="Rectangle9_332BE05C" />
                        <TextBlock HorizontalAlignment="Center"
                                   FontFamily="{DynamicResource MonacoFontFamily}"
                                   FontSize="{DynamicResource ButtonFontSize}"
                                   Foreground="White"
                                   Margin="0,10,0,0"
                                   Text="{Binding Value, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Slider}}}"
                                   AutomationProperties.AutomationId="TextBlock10_332BE05C" />
                    </StackPanel>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <ControlTemplate x:Key="Slider"
                     TargetType="Slider">
        <Grid AutomationProperties.AutomationId="Grid11_332BE05C">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"
                               AutomationProperties.AutomationId="RowDefinition12_332BE05C" />
                <RowDefinition Height="Auto"
                               MinHeight="{TemplateBinding MinHeight}"
                               AutomationProperties.AutomationId="RowDefinition13_332BE05C" />
                <RowDefinition Height="Auto"
                               AutomationProperties.AutomationId="RowDefinition14_332BE05C" />
            </Grid.RowDefinitions>
            <TickBar x:Name="TopTick"
                     Fill="LightGray"
                     VerticalAlignment="Top"
                     SnapsToDevicePixels="True"
                     Grid.Row="0"
                     Placement="Top"
                     Height="5"
                     Visibility="Collapsed"
                     AutomationProperties.AutomationId="TickBar15_332BE05C" />
            <Border x:Name="TrackBackground"
                    VerticalAlignment="Center"
                    BorderBrush="{DynamicResource Brush3}"
                    Background="{DynamicResource Brush3}"
                    Height="12"
                    Grid.Row="1"
                    BorderThickness="1"
                    AutomationProperties.AutomationId="Border16_332BE05C" />
            <Track Grid.Row="1"
                   x:Name="PART_Track"
                   AutomationProperties.AutomationId="Track17_332BE05C">
                <Track.DecreaseRepeatButton>
                    <RepeatButton Style="{StaticResource SliderRepeatButton1}"
                                  Command="Slider.DecreaseLarge"
                                  AutomationProperties.AutomationId="RepeatButton18_332BE05C" />
                </Track.DecreaseRepeatButton>
                <Track.Thumb>
                    <Thumb Style="{StaticResource SliderThumb}"
                           x:Name="PART_Thumb"
                           AutomationProperties.AutomationId="Thumb19_332BE05C" />
                </Track.Thumb>
                <Track.IncreaseRepeatButton>
                    <RepeatButton Style="{StaticResource SliderRepeatButton}"
                                  Command="Slider.IncreaseLarge"
                                  AutomationProperties.AutomationId="RepeatButton20_332BE05C" />
                </Track.IncreaseRepeatButton>
            </Track>
            <TickBar x:Name="BottomTick"
                     Fill="LightGray"
                     SnapsToDevicePixels="True"
                     Grid.Row="2"
                     Placement="Bottom"
                     Height="4"
                     Visibility="Collapsed"
                     AutomationProperties.AutomationId="TickBar21_332BE05C" />
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="TickPlacement"
                     Value="TopLeft"
                     AutomationProperties.AutomationId="Trigger22_332BE05C">
                <Setter TargetName="TopTick"
                        Property="Visibility"
                        Value="Visible" />
            </Trigger>
            <Trigger Property="TickPlacement"
                     Value="BottomRight"
                     AutomationProperties.AutomationId="Trigger23_332BE05C">
                <Setter TargetName="BottomTick"
                        Property="Visibility"
                        Value="Visible" />
            </Trigger>
            <Trigger Property="TickPlacement"
                     Value="Both"
                     AutomationProperties.AutomationId="Trigger24_332BE05C">
                <Setter TargetName="TopTick"
                        Property="Visibility"
                        Value="Visible" />
                <Setter TargetName="BottomTick"
                        Property="Visibility"
                        Value="Visible" />
            </Trigger>
            <Trigger Property="IsEnabled"
                     Value="False"
                     AutomationProperties.AutomationId="Trigger25_332BE05C">
                <Setter Property="Opacity"
                        TargetName="TrackBackground"
                        Value="0.7" />
                <Setter Property="Opacity"
                        TargetName="PART_Track"
                        Value="0.7" />
                <Setter Property="Opacity"
                        TargetName="PART_Thumb"
                        Value="0.9" />
                <Setter Property="Foreground"
                        Value="{DynamicResource DarkBrush1}" />
                <Setter Property="Effect"
                        Value="{x:Null}" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="MonacoSlider"
           TargetType="Slider">
        <Setter Property="Focusable"
                Value="False" />
        <Setter Property="SnapsToDevicePixels"
                Value="true" />
        <Setter Property="OverridesDefaultStyle"
                Value="true" />
        <Style.Triggers>
            <Trigger Property="Orientation"
                     Value="Horizontal"
                     AutomationProperties.AutomationId="Trigger26_332BE05C">
                <Setter Property="Template"
                        Value="{StaticResource Slider}" />
            </Trigger>
        </Style.Triggers>
    </Style>
</ResourceDictionary>