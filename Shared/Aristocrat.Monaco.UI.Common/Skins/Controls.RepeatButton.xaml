                Value="{DynamicResource ButtonFontSize}" />
        <Setter Property="Padding"
                Value="10,13" />
        <Setter Property="FontWeight"
                Value="Normal" />
        <Setter Property="SnapsToDevicePixels"
                Value="True" />
        <Setter Property="Effect">
            <Setter.Value>
                <DropShadowEffect Color="{DynamicResource HighlightColor1}"
                                  ShadowDepth="0"
                                  Opacity="1"
                                  BlurRadius="20"
                                  AutomationProperties.AutomationId="DropShadowEffect19_EE772BB0" />
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="RepeatButton">
                    <Grid AutomationProperties.AutomationId="Grid20_EE772BB0">
                        <Border x:Name="Border"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                Background="{TemplateBinding Background}"
                                CornerRadius="{Binding (controls:ControlsHelper.CornerRadius), Mode=OneWay, RelativeSource={RelativeSource TemplatedParent}}"
                                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                AutomationProperties.AutomationId="Border21_EE772BB0" />
                        <Border x:Name="DisabledVisualElement"
                                Background="{DynamicResource ControlsDisabledBrush}"
                                CornerRadius="{Binding (controls:ControlsHelper.CornerRadius), Mode=OneWay, RelativeSource={RelativeSource TemplatedParent}}"
                                IsHitTestVisible="False"
                                Opacity="0"
                                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                AutomationProperties.AutomationId="Border22_EE772BB0" />
                        <controls:ContentControlEx x:Name="PART_ContentPresenter"
                                                   ContentCharacterCasing="{Binding (controls:ControlsHelper.ContentCharacterCasing), RelativeSource={RelativeSource TemplatedParent}}"
                                                   ContentTemplate="{TemplateBinding ContentTemplate}"
                                                   Content="{TemplateBinding Content}"
                                                   ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}"
                                                   ContentStringFormat="{TemplateBinding ContentStringFormat}"
                                                   HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                   VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                                                   Margin="{TemplateBinding BorderThickness}"
                                                   Padding="{TemplateBinding Padding}"
                                                   RecognizesAccessKey="True"
                                                   SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled"
                                 Value="False"
                                 AutomationProperties.AutomationId="Trigger23_EE772BB0">
                            <Setter Property="Opacity"
                                    TargetName="DisabledVisualElement"
                                    Value="0.7" />
                            <Setter Property="Opacity"
                                    TargetName="PART_ContentPresenter"
                                    Value="0.7" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Style.Triggers>
            <Trigger Property="IsPressed"
                     Value="True"
                     AutomationProperties.AutomationId="Trigger24_EE772BB0">
                <Setter Property="Background">
                    <Setter.Value>
                        <LinearGradientBrush StartPoint="1,1"
                                             EndPoint="0,0"
                                             AutomationProperties.AutomationId="LinearGradientBrush25_EE772BB0">
                            <GradientStop Color="{DynamicResource Color1}"
                                          Offset="0"
                                          AutomationProperties.AutomationId="GradientStop26_EE772BB0" />
                            <GradientStop Color="{DynamicResource Color2}"
                                          Offset=".4"
                                          AutomationProperties.AutomationId="GradientStop27_EE772BB0" />
                            <GradientStop Color="{DynamicResource Color3}"
                                          Offset=".9"
                                          AutomationProperties.AutomationId="GradientStop28_EE772BB0" />
                            <GradientStop Color="{DynamicResource Color4}"
                                          Offset=".95"
                                          AutomationProperties.AutomationId="GradientStop29_EE772BB0" />
                        </LinearGradientBrush>
                    </Setter.Value>
                </Setter>
                <Setter Property="Foreground"
                        Value="{DynamicResource LightBrush3}" />
                <Setter Property="Effect"
                        Value="{x:Null}" />
            </Trigger>
            <Trigger Property="IsEnabled"
                     Value="False"
                     AutomationProperties.AutomationId="Trigger30_EE772BB0">
                <Setter Property="Foreground"
                        Value="{DynamicResource DarkBrush1}" />
                <Setter Property="Effect"
                        Value="{x:Null}" />
            </Trigger>
        </Style.Triggers>
    </Style>
