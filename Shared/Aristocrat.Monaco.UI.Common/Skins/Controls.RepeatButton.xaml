                                  AutomationProperties.AutomationId="GradientStop17_EE772BB0" />
                    <GradientStop Color="{DynamicResource LightColor5}"
                                  Offset=".5"
                                  AutomationProperties.AutomationId="GradientStop18_EE772BB0" />
                </LinearGradientBrush>
            </Setter.Value>
        </Setter>
        <Setter Property="Foreground"
                Value="{DynamicResource DarkBrush1}" />
        <Setter Property="BorderThickness"
                Value="0" />
        <Setter Property="controls:ControlsHelper.ContentCharacterCasing"
                Value="Upper" />
        <Setter Property="controls:ControlsHelper.CornerRadius"
                Value="0" />
        <Setter Property="FontFamily"
                Value="{DynamicResource MonacoFontFamily}" />
