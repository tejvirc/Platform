<UserControl x:Class="Aristocrat.Monaco.Hhr.UI.Controls.RaceTrackEntry"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:gif="http://wpfanimatedgif.codeplex.com"
             Unloaded="RaceTrackEntry_OnUnloaded"
             x:Name="TrackPanel"
             Width="376"
             Height="20"
             AutomationProperties.AutomationId="UserControl0_B8DD914C">
    <Image x:Name="Horse"
           gif:ImageBehavior.AnimatedSource="{Binding HorseImage, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=UserControl}}"
           gif:ImageBehavior.AnimationSpeedRatio="1.3"
           gif:ImageBehavior.RepeatBehavior="Forever"
           HorizontalAlignment="Left"
           Height="60"
           Margin="0 -30 0 0"
           AutomationProperties.AutomationId="Image1_B8DD914C"></Image>
</UserControl>