<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
		<TargetFramework>net472</TargetFramework>
		<ResolveAssemblyWarnOrErrorOnTargetArchitectureMismatch>None</ResolveAssemblyWarnOrErrorOnTargetArchitectureMismatch>
		<AssemblyTitle>Aristocrat.Bingo.Client</AssemblyTitle>
		<Company>Aristocrat Technologies Inc.</Company>
		<Product>Monaco</Product>
		<Copyright>Copyright © Aristocrat Technologies Inc. 2021</Copyright>
		<OutputPath>..\..\..\bin\$(Configuration)\Platform\bin\</OutputPath>
		<AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
		<AppendRuntimeIdentifierToOutputPath>false</AppendRuntimeIdentifierToOutputPath>
		<CodeAnalysisRuleSet>MinimumRecommendedRules.ruleset</CodeAnalysisRuleSet>
		<TreatWarningsAsErrors>true</TreatWarningsAsErrors>
		<RunCodeAnalysis>true</RunCodeAnalysis>
		<SuppressLegacyCodeAnalysisDeprecatedWarning>true</SuppressLegacyCodeAnalysisDeprecatedWarning>
		<Configurations>Debug;Release;Retail</Configurations>
		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
		<LangVersion>latest</LangVersion>
	</PropertyGroup>
	<PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
		<DefineConstants>CODE_ANALYSIS;TRACE;DEBUG</DefineConstants>
		<DebugType>full</DebugType>
		<DocumentationFile />
	</PropertyGroup>
	<PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
		<DefineConstants>CODE_ANALYSIS;TRACE</DefineConstants>
		<DebugType>pdbonly</DebugType>
		<DocumentationFile />
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Retail|AnyCPU'">
		<DefineConstants>CODE_ANALYSIS;TRACE;RETAIL</DefineConstants>
		<Optimize>true</Optimize>
		<DebugType>pdbonly</DebugType>
	</PropertyGroup>
	<ItemGroup>
		<None Remove="Messages\proto\Aristocrat.ServerApiBingo.proto" />
		<None Remove="Messages\proto\Aristocrat.ServerApiGateway.proto" />
	</ItemGroup>
	<ItemGroup>
		<Reference Include="Microsoft.CSharp" />
		<Reference Include="System.ComponentModel.DataAnnotations" />
		<Reference Include="System.IdentityModel" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="log4net" Version="2.0.8" />
		<PackageReference Include="Polly" Version="7.2.1" />
		<PackageReference Include="SimpleInjector" Version="4.9.2" />
    <PackageReference Include="Stateless" Version="5.11.0" />
		<PackageReference Include="System.Threading.Tasks.Dataflow" Version="4.11.0" />
		<PackageReference Include="Google.Protobuf" Version="3.15.8" />
		<PackageReference Include="Grpc" Version="2.37.1" />
		<PackageReference Include="Grpc.Tools" Version="2.37.1">
			<PrivateAssets>all</PrivateAssets>
			<IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
	</ItemGroup>
	<ItemGroup>
		<Protobuf Include="Messages\proto\Aristocrat.ServerApiBingo.proto" />
		<Protobuf Include="Messages\proto\Aristocrat.ServerApiGateway.proto" />
  </ItemGroup>
</Project>
