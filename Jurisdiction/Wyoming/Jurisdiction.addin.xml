      <AddinConfigurationGroupReference name="BaseConfigWizardPages" />
      <ExtensionPointConfiguration extensionPath="/Platform/Discovery/Configuration">
        <NodeSpecification addinId="Client12Addins.WyomingJurisdiction" filterid="WyomingHardwareConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/TiltLogger/Configuration">
        <NodeSpecification addinId="Client12Addins.WyomingJurisdiction" filterid="WyomingTiltLoggerConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/DoorMonitor/Configuration">
        <NodeSpecification addinId="Client12Addins.WyomingJurisdiction" filterid="WyomingDoorMonitorConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/ConfigWizard/Configuration">
        <NodeSpecification addinId="Client12Addins.WyomingJurisdiction" filterid="WyomingConfigWizardConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/OperatorMenu/Configuration">
        <NodeSpecification addinId="Client12Addins.WyomingJurisdiction" filterid="WyomingOperatorMenuConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/OperatorMenu/Sound/Configuration">
        <NodeSpecification addinId="Client12Addins.WyomingJurisdiction" filterid="WyomingOperatorMenuSoundConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/OperatorMenu/GamePreferences/Sounds">
        <NodeSpecification addinId="Client12Addins.WyomingJurisdiction" filterid="WyomingGamePreferencesSounds" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Locale/Configuration">
        <NodeSpecification addinId="Client12Addins.WyomingJurisdiction" filterid="WyomingLocaleConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Lobby/Configuration">
        <NodeSpecification addinId="Client12Addins.WyomingJurisdiction" filterid="WyomingLobbyConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/Configuration">
        <NodeSpecification addinId="Client12Addins.WyomingJurisdiction" filterid="WyomingAccountingConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Application/JurisdictionConfiguration">
        <NodeSpecification addinId="Client12Addins.WyomingJurisdiction" filterid="WyomingApplicationConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Gaming/Configuration">
        <NodeSpecification addinId="Client12Addins.WyomingJurisdiction" filterid="WyomingGamingConfig" />
