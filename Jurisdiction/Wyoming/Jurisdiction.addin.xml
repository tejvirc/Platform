  </Extension>
  <Extension path="/SAS/DefaultConfiguration">
    <FilePath FilePath="..\jurisdiction\Wyoming\SASDefault.config.xml" filterid="WyomingSASDefaultConfig" />
  </Extension>
  <Extension path="/Platform/Discovery/Configuration">
    <FilePath FilePath="..\jurisdiction\Wyoming\ConfiguredDevices.config.xml" filterid="WyomingHardwareConfig" />
  </Extension>
  <Extension path="/TiltLogger/Configuration">
    <FilePath FilePath="..\jurisdiction\Wyoming\TiltLogger.config.xml" filterid="WyomingTiltLoggerConfig" />
  </Extension>
  <Extension path="/DoorMonitor/Configuration">
    <DoorMonitorConfiguration>
      <DoorOpenAlarm RepeatSeconds="0" LoopCount="7" OperatorCanCancel="true" />
    </DoorMonitorConfiguration>
  </Extension>
  <Extension path="/ConfigWizard/Configuration">
    <FilePath FilePath="..\jurisdiction\Wyoming\ConfigWizard.config.xml" filterid="WyomingConfigWizardConfig" />
  </Extension>
  <Extension path="/OperatorMenu/Configuration">
    <FilePath FilePath="..\jurisdiction\Wyoming\OperatorMenu.config.xml" filterid="WyomingOperatorMenuConfig" />
  </Extension>
  <Extension path="/Locale/Configuration" filterid="WyomingLocaleConfig">
    <FilePath FilePath="..\jurisdiction\Wyoming\Locale.config.xml" />
  </Extension>
  <Extension path="/Lobby/Configuration">
    <FilePath FilePath="..\jurisdiction\Wyoming\Lobby.config.xml" filterid="WyomingLobbyConfig" />
  </Extension>
  <Extension path="/Accounting/Configuration">
    <FilePath FilePath="..\jurisdiction\Wyoming\Accounting.config.xml" filterid="WyomingAccountingConfig" />
  </Extension>
  <Extension path="/Application/JurisdictionConfiguration">
    <FilePath FilePath="..\jurisdiction\Wyoming\Application.config.xml" filterid="WyomingApplicationConfig" />
  </Extension>
  <Extension path="/Gaming/Configuration">
    <FilePath FilePath="..\jurisdiction\Wyoming\Gaming.config.xml" filterid="WyomingGamingConfig" />
  </Extension>
  <Extension path="/HardMeterMapping/Configuration">
    <FilePath FilePath="..\jurisdiction\DefaultAssets\HardMeterMapping.config.xml" filterid="WyomingHardMeterMappingConfig" />
  </Extension>
  <Extension path="/TowerLight/Configuration">
    <FilePath FilePath="..\jurisdiction\Wyoming\TowerLight.config.xml" filterid="WyomingTowerLightConfig" />
