<LocaleConfiguration>
  <PlayerTicket Locale="en-US" DateFormat="dd/MM/yyyy">
  	<Selectable>
		<Entry Locale="en-US" CurrencyValueLocale="en-ZA" CurrencyWordsLocale="en-ZA" />
