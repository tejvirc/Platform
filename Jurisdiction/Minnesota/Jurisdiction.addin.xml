  <Extension path="/SAS/DefaultConfiguration">
    <FilePath FilePath="..\jurisdiction\Minnesota\SASDefault.config.xml" filterid="MinnesotaSASDefaultConfig" />
  </Extension>
  <Extension path="/Platform/Discovery/Configuration">
    <FilePath FilePath="..\jurisdiction\Minnesota\ConfiguredDevices.config.xml" filterid="MinnesotaHardwareConfig" />
  </Extension>
  <Extension path="/TiltLogger/Configuration">
    <FilePath FilePath="..\jurisdiction\Minnesota\TiltLogger.config.xml" filterid="MinnesotaTiltLoggerConfig" />
  </Extension>
  <Extension path="/DoorMonitor/Configuration">
    <DoorMonitorConfiguration>
      <DoorOpenAlarm RepeatSeconds="0" LoopCount="7" OperatorCanCancel="true" />
    </DoorMonitorConfiguration>
  </Extension>
  <Extension path="/ConfigWizard/Configuration">
    <FilePath FilePath="..\jurisdiction\Minnesota\ConfigWizard.config.xml" filterid="MinnesotaConfigWizardConfig" />
  </Extension>
  <Extension path="/OperatorMenu/Configuration">
    <FilePath FilePath="..\jurisdiction\Minnesota\OperatorMenu.config.xml" filterid="MinnesotaOperatorMenuConfig" />
  </Extension>
  <Extension path="/Locale/Configuration" filterid="MinnesotaLocaleConfig">
    <FilePath FilePath="..\jurisdiction\Minnesota\Locale.config.xml" />
  </Extension>
  <Extension path="/Lobby/Configuration">
    <FilePath FilePath="..\jurisdiction\Minnesota\Lobby.config.xml" filterid="MinnesotaLobbyConfig" />
  </Extension>
  <Extension path="/Accounting/Configuration">
    <FilePath FilePath="..\jurisdiction\Minnesota\Accounting.config.xml" filterid="MinnesotaAccountingConfig" />
  </Extension>
  <Extension path="/Application/JurisdictionConfiguration">
    <FilePath FilePath="..\jurisdiction\Minnesota\Application.config.xml" filterid="MinnesotaApplicationConfig" />
  </Extension>
  <Extension path="/Gaming/Configuration">
    <FilePath FilePath="..\jurisdiction\Minnesota\Gaming.config.xml" filterid="MinnesotaGamingConfig" />
  </Extension>
  <Extension path="/HardMeterMapping/Configuration">
    <FilePath FilePath="..\jurisdiction\DefaultAssets\HardMeterMapping.config.xml" filterid="MinnesotaHardMeterMappingConfig" />
  </Extension>
  <Extension path="/Sound/Configuration">
    <FilePath FilePath="..\jurisdiction\DefaultAssets\Sound.config.xml" filterid="MinnesotaSoundConfig" />
