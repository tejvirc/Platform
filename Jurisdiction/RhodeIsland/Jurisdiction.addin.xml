      <ExtensionPointConfiguration extensionPath="/Application/OperatorMenu/DisplayMeters">
        <NodeSpecification addinId="Client12Addins.RhodeIslandJurisdiction" filterid="RhodeIslandDisplayMetersConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/TransferOutProviders">
        <NodeSpecification addinId="Client12Addins.Monaco.Accounting" typeName="Aristocrat.Monaco.Accounting.Wat.WatOffProvider" order="0" filterid="RhodeIslandAccountingTransferProviders" />
        <NodeSpecification addinId="Client12Addins.Monaco.Accounting" typeName="Aristocrat.Monaco.Accounting.Vouchers.VoucherOutProvider" order="1" filterid="RhodeIslandAccountingTransferProviders" />
