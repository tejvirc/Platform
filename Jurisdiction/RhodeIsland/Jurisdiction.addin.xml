        <NodeSpecification addinId="Client12Addins.RhodeIslandJurisdiction" filterid="RhodeIslandLobbyConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/Configuration">
        <NodeSpecification addinId="Client12Addins.RhodeIslandJurisdiction" filterid="RhodeIslandAccountingConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Application/JurisdictionConfiguration">
        <NodeSpecification addinId="Client12Addins.RhodeIslandJurisdiction" filterid="RhodeIslandApplicationConfig" />
