  <Extension path="/SAS/DefaultConfiguration">
    <FilePath FilePath="..\jurisdiction\RhodeIsland\SASDefault.config.xml" filterid="RhodeIslandSASDefaultConfig" />
  </Extension>
  <Extension path="/Platform/Discovery/Configuration">
    <FilePath FilePath="..\jurisdiction\RhodeIsland\ConfiguredDevices.config.xml" filterid="RhodeIslandHardwareConfig" />
  </Extension>
  <Extension path="/TiltLogger/Configuration">
    <FilePath FilePath="..\jurisdiction\RhodeIsland\TiltLogger.config.xml" filterid="RhodeIslandTiltLoggerConfig" />
  </Extension>
  <Extension path="/DoorMonitor/Configuration">
    <DoorMonitorConfiguration>
      <DoorOpenAlarm RepeatSeconds="0" LoopCount="7" OperatorCanCancel="true" />
    </DoorMonitorConfiguration>
  </Extension>
  <Extension path="/ConfigWizard/Configuration">
    <FilePath FilePath="..\jurisdiction\RhodeIsland\ConfigWizard.config.xml" filterid="RhodeIslandConfigWizardConfig" />
