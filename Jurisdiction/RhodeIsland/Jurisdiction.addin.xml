  <Extension path="/Locale/Configuration" filterid="RhodeIslandLocaleConfig">
    <FilePath FilePath="..\jurisdiction\RhodeIsland\Locale.config.xml" />
  </Extension>
  <Extension path="/Lobby/Configuration">
    <FilePath FilePath="..\jurisdiction\RhodeIsland\Lobby.config.xml" filterid="RhodeIslandLobbyConfig" />
  </Extension>
  <Extension path="/Accounting/Configuration">
    <FilePath FilePath="..\jurisdiction\RhodeIsland\Accounting.config.xml" filterid="RhodeIslandAccountingConfig" />
