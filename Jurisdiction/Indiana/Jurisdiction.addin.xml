        <NodeSpecification addinId="Client12Addins.IndianaJurisdiction" filterid="IndianaDisplayMetersConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/TransferOutProviders">
        <NodeSpecification addinId="Client12Addins.Monaco.Accounting" typeName="Aristocrat.Monaco.Accounting.Wat.WatOffProvider" order="0" filterid="IndianaAccountingTransferProviders" />
        <NodeSpecification addinId="Client12Addins.Monaco.Accounting" typeName="Aristocrat.Monaco.Accounting.Vouchers.VoucherOutProvider" order="1" filterid="IndianaAccountingTransferProviders" />
