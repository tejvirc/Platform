  <Extension path="/SAS/DefaultConfiguration">
    <FilePath FilePath="..\jurisdiction\Indiana\SASDefault.config.xml" filterid="IndianaSASDefaultConfig" />
  </Extension>
  <Extension path="/Platform/Discovery/Configuration">
    <FilePath FilePath="..\jurisdiction\Indiana\ConfiguredDevices.config.xml" filterid="IndianaHardwareConfig" />
