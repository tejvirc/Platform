    <FilePath FilePath="..\jurisdiction\NIGC\SASDefault.config.xml" filterid="NIGCSASDefaultConfig" />
  </Extension>
  <Extension path="/Platform/Discovery/Configuration">
    <FilePath FilePath="..\jurisdiction\NIGC\ConfiguredDevices.config.xml" filterid="NIGCHardwareConfig" />
