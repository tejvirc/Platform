  </Extension>
  <Extension path="/SAS/DefaultConfiguration">
    <FilePath FilePath="..\jurisdiction\GeorgiaCOAM\SASDefault.config.xml" filterid="GeorgiaCOAMSASDefaultConfig" />
  </Extension>
  <Extension path="/Platform/Discovery/Configuration">
    <FilePath FilePath="..\jurisdiction\GeorgiaCOAM\ConfiguredDevices.config.xml" filterid="GeorgiaCOAMHardwareConfig" />
  </Extension>
  <Extension path="/TiltLogger/Configuration">
    <FilePath FilePath="..\jurisdiction\GeorgiaCOAM\TiltLogger.config.xml" filterid="GeorgiaCOAMTiltLoggerConfig" />
  </Extension>
  <Extension path="/DoorMonitor/Configuration">
    <DoorMonitorConfiguration>
      <DoorOpenAlarm RepeatSeconds="0" LoopCount="7" OperatorCanCancel="true" />
    </DoorMonitorConfiguration>
  </Extension>
  <Extension path="/ConfigWizard/Configuration">
    <FilePath FilePath="..\jurisdiction\GeorgiaCOAM\ConfigWizard.config.xml" filterid="GeorgiaCOAMConfigWizardConfig" />
  </Extension>
  <Extension path="/OperatorMenu/Configuration">
    <FilePath FilePath="..\jurisdiction\GeorgiaCOAM\OperatorMenu.config.xml" filterid="GeorgiaCOAMOperatorMenuConfig" />
  </Extension>
  <Extension path="/Locale/Configuration">
    <FilePath FilePath="..\jurisdiction\GeorgiaCOAM\Locale.config.xml" filterid="GeorgiaCOAMLocaleConfig"/>
  </Extension>
  <Extension path="/Lobby/Configuration">
    <FilePath FilePath="..\jurisdiction\GeorgiaCOAM\Lobby.config.xml" filterid="GeorgiaCOAMLobbyConfig" />
  </Extension>
  <Extension path="/Accounting/Configuration">
    <FilePath FilePath="..\jurisdiction\GeorgiaCOAM\Accounting.config.xml" filterid="GeorgiaCOAMAccountingConfig" />
  </Extension>
  <Extension path="/Application/JurisdictionConfiguration">
    <FilePath FilePath="..\jurisdiction\GeorgiaCOAM\Application.config.xml" filterid="GeorgiaCOAMApplicationConfig" />
  </Extension>
  <Extension path="/Gaming/Configuration">
    <FilePath FilePath="..\jurisdiction\GeorgiaCOAM\Gaming.config.xml" filterid="GeorgiaCOAMGamingConfig" />
  </Extension>
  <Extension path="/TowerLight/Configuration">
    <FilePath FilePath="..\jurisdiction\GeorgiaCOAM\TowerLight.config.xml" filterid="GeorgiaCOAMTowerLightConfig" />
  </Extension>
  <Extension path="/HardMeterMapping/Configuration">
    <FilePath FilePath="..\jurisdiction\GeorgiaCOAM\HardMeterMapping.config.xml" filterid="GeorgiaCOAMHardMeterMappingConfig" />
  </Extension>
  <Extension path="/Sound/Configuration">
    <FilePath FilePath="..\jurisdiction\DefaultAssets\Sound.config.xml" filterid="GeorgiaCOAMSoundConfig" />
  </Extension>
  <Extension path="/Hardware/PrintableRegions">
    <FilePath FilePath="..\jurisdiction\GeorgiaCOAM\printable_regions.xml" filterid="GeorgiaCOAMPrintableRegionsConfig" />
  </Extension>
  <Extension path="/Hardware/PrintableTemplates">
    <FilePath FilePath="..\jurisdiction\GeorgiaCOAM\printable_templates.xml" filterid="GeorgiaCOAMPrintableTemplatesConfig" />
  </Extension>
  <Extension path="/Hardware/PrinterOverrides">
    <FilePath FilePath="..\jurisdiction\GeorgiaCOAM\PrinterOverrides.xml" filterid="GeorgiaCOAMPrinterOverridesConfig" />
  </Extension>
  <Extension path="/ErrorMessage/Configuration">
    <FilePath FilePath="..\jurisdiction\GeorgiaCOAM\ErrorMessage.config.xml" filterid="GeorgiaCOAMErrorMessageConfig" />
  </Extension>
  <Extension path="/Application/OperatorMenu/DisplayMeters">
    <FilePath FilePath="..\jurisdiction\GeorgiaCOAM\DisplayMeters.config.xml" filterid="GeorgiaCOAMDisplayMetersConfig" />
  </Extension>
  <Extension path="/Gaming/OperatorMenu/GameHistoryMeters">
    <FilePath FilePath="..\jurisdiction\GeorgiaCOAM\GameHistoryMeters.config.xml" filterid="GeorgiaCOAMHistoryMetersConfig" />
  </Extension>
  <Extension path="/Accounting/TransactionHistories" filterid="GeorgiaCOAMTransactionConfiguration">
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.BillTransaction" maxTransactions="50" persistenceLevel="Critical" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.Handpay.HandpayTransaction" maxTransactions="50" persistenceLevel="Critical" isPrintable="true" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.VoucherInTransaction" maxTransactions="50" persistenceLevel="Critical" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.VoucherOutTransaction" maxTransactions="50" persistenceLevel="Critical" isPrintable="true" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.HandCount.HardMeterOutTransaction" maxTransactions="50" persistenceLevel="Critical" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.HandCount.ResidualCreditsTransaction" maxTransactions="50" persistenceLevel="Critical" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.Wat.WatTransaction" maxTransactions="100" persistenceLevel="Critical" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.WatOnTransaction" maxTransactions="100" persistenceLevel="Critical" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Gaming.Contracts.Progressives.JackpotTransaction" maxTransactions="50" persistenceLevel="Critical" isPrintable="true" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Gaming.Contracts.Bonus.BonusTransaction" maxTransactions="100" persistenceLevel="Critical" isPrintable="true" />
  </Extension>
  <Extension path="/Accounting/BootExtender" filterid="GeorgiaCOAMAccountingRunnable">
    <BootExtender type="Aristocrat.Monaco.Gaming.Class3Coam.Class3CoamRunnable" />
  </Extension>
</Addin>