<Addin id="GeorgiaCOAMJurisdiction" namespace="Client12Addins" version="1.0">
  <Dependencies>
    <Addin id="Monaco.Kernel" version="1.0" />
    <Addin id="Monaco.Hardware" version="1.0" />
    <Addin id="Monaco.Accounting" version="1.0" />
    <Addin id="Monaco.Application" version="1.0" />
    <Addin id="Monaco.Application.UI" version="1.0" />
    <Addin id="Aristocrat.Monaco.Gaming" version="1.0" />
    <Addin id="SelectableJurisdiction" version="1.0" />
    <Addin id="AccountingRunnable" version="1.0" />
    <Addin id="SasBase" version="1.0" />
    <Addin id="SasConfiguration" version="1.0" />
    <Addin id="Sas.Client" version="1.0" />
  </Dependencies>
  <Runtime>
    <Import assembly="..\..\bin\Aristocrat.Monaco.Kernel.dll" />
    <Import assembly="..\..\bin\Aristocrat.Monaco.Gaming.Class3Coam.dll" />
  </Runtime>
  <Extension path="/Kernel/SelectableAddinConfiguration/Jurisdiction">
    <SelectableJurisdiction name="Georgia COAM" description="Top level Georgia COAM jurisdiction group" id="JUR000300,JUR000998,JUR000999,JUR000033">
      <AddinConfigurationGroupReference name="BaseOperatorMenus" />
      <AddinConfigurationGroupReference name="BaseConfigWizardPages" />
      <ExtensionPointConfiguration extensionPath="/Platform/Discovery/Configuration">
        <NodeSpecification addinId="Client12Addins.GeorgiaCOAMJurisdiction" filterid="GeorgiaCOAMHardwareConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/TiltLogger/Configuration">
        <NodeSpecification addinId="Client12Addins.GeorgiaCOAMJurisdiction" filterid="GeorgiaCOAMTiltLoggerConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/DoorMonitor/Configuration">
        <NodeSpecification addinId="Client12Addins.GeorgiaCOAMJurisdiction" filterid="GeorgiaCOAMDoorMonitorConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/ConfigWizard/Configuration">
        <NodeSpecification addinId="Client12Addins.GeorgiaCOAMJurisdiction" filterid="GeorgiaCOAMConfigWizardConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/OperatorMenu/Configuration">
        <NodeSpecification addinId="Client12Addins.GeorgiaCOAMJurisdiction" filterid="GeorgiaCOAMOperatorMenuConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/OperatorMenu/Sound/Configuration">
        <NodeSpecification addinId="Client12Addins.GeorgiaCOAMJurisdiction" filterid="GeorgiaCOAMOperatorMenuSoundConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/OperatorMenu/GamePreferences/Sounds">
        <NodeSpecification addinId="Client12Addins.GeorgiaCOAMJurisdiction" filterid="GeorgiaCOAMGamePreferencesSounds" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Locale/Configuration">
        <NodeSpecification addinId="Client12Addins.GeorgiaCOAMJurisdiction" filterid="GeorgiaCOAMLocaleConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Lobby/Configuration">
        <NodeSpecification addinId="Client12Addins.GeorgiaCOAMJurisdiction" filterid="GeorgiaCOAMLobbyConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/Configuration">
        <NodeSpecification addinId="Client12Addins.GeorgiaCOAMJurisdiction" filterid="GeorgiaCOAMAccountingConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Application/JurisdictionConfiguration">
        <NodeSpecification addinId="Client12Addins.GeorgiaCOAMJurisdiction" filterid="GeorgiaCOAMApplicationConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Gaming/Configuration">
        <NodeSpecification addinId="Client12Addins.GeorgiaCOAMJurisdiction" filterid="GeorgiaCOAMGamingConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/TowerLight/Configuration">
        <NodeSpecification addinId="Client12Addins.GeorgiaCOAMJurisdiction" filterid="GeorgiaCOAMTowerLightConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Hardware/PrintableRegions">
        <NodeSpecification addinId="Client12Addins.GeorgiaCOAMJurisdiction" filterid="GeorgiaCOAMPrintableRegionsConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Hardware/PrintableTemplates">
        <NodeSpecification addinId="Client12Addins.GeorgiaCOAMJurisdiction" filterid="GeorgiaCOAMPrintableTemplatesConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Hardware/PrinterOverrides">
        <NodeSpecification addinId="Client12Addins.GeorgiaCOAMJurisdiction" filterid="GeorgiaCOAMPrinterOverridesConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Application/OperatorMenu/DisplayMeters">
        <NodeSpecification addinId="Client12Addins.GeorgiaCOAMJurisdiction" filterid="GeorgiaCOAMDisplayMetersConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Gaming/OperatorMenu/GameHistoryMeters">
        <NodeSpecification addinId="Client12Addins.GeorgiaCOAMJurisdiction" filterid="GeorgiaCOAMHistoryMetersConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/TransferOutProviders">
        <NodeSpecification addinId="Client12Addins.Monaco.Accounting" typeName="Aristocrat.Monaco.Accounting.Wat.WatOffProvider" order="0" filterid="GeorgiaCOAMAccountingTransferProviders" />
        <NodeSpecification addinId="Client12Addins.Monaco.Accounting" typeName="Aristocrat.Monaco.Accounting.Vouchers.VoucherOutProvider" order="1" filterid="GeorgiaCOAMAccountingTransferProviders" />
        <NodeSpecification addinId="Client12Addins.Monaco.Accounting" typeName="Aristocrat.Monaco.Accounting.HandCount.HardMeterOutProvider" order="2" filterid="GeorgiaCOAMAccountingTransferProviders" />
        <!--<NodeSpecification addinId="Client12Addins.Monaco.Accounting" typeName="Aristocrat.Monaco.Accounting.Handpay.HandpayProvider" order="3" filterid="GeorgiaCOAMAccountingTransferProviders" />-->
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/TransactionHistories">
        <NodeSpecification addinId="Client12Addins.GeorgiaCOAMJurisdiction" filterid="GeorgiaCOAMTransactionConfiguration" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/BootExtender">
        <NodeSpecification addinId="Client12Addins.GeorgiaCOAMJurisdiction" filterid="GeorgiaCOAMAccountingRunnable" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/ErrorMessage/Configuration">
        <NodeSpecification addinId="Client12Addins.GeorgiaCOAMJurisdiction" filterid="GeorgiaCOAMErrorMessageConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/SAS/DefaultConfiguration">
        <NodeSpecification addinId="Client12Addins.GeorgiaCOAMJurisdiction" filterid="GeorgiaCOAMSASDefaultConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/HardMeterMapping/Configuration">
        <NodeSpecification addinId="Client12Addins.GeorgiaCOAMJurisdiction" filterid="GeorgiaCOAMHardMeterMappingConfig" />
      </ExtensionPointConfiguration>
