		<GameHistoryMeterNode Name="CoinDropCashBox" DisplayName="Coin Drop (Cash Box)" />
		<GameHistoryMeterNode Name="ExtraCoinOutRunaway" DisplayName="Extra Coin Out (Runaway)" />
	</MeterNodes>
</GameHistoryMetersConfiguration>