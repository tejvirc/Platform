		<GameHistoryMeterNode Name="PhysicalCoinOut" DisplayName="Physical Coin Out" Indent="true" />
		<GameHistoryMeterNode Name="VoucherOut" DisplayName="Voucher Out" Indent="true" />
		<GameHistoryMeterNode Name="TransferOut" DisplayName="Total WAT Out" Indent="true" />
