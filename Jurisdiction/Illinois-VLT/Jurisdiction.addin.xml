  <Extension path="/SAS/DefaultConfiguration">
    <FilePath FilePath="..\jurisdiction\Illinois-VLT\SASDefault.config.xml" filterid="IllinoisSASDefaultConfig" />
  </Extension>
  <Extension path="/Platform/Discovery/Configuration">
    <FilePath FilePath="..\jurisdiction\Illinois-VLT\ConfiguredDevices.config.xml" filterid="IllinoisHardwareConfig" />
  </Extension>
  <Extension path="/TiltLogger/Configuration">
    <FilePath FilePath="..\jurisdiction\Illinois-VLT\TiltLogger.config.xml" filterid="IllinoisTiltLoggerConfig" />
  </Extension>
  <Extension path="/DoorMonitor/Configuration">
    <DoorMonitorConfiguration>
      <DoorOpenAlarm RepeatSeconds="0" LoopCount="15" OperatorCanCancel="true" />
