  <Extension path="/SAS/DefaultConfiguration">
    <FilePath FilePath="..\jurisdiction\VirginIslands\SASDefault.config.xml" filterid="VirginIslandsSASDefaultConfig" />
  </Extension>
  <Extension path="/Platform/Discovery/Configuration">
    <FilePath FilePath="..\jurisdiction\VirginIslands\ConfiguredDevices.config.xml" filterid="VirginIslandsHardwareConfig" />
  </Extension>
  <Extension path="/TiltLogger/Configuration">
    <FilePath FilePath="..\jurisdiction\VirginIslands\TiltLogger.config.xml" filterid="VirginIslandsTiltLoggerConfig" />
  </Extension>
  <Extension path="/DoorMonitor/Configuration">
    <DoorMonitorConfiguration>
      <DoorOpenAlarm RepeatSeconds="0" LoopCount="7" OperatorCanCancel="true" />
