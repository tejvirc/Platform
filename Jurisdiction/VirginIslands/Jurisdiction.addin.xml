      <ExtensionPointConfiguration extensionPath="/ErrorMessage/Configuration">
        <NodeSpecification addinId="Client12Addins.VirginIslandsJurisdiction" filterid="VirginIslandsErrorMessageConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/SAS/DefaultConfiguration">
        <NodeSpecification addinId="Client12Addins.VirginIslandsJurisdiction" filterid="VirginIslandsSASDefaultConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/HardMeterMapping/Configuration">
        <NodeSpecification addinId="Client12Addins.VirginIslandsJurisdiction" filterid="VirginIslandsHardMeterMappingConfig" />
