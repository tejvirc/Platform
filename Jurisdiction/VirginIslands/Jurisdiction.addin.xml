        <NodeSpecification addinId="Client12Addins.VirginIslandsJurisdiction" filterid="VirginIslandsAccountingConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Application/JurisdictionConfiguration">
        <NodeSpecification addinId="Client12Addins.VirginIslandsJurisdiction" filterid="VirginIslandsApplicationConfig" />
