  </Extension>
  <Extension path="/ConfigWizard/Configuration">
    <FilePath FilePath="..\jurisdiction\VirginIslands\ConfigWizard.config.xml" filterid="VirginIslandsConfigWizardConfig" />
  </Extension>
  <Extension path="/OperatorMenu/Configuration">
    <FilePath FilePath="..\jurisdiction\VirginIslands\OperatorMenu.config.xml" filterid="VirginIslandsOperatorMenuConfig" />
  </Extension>
  <Extension path="/Locale/Configuration" filterid="VirginIslandsLocaleConfig">
    <FilePath FilePath="..\jurisdiction\VirginIslands\Locale.config.xml" />
  </Extension>
  <Extension path="/Lobby/Configuration">
    <FilePath FilePath="..\jurisdiction\VirginIslands\Lobby.config.xml" filterid="VirginIslandsLobbyConfig" />
  </Extension>
  <Extension path="/Accounting/Configuration">
    <FilePath FilePath="..\jurisdiction\VirginIslands\Accounting.config.xml" filterid="VirginIslandsAccountingConfig" />
  </Extension>
  <Extension path="/Application/JurisdictionConfiguration">
