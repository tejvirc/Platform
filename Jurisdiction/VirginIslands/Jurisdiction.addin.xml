  </Extension>
  <Extension path="/Gaming/Configuration">
    <FilePath FilePath="..\jurisdiction\VirginIslands\Gaming.config.xml" filterid="VirginIslandsGamingConfig" />
  </Extension>
  <Extension path="/HardMeterMapping/Configuration">
    <FilePath FilePath="..\jurisdiction\DefaultAssets\HardMeterMapping.config.xml" filterid="VirginIslandsHardMeterMappingConfig" />
  </Extension>
  <Extension path="/TowerLight/Configuration">
