  <Extension path="/SAS/DefaultConfiguration">
    <FilePath FilePath="..\jurisdiction\Ontario\SASDefault.config.xml" filterid="OntarioSASDefaultConfig" />
  </Extension>
  <Extension path="/Platform/Discovery/Configuration">
    <FilePath FilePath="..\jurisdiction\Ontario\ConfiguredDevices.config.xml" filterid="OntarioHardwareConfig" />
  </Extension>
  <Extension path="/TiltLogger/Configuration">
    <FilePath FilePath="..\jurisdiction\Ontario\TiltLogger.config.xml" filterid="OntarioTiltLoggerConfig" />
  </Extension>
  <Extension path="/DoorMonitor/Configuration">
    <DoorMonitorConfiguration>
      <DoorOpenAlarm RepeatSeconds="0" LoopCount="7" OperatorCanCancel="true">
        <FilePath FilePath="..\jurisdiction\DefaultAssets\alarm.ogg" filterid="OntarioDoorMonitorConfig" />
      </DoorOpenAlarm>
    </DoorMonitorConfiguration>
  </Extension>
  <Extension path="/ConfigWizard/Configuration">
    <FilePath FilePath="..\jurisdiction\Ontario\ConfigWizard.config.xml" filterid="OntarioConfigWizardConfig" />
  </Extension>
  <Extension path="/OperatorMenu/Sound/Configuration">
    <FilePath FilePath="..\jurisdiction\DefaultAssets\ding.ogg" Name="Ding" filterid="OntarioOperatorMenuSoundConfig" />
    <FilePath FilePath="..\jurisdiction\DefaultAssets\feature_bell.ogg" Name="Feature Bell" filterid="OntarioOperatorMenuSoundConfig" />
    <FilePath FilePath="..\jurisdiction\DefaultAssets\touch_sound.ogg" Name="Touch Sound" filterid="OntarioOperatorMenuSoundConfig" />
    <FilePath FilePath="..\jurisdiction\DefaultAssets\collect.ogg" Name="Collect" filterid="OntarioOperatorMenuSoundConfig" />
    <FilePath FilePath="..\jurisdiction\DefaultAssets\snd_print_ticket.ogg" Name="Send Print Ticket" filterid="OntarioOperatorMenuSoundConfig" />
  </Extension>
  <Extension path="/OperatorMenu/GamePreferences/Sounds">
    <FilePath FilePath="..\jurisdiction\DefaultAssets\ding.ogg" Name="Ding" filterid="OntarioGamePreferencesSounds" />
    <FilePath FilePath="..\jurisdiction\DefaultAssets\ReelClick.ogg" Name="ReelClick" filterid="OntarioGamePreferencesSounds" />
    <FilePath FilePath="..\jurisdiction\DefaultAssets\CardFlip.ogg" Name="CardFlip" filterid="OntarioGamePreferencesSounds" />
