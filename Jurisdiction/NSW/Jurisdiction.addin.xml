<Addin id="NSWJurisdiction" namespace="Client12Addins" version="1.0">
  <Dependencies>
    <Addin id="Monaco.Kernel" version="1.0" />
    <Addin id="Monaco.Hardware" version="1.0" />
    <Addin id="Monaco.Accounting" version="1.0" />
    <Addin id="Monaco.Application" version="1.0" />
    <Addin id="Monaco.Application.UI" version="1.0" />
    <Addin id="Aristocrat.Monaco.Gaming" version="1.0" />
    <Addin id="SelectableJurisdiction" version="1.0" />
    <Addin id="AccountingRunnable" version="1.0" />
    <Addin id="SasBase" version="1.0" />
    <Addin id="SasConfiguration" version="1.0" />
    <Addin id="Sas.Client" version="1.0" />
  </Dependencies>
  <Runtime>
    <Import assembly="..\..\bin\Aristocrat.Monaco.Kernel.dll" />
    <Import assembly="..\..\bin\Aristocrat.Monaco.Gaming.Class3.dll" />
  </Runtime>
  <Extension path="/Kernel/SelectableAddinConfiguration/Jurisdiction">
    <SelectableJurisdiction name="NSW" description="Top level NSW jurisdiction group" id="JUR000998,JUR000999,JUR000033">
      <AddinConfigurationGroupReference name="BaseOperatorMenus" />
      <AddinConfigurationGroupReference name="BaseConfigWizardPages" />
      <ExtensionPointConfiguration extensionPath="/Platform/Discovery/Configuration">
        <NodeSpecification addinId="Client12Addins.NSWJurisdiction" filterid="NSWHardwareConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/TiltLogger/Configuration">
        <NodeSpecification addinId="Client12Addins.NSWJurisdiction" filterid="NSWTiltLoggerConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/DoorMonitor/Configuration">
        <NodeSpecification addinId="Client12Addins.NSWJurisdiction" filterid="NSWDoorMonitorConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/ConfigWizard/Configuration">
        <NodeSpecification addinId="Client12Addins.NSWJurisdiction" filterid="NSWConfigWizardConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/OperatorMenu/Configuration">
        <NodeSpecification addinId="Client12Addins.NSWJurisdiction" filterid="NSWOperatorMenuConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/OperatorMenu/Sound/Configuration">
        <NodeSpecification addinId="Client12Addins.NSWJurisdiction" filterid="NSWOperatorMenuSoundConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/OperatorMenu/GamePreferences/Sounds">
        <NodeSpecification addinId="Client12Addins.NSWJurisdiction" filterid="NSWGamePreferencesSounds" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Locale/Configuration">
        <NodeSpecification addinId="Client12Addins.NSWJurisdiction" filterid="NSWLocaleConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Lobby/Configuration">
        <NodeSpecification addinId="Client12Addins.NSWJurisdiction" filterid="NSWLobbyConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/Configuration">
        <NodeSpecification addinId="Client12Addins.NSWJurisdiction" filterid="NSWAccountingConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Application/JurisdictionConfiguration">
        <NodeSpecification addinId="Client12Addins.NSWJurisdiction" filterid="NSWApplicationConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Gaming/Configuration">
        <NodeSpecification addinId="Client12Addins.NSWJurisdiction" filterid="NSWGamingConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/TowerLight/Configuration">
        <NodeSpecification addinId="Client12Addins.NSWJurisdiction" filterid="NSWTowerLightConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Hardware/PrintableRegions">
        <NodeSpecification addinId="Client12Addins.NSWJurisdiction" filterid="NSWPrintableRegionsConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Hardware/PrintableTemplates">
        <NodeSpecification addinId="Client12Addins.NSWJurisdiction" filterid="NSWPrintableTemplatesConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Hardware/PrinterOverrides">
        <NodeSpecification addinId="Client12Addins.NSWJurisdiction" filterid="NSWPrinterOverridesConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Application/OperatorMenu/DisplayMeters">
        <NodeSpecification addinId="Client12Addins.NSWJurisdiction" filterid="NSWDisplayMetersConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/TransferOutProviders">
        <NodeSpecification addinId="Client12Addins.Monaco.Accounting" typeName="Aristocrat.Monaco.Accounting.Wat.WatOffProvider" order="0" filterid="NSWAccountingTransferProviders" />
        <NodeSpecification addinId="Client12Addins.Monaco.Accounting" typeName="Aristocrat.Monaco.Accounting.Vouchers.VoucherOutProvider" order="1" filterid="NSWAccountingTransferProviders" />
        <NodeSpecification addinId="Client12Addins.Monaco.Accounting" typeName="Aristocrat.Monaco.Accounting.Handpay.HandpayProvider" order="2" filterid="NSWAccountingTransferProviders" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/TransactionHistories">
        <NodeSpecification addinId="Client12Addins.NSWJurisdiction" filterid="NSWTransactionConfiguration" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/BootExtender">
        <NodeSpecification addinId="Client12Addins.NSWJurisdiction" filterid="NSWAccountingRunnable" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/ErrorMessage/Configuration">
        <NodeSpecification addinId="Client12Addins.NSWJurisdiction" filterid="NSWErrorMessageConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/SAS/DefaultConfiguration">
        <NodeSpecification addinId="Client12Addins.NSWJurisdiction" filterid="NSWSASDefaultConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/HardMeterMapping/Configuration">
        <NodeSpecification addinId="Client12Addins.NSWJurisdiction" filterid="NSWHardMeterMappingConfig" />
      </ExtensionPointConfiguration>
    </SelectableJurisdiction>
  </Extension>
  <Extension path="/SAS/DefaultConfiguration">
    <FilePath FilePath="..\jurisdiction\NSW\SASDefault.config.xml" filterid="NSWSASDefaultConfig" />
  </Extension>
  <Extension path="/Platform/Discovery/Configuration">
    <FilePath FilePath="..\jurisdiction\NSW\ConfiguredDevices.config.xml" filterid="NSWHardwareConfig" />
  </Extension>
  <Extension path="/TiltLogger/Configuration">
    <FilePath FilePath="..\jurisdiction\NSW\TiltLogger.config.xml" filterid="NSWTiltLoggerConfig" />
  </Extension>
  <Extension path="/DoorMonitor/Configuration">
    <DoorMonitorConfiguration>
      <DoorOpenAlarm RepeatSeconds="0" LoopCount="7" OperatorCanCancel="true" CanStopSoundWhenDoorIsClosed="false" />
    </DoorMonitorConfiguration>
  </Extension>
  <Extension path="/ConfigWizard/Configuration">
    <FilePath FilePath="..\jurisdiction\NSW\ConfigWizard.config.xml" filterid="NSWConfigWizardConfig" />
  </Extension>
  <Extension path="/OperatorMenu/Configuration">
    <FilePath FilePath="..\jurisdiction\NSW\OperatorMenu.config.xml" filterid="NSWOperatorMenuConfig" />
  </Extension>
  <Extension path="/Locale/Configuration" filterid="NSWLocaleConfig">
    <FilePath FilePath="..\jurisdiction\NSW\Locale.config.xml" />
  </Extension>
  <Extension path="/Lobby/Configuration">
    <FilePath FilePath="..\jurisdiction\NSW\Lobby.config.xml" filterid="NSWLobbyConfig" />
  </Extension>
  <Extension path="/Accounting/Configuration">
    <FilePath FilePath="..\jurisdiction\NSW\Accounting.config.xml" filterid="NSWAccountingConfig" />
  </Extension>
  <Extension path="/Application/JurisdictionConfiguration">
    <FilePath FilePath="..\jurisdiction\NSW\Application.config.xml" filterid="NSWApplicationConfig" />
  </Extension>
  <Extension path="/Gaming/Configuration">
    <FilePath FilePath="..\jurisdiction\NSW\Gaming.config.xml" filterid="NSWGamingConfig" />
  </Extension>
  <Extension path="/TowerLight/Configuration">
    <FilePath FilePath="..\jurisdiction\NSW\TowerLight.config.xml" filterid="NSWTowerLightConfig" />
  </Extension>
  <Extension path="/HardMeterMapping/Configuration">
    <FilePath FilePath="..\jurisdiction\DefaultAssets\HardMeterMapping.config.xml" filterid="NSWHardMeterMappingConfig" />
  </Extension>
  <Extension path="/Sound/Configuration">
    <FilePath FilePath="..\jurisdiction\DefaultAssets\Sound.config.xml" filterid="NSWSoundConfig" />
  </Extension>
  <Extension path="/Hardware/PrintableRegions">
    <FilePath FilePath="..\jurisdiction\NSW\printable_regions.xml" filterid="NSWPrintableRegionsConfig" />
  </Extension>
  <Extension path="/Hardware/PrintableTemplates">
    <FilePath FilePath="..\jurisdiction\NSW\printable_templates.xml" filterid="NSWPrintableTemplatesConfig" />
  </Extension>
  <Extension path="/Hardware/PrinterOverrides">
    <FilePath FilePath="..\jurisdiction\NSW\PrinterOverrides.xml" filterid="NSWPrinterOverridesConfig" />
  </Extension>
  <Extension path="/ErrorMessage/Configuration">
    <FilePath FilePath="..\jurisdiction\NSW\ErrorMessage.config.xml" filterid="NSWErrorMessageConfig" />
  </Extension>
  <Extension path="/Application/OperatorMenu/DisplayMeters">
    <FilePath FilePath="..\jurisdiction\NSW\DisplayMeters.config.xml" filterid="NSWDisplayMetersConfig" />
  </Extension>
  <Extension path="/Accounting/TransactionHistories" filterid="NSWTransactionConfiguration">
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.BillTransaction" maxTransactions="50" persistenceLevel="Critical" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.Handpay.HandpayTransaction" maxTransactions="50" persistenceLevel="Critical" isPrintable="true" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.VoucherInTransaction" maxTransactions="50" persistenceLevel="Critical" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.VoucherOutTransaction" maxTransactions="50" persistenceLevel="Critical" isPrintable="true" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.Wat.WatTransaction" maxTransactions="100" persistenceLevel="Critical" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.WatOnTransaction" maxTransactions="100" persistenceLevel="Critical" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Gaming.Contracts.Progressives.JackpotTransaction" maxTransactions="50" persistenceLevel="Critical" isPrintable="true" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Gaming.Contracts.Bonus.BonusTransaction" maxTransactions="100" persistenceLevel="Critical" isPrintable="true" />
  </Extension>
  <Extension path="/Accounting/BootExtender" filterid="NSWAccountingRunnable">
    <BootExtender type="Aristocrat.Monaco.Gaming.Class3.Class3Runnable" />
  </Extension>
</Addin>