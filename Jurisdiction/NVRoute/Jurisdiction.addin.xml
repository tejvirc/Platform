  </Extension>
  <Extension path="/SAS/DefaultConfiguration">
    <FilePath FilePath="..\jurisdiction\NVRoute\SASDefault.config.xml" filterid="NVRouteSASDefaultConfig" />
  </Extension>
  <Extension path="/Platform/Discovery/Configuration">
    <FilePath FilePath="..\jurisdiction\NVRoute\ConfiguredDevices.config.xml" filterid="NVRouteHardwareConfig" />
  </Extension>
  <Extension path="/TiltLogger/Configuration">
    <FilePath FilePath="..\jurisdiction\NVRoute\TiltLogger.config.xml" filterid="NVRouteTiltLoggerConfig" />
  </Extension>
  <Extension path="/DoorMonitor/Configuration">
    <DoorMonitorConfiguration>
      <DoorOpenAlarm RepeatSeconds="0" LoopCount="7" OperatorCanCancel="true">
        <FilePath FilePath="..\jurisdiction\DefaultAssets\alarm.ogg" filterid="NVRouteDoorMonitorConfig" />
      </DoorOpenAlarm>
    </DoorMonitorConfiguration>
  </Extension>
  <Extension path="/ConfigWizard/Configuration">
    <FilePath FilePath="..\jurisdiction\NVRoute\ConfigWizard.config.xml" filterid="NVRouteConfigWizardConfig" />
  </Extension>
  <Extension path="/OperatorMenu/Sound/Configuration">
    <FilePath FilePath="..\jurisdiction\DefaultAssets\ding.ogg" Name="Ding" filterid="NVRouteOperatorMenuSoundConfig" />
    <FilePath FilePath="..\jurisdiction\DefaultAssets\feature_bell.ogg" Name="Feature Bell" filterid="NVRouteOperatorMenuSoundConfig" />
    <FilePath FilePath="..\jurisdiction\DefaultAssets\touch_sound.ogg" Name="Touch Sound" filterid="NVRouteOperatorMenuSoundConfig" />
    <FilePath FilePath="..\jurisdiction\DefaultAssets\collect.ogg" Name="Collect" filterid="NVRouteOperatorMenuSoundConfig" />
    <FilePath FilePath="..\jurisdiction\DefaultAssets\snd_print_ticket.ogg" Name="Send Print Ticket" filterid="NVRouteOperatorMenuSoundConfig" />
  </Extension>
  <Extension path="/OperatorMenu/GamePreferences/Sounds">
    <FilePath FilePath="..\jurisdiction\DefaultAssets\ding.ogg" Name="Ding" filterid="NVRouteGamePreferencesSounds" />
    <FilePath FilePath="..\jurisdiction\DefaultAssets\ReelClick.ogg" Name="ReelClick" filterid="NVRouteGamePreferencesSounds" />
    <FilePath FilePath="..\jurisdiction\DefaultAssets\CardFlip.ogg" Name="CardFlip" filterid="NVRouteGamePreferencesSounds" />
    <FilePath FilePath="..\jurisdiction\DefaultAssets\BallDrop.ogg" Name="BallDrop" filterid="NVRouteGamePreferencesSounds" />
  </Extension>
  <Extension path="/OperatorMenu/Configuration">
