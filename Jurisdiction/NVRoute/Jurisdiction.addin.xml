      <ExtensionPointConfiguration extensionPath="/Hardware/PrintableRegions">
        <NodeSpecification addinId="Client12Addins.NVRouteJurisdiction" filterid="NVRoutePrintableRegionsConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Hardware/PrintableTemplates">
        <NodeSpecification addinId="Client12Addins.NVRouteJurisdiction" filterid="NVRoutePrintableTemplatesConfig" />
      </ExtensionPointConfiguration>
