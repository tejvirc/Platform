      <ExtensionPointConfiguration extensionPath="/Locale/Overrides">
        <NodeSpecification addinId="Client12Addins.NVRouteJurisdiction" filterid="NVRouteLocaleOverrides" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Lobby/Configuration">
        <NodeSpecification addinId="Client12Addins.NVRouteJurisdiction" filterid="NVRouteLobbyConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/Configuration">
        <NodeSpecification addinId="Client12Addins.NVRouteJurisdiction" filterid="NVRouteAccountingConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Application/JurisdictionConfiguration">
        <NodeSpecification addinId="Client12Addins.NVRouteJurisdiction" filterid="NVRouteApplicationConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Gaming/Configuration">
        <NodeSpecification addinId="Client12Addins.NVRouteJurisdiction" filterid="NVRouteGamingConfig" />
