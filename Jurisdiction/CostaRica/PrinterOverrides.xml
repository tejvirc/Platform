      <Mapping Description="handpay receipt no barcode" PlatformTemplateId="107" PrinterTemplateId="8" Regions="0 121 123 106 0 0 0 1 2 128 3 129 0 0 125 0" />
    </PrinterTemplateMappings>
  </Override>
  
  
