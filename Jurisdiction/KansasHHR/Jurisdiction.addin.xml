        <NodeSpecification addinId="Client12Addins.KansasHHRJurisdiction" filterid="KansasHHRPrintableTemplatesConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Hardware/PrinterOverrides">
        <NodeSpecification addinId="Client12Addins.KansasHHRJurisdiction" filterid="KansasHHRPrinterOverridesConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Application/OperatorMenu/DisplayMeters">
        <NodeSpecification addinId="Client12Addins.KansasHHRJurisdiction" filterid="KansasHHRDisplayMetersConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/TransferOutProviders">
        <NodeSpecification addinId="Client12Addins.Monaco.Accounting" typeName="Aristocrat.Monaco.Accounting.Wat.WatOffProvider" order="0" filterid="KansasHHRAccountingTransferProviders" />
        <NodeSpecification addinId="Client12Addins.Monaco.Accounting" typeName="Aristocrat.Monaco.Accounting.Vouchers.VoucherOutProvider" order="1" filterid="KansasHHRAccountingTransferProviders" />
        <NodeSpecification addinId="Client12Addins.Monaco.Accounting" typeName="Aristocrat.Monaco.Accounting.Handpay.HandpayProvider" order="2" filterid="KansasHHRAccountingTransferProviders" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/TransactionHistories">
        <NodeSpecification addinId="Client12Addins.KansasHHRJurisdiction" filterid="KansasHHRTransactionConfiguration" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/BootExtender">
        <NodeSpecification addinId="Client12Addins.KansasHHRJurisdiction" filterid="KansasHHRAccountingRunnable" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/ErrorMessage/Configuration">
        <NodeSpecification addinId="Client12Addins.KansasHHRJurisdiction" filterid="KansasHHRErrorMessageConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/SAS/DefaultConfiguration">
        <NodeSpecification addinId="Client12Addins.KansasHHRJurisdiction" filterid="KansasHHRSASDefaultConfig" />
