        <NodeSpecification addinId="Client12Addins.KansasHHRJurisdiction" filterid="KansasHHRLocaleConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Lobby/Configuration">
        <NodeSpecification addinId="Client12Addins.KansasHHRJurisdiction" filterid="KansasHHRLobbyConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/Configuration">
        <NodeSpecification addinId="Client12Addins.KansasHHRJurisdiction" filterid="KansasHHRAccountingConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Application/JurisdictionConfiguration">
        <NodeSpecification addinId="Client12Addins.KansasHHRJurisdiction" filterid="KansasHHRApplicationConfig" />
