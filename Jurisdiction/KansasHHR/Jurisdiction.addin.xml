        <NodeSpecification addinId="Client12Addins.KansasHHRJurisdiction" filterid="KansasHHRHardwareConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/TiltLogger/Configuration">
        <NodeSpecification addinId="Client12Addins.KansasHHRJurisdiction" filterid="KansasHHRTiltLoggerConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/DoorMonitor/Configuration">
        <NodeSpecification addinId="Client12Addins.KansasHHRJurisdiction" filterid="KansasHHRDoorMonitorConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/ConfigWizard/Configuration">
        <NodeSpecification addinId="Client12Addins.KansasHHRJurisdiction" filterid="KansasHHRConfigWizardConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/OperatorMenu/Configuration">
        <NodeSpecification addinId="Client12Addins.KansasHHRJurisdiction" filterid="KansasHHROperatorMenuConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/OperatorMenu/Sound/Configuration">
        <NodeSpecification addinId="Client12Addins.KansasHHRJurisdiction" filterid="KansasHHROperatorMenuSoundConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/OperatorMenu/GamePreferences/Sounds">
