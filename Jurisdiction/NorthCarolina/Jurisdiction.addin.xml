    <FilePath FilePath="..\jurisdiction\DefaultAssets\touch_sound.ogg" Name="Touch Sound" filterid="NorthCarolinaOperatorMenuSoundConfig" />
    <FilePath FilePath="..\jurisdiction\DefaultAssets\collect.ogg" Name="Collect" filterid="NorthCarolinaOperatorMenuSoundConfig" />
