      <AddinConfigurationGroupReference name="BaseOperatorMenus" />
      <AddinConfigurationGroupReference name="BaseConfigWizardPages" />
      <ExtensionPointConfiguration extensionPath="/Platform/Discovery/Configuration">
        <NodeSpecification addinId="Client12Addins.NewJerseyJurisdiction" filterid="NewJerseyHardwareConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/TiltLogger/Configuration">
        <NodeSpecification addinId="Client12Addins.NewJerseyJurisdiction" filterid="NewJerseyTiltLoggerConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/DoorMonitor/Configuration">
        <NodeSpecification addinId="Client12Addins.NewJerseyJurisdiction" filterid="NewJerseyDoorMonitorConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/ConfigWizard/Configuration">
        <NodeSpecification addinId="Client12Addins.NewJerseyJurisdiction" filterid="NewJerseyConfigWizardConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/OperatorMenu/Configuration">
        <NodeSpecification addinId="Client12Addins.NewJerseyJurisdiction" filterid="NewJerseyOperatorMenuConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/OperatorMenu/Sound/Configuration">
        <NodeSpecification addinId="Client12Addins.NewJerseyJurisdiction" filterid="NewJerseyOperatorMenuSoundConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/OperatorMenu/GamePreferences/Sounds">
        <NodeSpecification addinId="Client12Addins.NewJerseyJurisdiction" filterid="NewJerseyGamePreferencesSounds" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Locale/Configuration">
        <NodeSpecification addinId="Client12Addins.NewJerseyJurisdiction" filterid="NewJerseyLocaleConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Lobby/Configuration">
        <NodeSpecification addinId="Client12Addins.NewJerseyJurisdiction" filterid="NewJerseyLobbyConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/Configuration">
        <NodeSpecification addinId="Client12Addins.NewJerseyJurisdiction" filterid="NewJerseyAccountingConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Application/JurisdictionConfiguration">
        <NodeSpecification addinId="Client12Addins.NewJerseyJurisdiction" filterid="NewJerseyApplicationConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Gaming/Configuration">
        <NodeSpecification addinId="Client12Addins.NewJerseyJurisdiction" filterid="NewJerseyGamingConfig" />
