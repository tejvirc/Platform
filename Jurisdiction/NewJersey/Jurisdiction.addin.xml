  </Extension>
  <Extension path="/SAS/DefaultConfiguration">
    <FilePath FilePath="..\jurisdiction\NewJersey\SASDefault.config.xml" filterid="NewJerseySASDefaultConfig" />
  </Extension>
  <Extension path="/Platform/Discovery/Configuration">
    <FilePath FilePath="..\jurisdiction\NewJersey\ConfiguredDevices.config.xml" filterid="NewJerseyHardwareConfig" />
  </Extension>
  <Extension path="/TiltLogger/Configuration">
    <FilePath FilePath="..\jurisdiction\NewJersey\TiltLogger.config.xml" filterid="NewJerseyTiltLoggerConfig" />
  </Extension>
  <Extension path="/DoorMonitor/Configuration">
    <DoorMonitorConfiguration>
      <DoorOpenAlarm RepeatSeconds="0" LoopCount="7" OperatorCanCancel="true" />
    </DoorMonitorConfiguration>
  </Extension>
  <Extension path="/ConfigWizard/Configuration">
    <FilePath FilePath="..\jurisdiction\NewJersey\ConfigWizard.config.xml" filterid="NewJerseyConfigWizardConfig" />
  </Extension>
  <Extension path="/OperatorMenu/Configuration">
    <FilePath FilePath="..\jurisdiction\NewJersey\OperatorMenu.config.xml" filterid="NewJerseyOperatorMenuConfig" />
  </Extension>
  <Extension path="/Locale/Configuration" filterid="NewJerseyLocaleConfig">
    <FilePath FilePath="..\jurisdiction\NewJersey\Locale.config.xml" />
  </Extension>
  <Extension path="/Lobby/Configuration">
    <FilePath FilePath="..\jurisdiction\NewJersey\Lobby.config.xml" filterid="NewJerseyLobbyConfig" />
  </Extension>
  <Extension path="/Accounting/Configuration">
    <FilePath FilePath="..\jurisdiction\NewJersey\Accounting.config.xml" filterid="NewJerseyAccountingConfig" />
  </Extension>
  <Extension path="/Application/JurisdictionConfiguration">
    <FilePath FilePath="..\jurisdiction\NewJersey\Application.config.xml" filterid="NewJerseyApplicationConfig" />
  </Extension>
  <Extension path="/Gaming/Configuration">
    <FilePath FilePath="..\jurisdiction\NewJersey\Gaming.config.xml" filterid="NewJerseyGamingConfig" />
  </Extension>
  <Extension path="/HardMeterMapping/Configuration">
    <FilePath FilePath="..\jurisdiction\DefaultAssets\HardMeterMapping.config.xml" filterid="NewJerseyHardMeterMappingConfig" />
  </Extension>
  <Extension path="/TowerLight/Configuration">
    <FilePath FilePath="..\jurisdiction\NewJersey\TowerLight.config.xml" filterid="NewJerseyTowerLightConfig" />
