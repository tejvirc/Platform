      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Hardware/PrintableTemplates">
        <NodeSpecification addinId="Client12Addins.NewJerseyJurisdiction" filterid="NewJerseyPrintableTemplatesConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Hardware/PrinterOverrides">
        <NodeSpecification addinId="Client12Addins.NewJerseyJurisdiction" filterid="NewJerseyPrinterOverridesConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Application/OperatorMenu/DisplayMeters">
        <NodeSpecification addinId="Client12Addins.NewJerseyJurisdiction" filterid="NewJerseyDisplayMetersConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/TransferOutProviders">
        <NodeSpecification addinId="Client12Addins.Monaco.Accounting" typeName="Aristocrat.Monaco.Accounting.Wat.WatOffProvider" order="0" filterid="NewJerseyAccountingTransferProviders" />
        <NodeSpecification addinId="Client12Addins.Monaco.Accounting" typeName="Aristocrat.Monaco.Accounting.Vouchers.VoucherOutProvider" order="1" filterid="NewJerseyAccountingTransferProviders" />
        <NodeSpecification addinId="Client12Addins.Monaco.Accounting" typeName="Aristocrat.Monaco.Accounting.Handpay.HandpayProvider" order="2" filterid="NewJerseyAccountingTransferProviders" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/TransactionHistories">
        <NodeSpecification addinId="Client12Addins.NewJerseyJurisdiction" filterid="NewJerseyTransactionConfiguration" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/BootExtender">
        <NodeSpecification addinId="Client12Addins.NewJerseyJurisdiction" filterid="NewJerseyAccountingRunnable" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/ErrorMessage/Configuration">
        <NodeSpecification addinId="Client12Addins.NewJerseyJurisdiction" filterid="NewJerseyErrorMessageConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/SAS/DefaultConfiguration">
        <NodeSpecification addinId="Client12Addins.NewJerseyJurisdiction" filterid="NewJerseySASDefaultConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/HardMeterMapping/Configuration">
        <NodeSpecification addinId="Client12Addins.NewJerseyJurisdiction" filterid="NewJerseyHardMeterMappingConfig" />
