    <FilePath FilePath="..\jurisdiction\Chile\ConfiguredDevices.config.xml" filterid="ChileHardwareConfig" />
  </Extension>
  <Extension path="/TiltLogger/Configuration">
    <FilePath FilePath="..\jurisdiction\Chile\TiltLogger.config.xml" filterid="ChileTiltLoggerConfig" />
  </Extension>
  <Extension path="/DoorMonitor/Configuration">
    <DoorMonitorConfiguration>
      <DoorOpenAlarm RepeatSeconds="0" LoopCount="7" OperatorCanCancel="true">
        <FilePath FilePath="..\jurisdiction\DefaultAssets\alarm.ogg" filterid="ChileDoorMonitorConfig" />
      </DoorOpenAlarm>
    </DoorMonitorConfiguration>
  </Extension>
  <Extension path="/ConfigWizard/Configuration">
    <FilePath FilePath="..\jurisdiction\Chile\ConfigWizard.config.xml" filterid="ChileConfigWizardConfig" />
  </Extension>
  <Extension path="/OperatorMenu/Sound/Configuration">
    <FilePath FilePath="..\jurisdiction\DefaultAssets\ding.ogg" Name="Ding" filterid="ChileOperatorMenuSoundConfig" />
    <FilePath FilePath="..\jurisdiction\DefaultAssets\feature_bell.ogg" Name="Feature Bell" filterid="ChileOperatorMenuSoundConfig" />
    <FilePath FilePath="..\jurisdiction\DefaultAssets\touch_sound.ogg" Name="Touch Sound" filterid="ChileOperatorMenuSoundConfig" />
    <FilePath FilePath="..\jurisdiction\DefaultAssets\collect.ogg" Name="Collect" filterid="ChileOperatorMenuSoundConfig" />
    <FilePath FilePath="..\jurisdiction\DefaultAssets\snd_print_ticket.ogg" Name="Send Print Ticket" filterid="ChileOperatorMenuSoundConfig" />
