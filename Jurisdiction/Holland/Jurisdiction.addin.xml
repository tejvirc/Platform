      <ExtensionPointConfiguration extensionPath="/Hardware/PrintableTemplates">
        <NodeSpecification addinId="Client12Addins.HollandJurisdiction" filterid="HollandPrintableTemplatesConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Hardware/PrinterOverrides">
        <NodeSpecification addinId="Client12Addins.HollandJurisdiction" filterid="HollandPrinterOverridesConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Application/OperatorMenu/DisplayMeters">
        <NodeSpecification addinId="Client12Addins.HollandJurisdiction" filterid="HollandDisplayMetersConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/TransferOutProviders">
        <NodeSpecification addinId="Client12Addins.Monaco.Accounting" typeName="Aristocrat.Monaco.Accounting.Wat.WatOffProvider" order="0" filterid="HollandAccountingTransferProviders" />
        <NodeSpecification addinId="Client12Addins.Monaco.Accounting" typeName="Aristocrat.Monaco.Accounting.Vouchers.VoucherOutProvider" order="1" filterid="HollandAccountingTransferProviders" />
        <NodeSpecification addinId="Client12Addins.Monaco.Accounting" typeName="Aristocrat.Monaco.Accounting.Handpay.HandpayProvider" order="2" filterid="HollandAccountingTransferProviders" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/TransactionHistories">
        <NodeSpecification addinId="Client12Addins.HollandJurisdiction" filterid="HollandTransactionConfiguration" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/BootExtender">
        <NodeSpecification addinId="Client12Addins.HollandJurisdiction" filterid="HollandAccountingRunnable" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/ErrorMessage/Configuration">
        <NodeSpecification addinId="Client12Addins.HollandJurisdiction" filterid="HollandErrorMessageConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/SAS/DefaultConfiguration">
        <NodeSpecification addinId="Client12Addins.HollandJurisdiction" filterid="HollandSASDefaultConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/HardMeterMapping/Configuration">
        <NodeSpecification addinId="Client12Addins.HollandJurisdiction" filterid="HollandHardMeterMappingConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Currency/Configuration">
        <NodeSpecification addinId="Client12Addins.HollandJurisdiction" filterid="HollandCurrencyConfig" />
      </ExtensionPointConfiguration>
    </SelectableJurisdiction>
  </Extension>
  <Extension path="/Currency/Configuration">
    <FilePath FilePath="..\jurisdiction\Holland\Currencies.config.xml" filterid="HollandCurrencyConfig" />
  </Extension>
  <Extension path="/SAS/DefaultConfiguration">
    <FilePath FilePath="..\jurisdiction\Holland\SASDefault.config.xml" filterid="HollandSASDefaultConfig" />
  </Extension>
  <Extension path="/Platform/Discovery/Configuration">
    <FilePath FilePath="..\jurisdiction\Holland\ConfiguredDevices.config.xml" filterid="HollandHardwareConfig" />
  </Extension>
  <Extension path="/TiltLogger/Configuration">
    <FilePath FilePath="..\jurisdiction\Holland\TiltLogger.config.xml" filterid="HollandTiltLoggerConfig" />
  </Extension>
  <Extension path="/DoorMonitor/Configuration">
    <DoorMonitorConfiguration>
      <DoorOpenAlarm RepeatSeconds="0" LoopCount="7" OperatorCanCancel="true" />
    </DoorMonitorConfiguration>
  </Extension>
  <Extension path="/ConfigWizard/Configuration">
    <FilePath FilePath="..\jurisdiction\Holland\ConfigWizard.config.xml" filterid="HollandConfigWizardConfig" />
  </Extension>
  <Extension path="/OperatorMenu/Configuration">
    <FilePath FilePath="..\jurisdiction\Holland\OperatorMenu.config.xml" filterid="HollandOperatorMenuConfig" />
  </Extension>
  <Extension path="/Locale/Configuration" filterid="HollandLocaleConfig">
    <FilePath FilePath="..\jurisdiction\Holland\Locale.config.xml" />
  </Extension>
  <Extension path="/Lobby/Configuration">
    <FilePath FilePath="..\jurisdiction\Holland\Lobby.config.xml" filterid="HollandLobbyConfig" />
  </Extension>
  <Extension path="/Accounting/Configuration">
    <FilePath FilePath="..\jurisdiction\Holland\Accounting.config.xml" filterid="HollandAccountingConfig" />
  </Extension>
  <Extension path="/Application/JurisdictionConfiguration">
    <FilePath FilePath="..\jurisdiction\Holland\Application.config.xml" filterid="HollandApplicationConfig" />
  </Extension>
  <Extension path="/Gaming/Configuration">
    <FilePath FilePath="..\jurisdiction\Holland\Gaming.config.xml" filterid="HollandGamingConfig" />
  </Extension>
  <Extension path="/TowerLight/Configuration">
    <FilePath FilePath="..\jurisdiction\Holland\TowerLight.config.xml" filterid="HollandTowerLightConfig" />
  </Extension>
  <Extension path="/HardMeterMapping/Configuration">
    <FilePath FilePath="..\jurisdiction\Holland\HardMeterMapping.config.xml" filterid="HollandHardMeterMappingConfig" />
  </Extension>
  <Extension path="/Sound/Configuration">
    <FilePath FilePath="..\jurisdiction\DefaultAssets\Sound.config.xml" filterid="HollandSoundConfig" />
  </Extension>
  <Extension path="/Hardware/PrintableRegions">
    <FilePath FilePath="..\jurisdiction\Holland\printable_regions.xml" filterid="HollandPrintableRegionsConfig" />
  </Extension>
  <Extension path="/Hardware/PrintableTemplates">
    <FilePath FilePath="..\jurisdiction\Holland\printable_templates.xml" filterid="HollandPrintableTemplatesConfig" />
  </Extension>
  <Extension path="/Hardware/PrinterOverrides">
    <FilePath FilePath="..\jurisdiction\Holland\PrinterOverrides.xml" filterid="HollandPrinterOverridesConfig" />
  </Extension>
  <Extension path="/ErrorMessage/Configuration">
    <FilePath FilePath="..\jurisdiction\Holland\ErrorMessage.config.xml" filterid="HollandErrorMessageConfig" />
  </Extension>
  <Extension path="/Application/OperatorMenu/DisplayMeters">
    <FilePath FilePath="..\jurisdiction\Holland\DisplayMeters.config.xml" filterid="HollandDisplayMetersConfig" />
  </Extension>
  <Extension path="/Accounting/TransactionHistories" filterid="HollandTransactionConfiguration">
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.BillTransaction" maxTransactions="50" persistenceLevel="Critical" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.Handpay.HandpayTransaction" maxTransactions="50" persistenceLevel="Critical" isPrintable="true" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.VoucherInTransaction" maxTransactions="50" persistenceLevel="Critical" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.VoucherOutTransaction" maxTransactions="50" persistenceLevel="Critical" isPrintable="true" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.Wat.WatTransaction" maxTransactions="100" persistenceLevel="Critical" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.WatOnTransaction" maxTransactions="100" persistenceLevel="Critical" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Gaming.Contracts.Progressives.JackpotTransaction" maxTransactions="50" persistenceLevel="Critical" isPrintable="true" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Gaming.Contracts.Bonus.BonusTransaction" maxTransactions="100" persistenceLevel="Critical" isPrintable="true" />
  </Extension>
  <Extension path="/Accounting/BootExtender" filterid="HollandAccountingRunnable">
    <BootExtender type="Aristocrat.Monaco.Gaming.Class3.Class3Runnable" />
  </Extension>
</Addin>