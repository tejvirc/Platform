  <Extension path="/SAS/DefaultConfiguration">
    <FilePath FilePath="..\jurisdiction\Jamaica\SASDefault.config.xml" filterid="JamaicaSASDefaultConfig" />
  </Extension>
  <Extension path="/Platform/Discovery/Configuration">
    <FilePath FilePath="..\jurisdiction\Jamaica\ConfiguredDevices.config.xml" filterid="JamaicaHardwareConfig" />
  </Extension>
  <Extension path="/TiltLogger/Configuration">
    <FilePath FilePath="..\jurisdiction\Jamaica\TiltLogger.config.xml" filterid="JamaicaTiltLoggerConfig" />
  </Extension>
  <Extension path="/DoorMonitor/Configuration">
    <DoorMonitorConfiguration>
      <DoorOpenAlarm RepeatSeconds="0" LoopCount="7" OperatorCanCancel="true" />
    </DoorMonitorConfiguration>
  </Extension>
  <Extension path="/ConfigWizard/Configuration">
    <FilePath FilePath="..\jurisdiction\Jamaica\ConfigWizard.config.xml" filterid="JamaicaConfigWizardConfig" />
  </Extension>
  <Extension path="/OperatorMenu/Configuration">
    <FilePath FilePath="..\jurisdiction\Jamaica\OperatorMenu.config.xml" filterid="JamaicaOperatorMenuConfig" />
  </Extension>
  <Extension path="/Locale/Configuration" filterid="JamaicaLocaleConfig">
    <FilePath FilePath="..\jurisdiction\Jamaica\Locale.config.xml" />
  </Extension>
  <Extension path="/Lobby/Configuration">
    <FilePath FilePath="..\jurisdiction\Jamaica\Lobby.config.xml" filterid="JamaicaLobbyConfig" />
  </Extension>
  <Extension path="/Accounting/Configuration">
    <FilePath FilePath="..\jurisdiction\Jamaica\Accounting.config.xml" filterid="JamaicaAccountingConfig" />
  </Extension>
  <Extension path="/Application/JurisdictionConfiguration">
    <FilePath FilePath="..\jurisdiction\Jamaica\Application.config.xml" filterid="JamaicaApplicationConfig" />
  </Extension>
  <Extension path="/Gaming/Configuration">
    <FilePath FilePath="..\jurisdiction\Jamaica\Gaming.config.xml" filterid="JamaicaGamingConfig" />
  </Extension>
  <Extension path="/HardMeterMapping/Configuration">
    <FilePath FilePath="..\jurisdiction\DefaultAssets\HardMeterMapping.config.xml" filterid="JamaicaHardMeterMappingConfig" />
  </Extension>
  <Extension path="/Hardware/SoundConfiguration">
    <FilePath FilePath="..\jurisdiction\Jamaica\Sound.config.xml" filterid="JamaicaSoundConfig" />
