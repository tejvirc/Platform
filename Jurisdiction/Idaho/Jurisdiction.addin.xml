<Addin id="IdahoJurisdiction" namespace="Client12Addins" version="1.0">
  <Dependencies>
    <Addin id="Monaco.Kernel" version="1.0" />
    <Addin id="Monaco.Hardware" version="1.0" />
    <Addin id="Monaco.Accounting" version="1.0" />
    <Addin id="Monaco.Application" version="1.0" />
    <Addin id="Monaco.Application.UI" version="1.0" />
    <Addin id="Aristocrat.Monaco.Gaming" version="1.0" />
    <Addin id="SelectableJurisdiction" version="1.0" />
    <Addin id="AccountingRunnable" version="1.0" />
    <Addin id="SasBase" version="1.0" />
    <Addin id="SasConfiguration" version="1.0" />
    <Addin id="Sas.Client" version="1.0" />
  </Dependencies>
  <Runtime>
    <Import assembly="..\..\bin\Aristocrat.Monaco.Kernel.dll" />
    <Import assembly="..\..\bin\Aristocrat.Monaco.Gaming.Class3.dll" />
  </Runtime>
  <Extension path="/Kernel/SelectableAddinConfiguration/Jurisdiction">
    <SelectableJurisdiction name="Idaho" description="Top level Idaho jurisdiction group" id="JUR000102,JUR000998,JUR000999,JUR000033">
      <AddinConfigurationGroupReference name="BaseOperatorMenus" />
      <AddinConfigurationGroupReference name="BaseConfigWizardPages" />
      <ExtensionPointConfiguration extensionPath="/Platform/Discovery/Configuration">
        <NodeSpecification addinId="Client12Addins.IdahoJurisdiction" filterid="IdahoHardwareConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/TiltLogger/Configuration">
        <NodeSpecification addinId="Client12Addins.IdahoJurisdiction" filterid="IdahoTiltLoggerConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/DoorMonitor/Configuration">
        <NodeSpecification addinId="Client12Addins.IdahoJurisdiction" filterid="IdahoDoorMonitorConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/ConfigWizard/Configuration">
        <NodeSpecification addinId="Client12Addins.IdahoJurisdiction" filterid="IdahoConfigWizardConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/OperatorMenu/Configuration">
        <NodeSpecification addinId="Client12Addins.IdahoJurisdiction" filterid="IdahoOperatorMenuConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/OperatorMenu/Sound/Configuration">
        <NodeSpecification addinId="Client12Addins.IdahoJurisdiction" filterid="IdahoOperatorMenuSoundConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/OperatorMenu/GamePreferences/Sounds">
        <NodeSpecification addinId="Client12Addins.IdahoJurisdiction" filterid="IdahoGamePreferencesSounds" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Locale/Configuration">
        <NodeSpecification addinId="Client12Addins.IdahoJurisdiction" filterid="IdahoLocaleConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Lobby/Configuration">
        <NodeSpecification addinId="Client12Addins.IdahoJurisdiction" filterid="IdahoLobbyConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/Configuration">
        <NodeSpecification addinId="Client12Addins.IdahoJurisdiction" filterid="IdahoAccountingConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Application/JurisdictionConfiguration">
        <NodeSpecification addinId="Client12Addins.IdahoJurisdiction" filterid="IdahoApplicationConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Gaming/Configuration">
        <NodeSpecification addinId="Client12Addins.IdahoJurisdiction" filterid="IdahoGamingConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/TowerLight/Configuration">
        <NodeSpecification addinId="Client12Addins.IdahoJurisdiction" filterid="IdahoTowerLightConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Hardware/PrintableRegions">
        <NodeSpecification addinId="Client12Addins.IdahoJurisdiction" filterid="IdahoPrintableRegionsConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Hardware/PrintableTemplates">
        <NodeSpecification addinId="Client12Addins.IdahoJurisdiction" filterid="IdahoPrintableTemplatesConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Hardware/PrinterOverrides">
        <NodeSpecification addinId="Client12Addins.IdahoJurisdiction" filterid="IdahoPrinterOverridesConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Application/OperatorMenu/DisplayMeters">
        <NodeSpecification addinId="Client12Addins.IdahoJurisdiction" filterid="IdahoDisplayMetersConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/TransferOutProviders">
        <NodeSpecification addinId="Client12Addins.Monaco.Accounting" typeName="Aristocrat.Monaco.Accounting.Wat.WatOffProvider" order="0" filterid="IdahoAccountingTransferProviders" />
        <NodeSpecification addinId="Client12Addins.Monaco.Accounting" typeName="Aristocrat.Monaco.Accounting.Vouchers.VoucherOutProvider" order="1" filterid="IdahoAccountingTransferProviders" />
        <NodeSpecification addinId="Client12Addins.Monaco.Accounting" typeName="Aristocrat.Monaco.Accounting.Handpay.HandpayProvider" order="2" filterid="IdahoAccountingTransferProviders" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/TransactionHistories">
        <NodeSpecification addinId="Client12Addins.IdahoJurisdiction" filterid="IdahoTransactionConfiguration" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/BootExtender">
        <NodeSpecification addinId="Client12Addins.IdahoJurisdiction" filterid="IdahoAccountingRunnable" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/ErrorMessage/Configuration">
        <NodeSpecification addinId="Client12Addins.IdahoJurisdiction" filterid="IdahoErrorMessageConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/SAS/DefaultConfiguration">
        <NodeSpecification addinId="Client12Addins.IdahoJurisdiction" filterid="IdahoSASDefaultConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/HardMeterMapping/Configuration">
        <NodeSpecification addinId="Client12Addins.IdahoJurisdiction" filterid="IdahoHardMeterMappingConfig" />
      </ExtensionPointConfiguration>
    </SelectableJurisdiction>
  </Extension>
  <Extension path="/SAS/DefaultConfiguration">
    <FilePath FilePath="..\jurisdiction\Idaho\SASDefault.config.xml" filterid="IdahoSASDefaultConfig" />
  </Extension>
  <Extension path="/Platform/Discovery/Configuration">
    <FilePath FilePath="..\jurisdiction\Idaho\ConfiguredDevices.config.xml" filterid="IdahoHardwareConfig" />
  </Extension>
  <Extension path="/TiltLogger/Configuration">
    <FilePath FilePath="..\jurisdiction\Idaho\TiltLogger.config.xml" filterid="IdahoTiltLoggerConfig" />
  </Extension>
  <Extension path="/DoorMonitor/Configuration">
    <DoorMonitorConfiguration>
      <DoorOpenAlarm RepeatSeconds="0" LoopCount="7" OperatorCanCancel="true" />
    </DoorMonitorConfiguration>
  </Extension>
  <Extension path="/ConfigWizard/Configuration">
    <FilePath FilePath="..\jurisdiction\Idaho\ConfigWizard.config.xml" filterid="IdahoConfigWizardConfig" />
  </Extension>
  <Extension path="/OperatorMenu/Configuration">
    <FilePath FilePath="..\jurisdiction\Idaho\OperatorMenu.config.xml" filterid="IdahoOperatorMenuConfig" />
  </Extension>
  <Extension path="/Locale/Configuration" filterid="IdahoLocaleConfig">
    <FilePath FilePath="..\jurisdiction\Idaho\Locale.config.xml" />
  </Extension>
  <Extension path="/Lobby/Configuration">
    <FilePath FilePath="..\jurisdiction\Idaho\Lobby.config.xml" filterid="IdahoLobbyConfig" />
  </Extension>
  <Extension path="/Accounting/Configuration">
    <FilePath FilePath="..\jurisdiction\Idaho\Accounting.config.xml" filterid="IdahoAccountingConfig" />
  </Extension>
  <Extension path="/Application/JurisdictionConfiguration">
    <FilePath FilePath="..\jurisdiction\Idaho\Application.config.xml" filterid="IdahoApplicationConfig" />
  </Extension>
  <Extension path="/Gaming/Configuration">
    <FilePath FilePath="..\jurisdiction\Idaho\Gaming.config.xml" filterid="IdahoGamingConfig" />
  </Extension>
  <Extension path="/HardMeterMapping/Configuration">
    <FilePath FilePath="..\jurisdiction\DefaultAssets\HardMeterMapping.config.xml" filterid="IdahoHardMeterMappingConfig" />
  </Extension>
  <Extension path="/Sound/Configuration">
    <FilePath FilePath="..\jurisdiction\DefaultAssets\Sound.config.xml" filterid="IdahoSoundConfig" />
  </Extension>
  <Extension path="/TowerLight/Configuration">
    <FilePath FilePath="..\jurisdiction\Idaho\TowerLight.config.xml" filterid="IdahoTowerLightConfig" />
  </Extension>
  <Extension path="/Hardware/PrintableRegions">
    <FilePath FilePath="..\jurisdiction\Idaho\printable_regions.xml" filterid="IdahoPrintableRegionsConfig" />
  </Extension>
  <Extension path="/Hardware/PrintableTemplates">
    <FilePath FilePath="..\jurisdiction\Idaho\printable_templates.xml" filterid="IdahoPrintableTemplatesConfig" />
  </Extension>
  <Extension path="/Hardware/PrinterOverrides">
    <FilePath FilePath="..\jurisdiction\Idaho\PrinterOverrides.xml" filterid="IdahoPrinterOverridesConfig" />
  </Extension>
  <Extension path="/ErrorMessage/Configuration">
    <FilePath FilePath="..\jurisdiction\Idaho\ErrorMessage.config.xml" filterid="IdahoErrorMessageConfig" />
  </Extension>
  <Extension path="/Application/OperatorMenu/DisplayMeters">
    <FilePath FilePath="..\jurisdiction\Idaho\DisplayMeters.config.xml" filterid="IdahoDisplayMetersConfig" />
  </Extension>
  <Extension path="/Accounting/TransactionHistories" filterid="IdahoTransactionConfiguration">
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.BillTransaction" maxTransactions="50" persistenceLevel="Critical" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.Handpay.HandpayTransaction" maxTransactions="50" persistenceLevel="Critical" isPrintable="true" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.VoucherInTransaction" maxTransactions="50" persistenceLevel="Critical" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.VoucherOutTransaction" maxTransactions="50" persistenceLevel="Critical" isPrintable="true" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.Wat.WatTransaction" maxTransactions="100" persistenceLevel="Critical" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.WatOnTransaction" maxTransactions="100" persistenceLevel="Critical" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Gaming.Contracts.Progressives.JackpotTransaction" maxTransactions="50" persistenceLevel="Critical" isPrintable="true" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Gaming.Contracts.Bonus.BonusTransaction" maxTransactions="100" persistenceLevel="Critical" isPrintable="true" />
  </Extension>
  <Extension path="/Accounting/BootExtender" filterid="IdahoAccountingRunnable">
    <BootExtender type="Aristocrat.Monaco.Gaming.Class3.Class3Runnable" />
  </Extension>
</Addin>