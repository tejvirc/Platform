<Addin id="DenmarkJurisdiction" namespace="Client12Addins" version="1.0">

  <Dependencies>
    <Addin id="Monaco.Kernel" version="1.0" />
    <Addin id="Monaco.Hardware" version="1.0" />
    <Addin id="Monaco.Accounting" version="1.0" />
    <Addin id="Monaco.Application" version="1.0" />
    <Addin id="Monaco.Application.UI" version="1.0" />
    <Addin id="Aristocrat.Monaco.Gaming" version="1.0" />
    <Addin id="SelectableJurisdiction" version="1.0" />
    <Addin id="AccountingRunnable" version="1.0" />
    <Addin id="SasBase" version="1.0" />
    <Addin id="SasConfiguration" version="1.0" />
    <Addin id="Sas.Client" version="1.0" />
  </Dependencies>

  <Runtime>
    <Import assembly="..\..\bin\Aristocrat.Monaco.Kernel.dll" />
    <Import assembly="..\..\bin\Aristocrat.Monaco.Gaming.Class3.dll" />
  </Runtime>

  <Extension path="/Kernel/SelectableAddinConfiguration/Jurisdiction">
    <SelectableJurisdiction name="Denmark" description="Top level Denmark jurisdiction group" id="JUR000152,JUR000998,JUR000999,JUR000033">
      <AddinConfigurationGroupReference name="BaseOperatorMenus" />
      <AddinConfigurationGroupReference name="BaseConfigWizardPages" />
      <ExtensionPointConfiguration extensionPath="/Platform/Discovery/Configuration">
        <NodeSpecification addinId="Client12Addins.DenmarkJurisdiction" filterid="DenmarkHardwareConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/TiltLogger/Configuration">
        <NodeSpecification addinId="Client12Addins.DenmarkJurisdiction" filterid="DenmarkTiltLoggerConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/DoorMonitor/Configuration">
        <NodeSpecification addinId="Client12Addins.DenmarkJurisdiction" filterid="DenmarkDoorMonitorConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/ConfigWizard/Configuration">
        <NodeSpecification addinId="Client12Addins.DenmarkJurisdiction" filterid="DenmarkConfigWizardConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/OperatorMenu/Configuration">
        <NodeSpecification addinId="Client12Addins.DenmarkJurisdiction" filterid="DenmarkOperatorMenuConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/OperatorMenu/Sound/Configuration">
        <NodeSpecification addinId="Client12Addins.DenmarkJurisdiction" filterid="DenmarkOperatorMenuSoundConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/OperatorMenu/GamePreferences/Sounds">
        <NodeSpecification addinId="Client12Addins.DenmarkJurisdiction" filterid="DenmarkGamePreferencesSounds" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Locale/Configuration">
        <NodeSpecification addinId="Client12Addins.DenmarkJurisdiction" filterid="DenmarkLocaleConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Locale/Overrides">
        <NodeSpecification addinId="Client12Addins.DenmarkJurisdiction" filterid="DenmarkLocaleOverrides" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Lobby/Configuration">
        <NodeSpecification addinId="Client12Addins.DenmarkJurisdiction" filterid="DenmarkLobbyConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/Configuration">
        <NodeSpecification addinId="Client12Addins.DenmarkJurisdiction" filterid="DenmarkAccountingConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Application/JurisdictionConfiguration">
        <NodeSpecification addinId="Client12Addins.DenmarkJurisdiction" filterid="DenmarkApplicationConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Gaming/Configuration">
        <NodeSpecification addinId="Client12Addins.DenmarkJurisdiction" filterid="DenmarkGamingConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/TowerLight/Configuration">
        <NodeSpecification addinId="Client12Addins.DenmarkJurisdiction" filterid="DenmarkTowerLightConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Hardware/PrintableRegions">
        <NodeSpecification addinId="Client12Addins.DenmarkJurisdiction" filterid="DenmarkPrintableRegionsConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Hardware/PrintableTemplates">
        <NodeSpecification addinId="Client12Addins.DenmarkJurisdiction" filterid="DenmarkPrintableTemplatesConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Hardware/PrinterOverrides">
        <NodeSpecification addinId="Client12Addins.DenmarkJurisdiction" filterid="DenmarkPrinterOverridesConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Application/OperatorMenu/DisplayMeters">
        <NodeSpecification addinId="Client12Addins.DenmarkJurisdiction" filterid="DenmarkDisplayMetersConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/TransferOutProviders">
        <NodeSpecification addinId="Client12Addins.Monaco.Accounting" typeName="Aristocrat.Monaco.Accounting.Wat.WatOffProvider" order="0" filterid="DenmarkAccountingTransferProviders"/>
        <NodeSpecification addinId="Client12Addins.Monaco.Accounting" typeName="Aristocrat.Monaco.Accounting.Vouchers.VoucherOutProvider" order="1" filterid="DenmarkAccountingTransferProviders"/>
        <NodeSpecification addinId="Client12Addins.Monaco.Accounting" typeName="Aristocrat.Monaco.Accounting.Handpay.HandpayProvider" order="2" filterid="DenmarkAccountingTransferProviders"/>
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/TransactionHistories">
        <NodeSpecification addinId="Client12Addins.DenmarkJurisdiction" filterid="DenmarkTransactionConfiguration" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Accounting/BootExtender">
        <NodeSpecification addinId="Client12Addins.DenmarkJurisdiction" filterid="DenmarkAccountingRunnable" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/ErrorMessage/Configuration">
        <NodeSpecification addinId="Client12Addins.DenmarkJurisdiction" filterid="DenmarkErrorMessageConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/SAS/DefaultConfiguration">
        <NodeSpecification addinId="Client12Addins.DenmarkJurisdiction" filterid="DenmarkSASDefaultConfig" />
      </ExtensionPointConfiguration>
	  <ExtensionPointConfiguration extensionPath="/HardMeterMapping/Configuration">
        <NodeSpecification addinId="Client12Addins.DenmarkJurisdiction" filterid="DenmarkHardMeterMappingConfig" />
      </ExtensionPointConfiguration>
      <ExtensionPointConfiguration extensionPath="/Currency/Configuration">
        <NodeSpecification addinId="Client12Addins.DenmarkJurisdiction" filterid="DenmarkCurrencyConfig" />
      </ExtensionPointConfiguration>
    </SelectableJurisdiction>
  </Extension>

  <Extension path="/Currency/Configuration">
    <FilePath FilePath="..\jurisdiction\Denmark\Currencies.config.xml" filterid="DenmarkCurrencyConfig" />
  </Extension>

  <Extension path="/SAS/DefaultConfiguration">
    <FilePath FilePath="..\jurisdiction\Denmark\SASDefault.config.xml" filterid="DenmarkSASDefaultConfig" />
  </Extension>

  <Extension path="/Platform/Discovery/Configuration">
    <FilePath FilePath="..\jurisdiction\Denmark\ConfiguredDevices.config.xml" filterid="DenmarkHardwareConfig" />
  </Extension>

  <Extension path="/TiltLogger/Configuration">
    <FilePath FilePath="..\jurisdiction\Denmark\TiltLogger.config.xml" filterid="DenmarkTiltLoggerConfig" />
  </Extension>

  <Extension path="/DoorMonitor/Configuration">
    <DoorMonitorConfiguration>
      <DoorOpenAlarm RepeatSeconds="0" LoopCount="7" OperatorCanCancel="true">
        <FilePath FilePath="..\jurisdiction\DefaultAssets\alarm.ogg" filterid="DenmarkDoorMonitorConfig" />
      </DoorOpenAlarm>
    </DoorMonitorConfiguration>
  </Extension>

  <Extension path="/ConfigWizard/Configuration">
    <FilePath FilePath="..\jurisdiction\Denmark\ConfigWizard.config.xml" filterid="DenmarkConfigWizardConfig" />
  </Extension>

  <Extension path="/OperatorMenu/Sound/Configuration">
    <FilePath FilePath="..\jurisdiction\DefaultAssets\ding.ogg" Name="Ding" filterid="DenmarkOperatorMenuSoundConfig" />
    <FilePath FilePath="..\jurisdiction\DefaultAssets\feature_bell.ogg" Name="Feature Bell" filterid="DenmarkOperatorMenuSoundConfig" />
    <FilePath FilePath="..\jurisdiction\DefaultAssets\touch_sound.ogg" Name="Touch Sound" filterid="DenmarkOperatorMenuSoundConfig" />
    <FilePath FilePath="..\jurisdiction\DefaultAssets\collect.ogg" Name="Collect" filterid="DenmarkOperatorMenuSoundConfig" />
    <FilePath FilePath="..\jurisdiction\DefaultAssets\snd_print_ticket.ogg" Name="Send Print Ticket" filterid="DenmarkOperatorMenuSoundConfig" />
  </Extension>

  <Extension path="/OperatorMenu/GamePreferences/Sounds">
    <FilePath FilePath="..\jurisdiction\DefaultAssets\ding.ogg" Name="Ding" filterid="DenmarkGamePreferencesSounds" />
    <FilePath FilePath="..\jurisdiction\DefaultAssets\ReelClick.ogg" Name="ReelClick" filterid="DenmarkGamePreferencesSounds" />
    <FilePath FilePath="..\jurisdiction\DefaultAssets\CardFlip.ogg" Name="CardFlip" filterid="DenmarkGamePreferencesSounds" />
    <FilePath FilePath="..\jurisdiction\DefaultAssets\BallDrop.ogg" Name="BallDrop" filterid="DenmarkGamePreferencesSounds" />
  </Extension>

  <Extension path="/OperatorMenu/Configuration">
    <FilePath FilePath="..\jurisdiction\Denmark\OperatorMenu.config.xml" filterid="DenmarkOperatorMenuConfig" />
  </Extension>

  <Extension path="/Locale/Configuration" filterid="DenmarkLocaleConfig">
    <FilePath FilePath="..\jurisdiction\Denmark\Locale.config.xml" />
  </Extension>
  
  <Extension path="/Locale/Overrides" filterid="DenmarkLocaleOverrides">
    <FilePath FilePath="..\jurisdiction\DefaultAssets\resources\C3\" />
    <FilePath FilePath="..\jurisdiction\Denmark\resources\" />
  </Extension>

  <Extension path="/Lobby/Configuration">
    <FilePath FilePath="..\jurisdiction\Denmark\Lobby.config.xml" filterid="DenmarkLobbyConfig" />
  </Extension>

  <Extension path="/Accounting/Configuration">
    <FilePath FilePath="..\jurisdiction\Denmark\Accounting.config.xml" filterid="DenmarkAccountingConfig" />
  </Extension>

  <Extension path="/Application/JurisdictionConfiguration">
    <FilePath FilePath="..\jurisdiction\Denmark\Application.config.xml" filterid="DenmarkApplicationConfig" />
  </Extension>

  <Extension path="/Gaming/Configuration">
    <FilePath FilePath="..\jurisdiction\Denmark\Gaming.config.xml" filterid="DenmarkGamingConfig" />
  </Extension>

  <Extension path="/TowerLight/Configuration">
    <FilePath FilePath="..\jurisdiction\Denmark\TowerLight.config.xml" filterid="DenmarkTowerLightConfig" />
  </Extension>
  
  <Extension path="/HardMeterMapping/Configuration">
    <FilePath FilePath="..\jurisdiction\DefaultAssets\HardMeterMapping.Eurasia.config.xml" filterid="DenmarkHardMeterMappingConfig" />
  </Extension>
  
  <Extension path="/Hardware/PrintableRegions">
    <FilePath FilePath="..\jurisdiction\Denmark\printable_regions.xml" filterid="DenmarkPrintableRegionsConfig" />
  </Extension>

  <Extension path="/Hardware/PrintableTemplates">
    <FilePath FilePath="..\jurisdiction\Denmark\printable_templates.xml"
              filterid="DenmarkPrintableTemplatesConfig" />
  </Extension>

  <Extension path="/Hardware/PrinterOverrides">
    <FilePath FilePath="..\jurisdiction\Denmark\PrinterOverrides.xml" filterid="DenmarkPrinterOverridesConfig" />
  </Extension>

  <Extension path="/ErrorMessage/Configuration">
    <FilePath FilePath="..\jurisdiction\Denmark\ErrorMessage.config.xml" filterid="DenmarkErrorMessageConfig" />
  </Extension>

  <Extension path="/Application/OperatorMenu/DisplayMeters">
    <FilePath FilePath="..\jurisdiction\Denmark\DisplayMeters.config.xml" filterid="DenmarkDisplayMetersConfig" />
  </Extension>

  <Extension path="/Accounting/TransactionHistories" filterid="DenmarkTransactionConfiguration">
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.BillTransaction" 
	                            maxTransactions="50"
                                persistenceLevel="Critical" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.Handpay.HandpayTransaction" 
	                            maxTransactions="50"
                                persistenceLevel="Critical" 
								isPrintable="true" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.VoucherInTransaction"
                                maxTransactions="50"
                                persistenceLevel="Critical" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.VoucherOutTransaction"
                                maxTransactions="50"
                                persistenceLevel="Critical"
                                isPrintable="true" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.Wat.WatTransaction"
                                maxTransactions="100"
                                persistenceLevel="Critical" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Accounting.Contracts.WatOnTransaction"
                                maxTransactions="100"
                                persistenceLevel="Critical" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Gaming.Contracts.Progressives.JackpotTransaction" 
	                            maxTransactions="50" 
								persistenceLevel="Critical" 
								isPrintable="true" />
    <TransactionHistoryProvider type="Aristocrat.Monaco.Gaming.Contracts.Bonus.BonusTransaction" 
	                            maxTransactions="100" 
								persistenceLevel="Critical" 
								isPrintable="true" />
  </Extension>

  <Extension path="/Accounting/BootExtender" filterid="DenmarkAccountingRunnable">
    <BootExtender type="Aristocrat.Monaco.Gaming.Class3.Class3Runnable" />
  </Extension>

</Addin>
