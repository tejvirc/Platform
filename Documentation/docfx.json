          "src": "../Accounting"
        }
      ],
      "dest": "obj/accounting",
      "filter": "filterConfig.yml",
      "properties": {
        "TargetFramework": "net472"
      }
    },
    {
      "src": [
        {
          "files": ["**/*.csproj"],
          "exclude": ["**/*Test*/**", "**/bin/**", "**/obj/**"],
