      }
    },
    {
      "src": [
        {
          "files": ["**/*.csproj"],
          "exclude": ["**/*Test*/**", "**/bin/**", "**/obj/**"],
          "src": "../Shared/Aristocrat.Monaco.Localization"
        },
        {
          "files": ["**/*.csproj"],
          "exclude": ["**/*Test*/**", "**/bin/**", "**/obj/**"],
          "src": "../Shared/Aristocrat.Monaco.Common"
        },
        {
          "files": ["**/*.csproj"],
          "exclude": ["**/*Test*/**", "**/bin/**", "**/obj/**"],
          "src": "../Shared/Aristocrat.Monaco.UI.Common"
        }
      ],
      "dest": "obj/common",
      "filter": "filterConfig.yml",
      "properties": {
        "TargetFramework": "net472"
      }
    },
    {
      "src": [
        {
          "files": ["**/*.csproj"],
          "exclude": ["**/*Test*/**", "**/bin/**", "**/obj/**"],
