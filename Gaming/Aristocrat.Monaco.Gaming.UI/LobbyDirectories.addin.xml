<?xml version="1.0" encoding="utf-8" ?>
<Addin id="LobbyDirectories" namespace="Client12Addins" version="1.0" >

  <Dependencies>
    <Addin id="Monaco.Kernel" version="1.0" />
  </Dependencies>

  <Extension path="/Kernel/PathMapping">
    <PathMapping platformPath="/BrowserCache" fileSystemPath="..\cache" createIfNotExists="true" />
  </Extension>

  <ExtensionPoint path = "/Browser/Configuration">
    <ExtensionNode name="FilePath" type="Aristocrat.Monaco.Kernel.FilePathExtensionNode" />
  </ExtensionPoint>

</Addin>