<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:bink="clr-namespace:ManagedBink;assembly=BinkViewerControl"        
    xmlns:converters="clr-namespace:Aristocrat.Monaco.Gaming.UI.Converters"
    xmlns:viewModels="clr-namespace:Aristocrat.Monaco.Gaming.UI.ViewModels"
    xmlns:models="clr-namespace:Aristocrat.Monaco.Gaming.UI.Models"
    xmlns:lobby="clr-namespace:Aristocrat.Monaco.Gaming.UI.Views.Lobby"
    xmlns:common="http://monaco.aristocrat.com/common"
    xmlns:controls="clr-namespace:Aristocrat.Monaco.Gaming.UI.Views.Controls"
    xmlns:commonControls="clr-namespace:Aristocrat.Monaco.UI.Common.Controls;assembly=Aristocrat.Monaco.UI.Common"
    xmlns:gif="http://wpfanimatedgif.codeplex.com"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:b="http://schemas.microsoft.com/xaml/behaviors">

    <!-- Apply MahApps.Metro styles to all controls -->
    <ResourceDictionary.MergedDictionaries>
        <!-- MahApps.Metro resource dictionaries. Make sure that all file names are Case Sensitive! -->
        <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Controls.xaml" />
        <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Fonts.xaml" />
         <!--<ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Colors.xaml" />-->
        <!-- Accent and AppTheme setting -->
        <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Themes/Light.Blue.xaml" />
         <!--<ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Accents/BaseLight.xaml" />-->

        <ResourceDictionary Source="../../Skins/Styles.xaml"/>
        <ResourceDictionary Source="/Aristocrat.Monaco.UI.Common;component/Skins/Converters.xaml"/>

        <!-- Jurisdiction xaml dictionary is loaded dynamically; see SkinLoader -->
    </ResourceDictionary.MergedDictionaries>
    <converters:AttractActiveToVisibilityConverter x:Key="AttractToVisibilityConv" />
    <converters:ViewFiltersToBoolConverter x:Key="FiltersToBoolConv" />
    <converters:DecoratorIconVisibilityConverter x:Key="AnimatedIconVisibilityConv" />
    <converters:DecoratorIconVisibilityConverter x:Key="StaticIconVisibilityConv"
                                                 InvertEnabledValue="True" />
    <converters:ProgressiveLabelVisibilityConverter x:Key="ProgressiveLabelVisibilityConv" />
    <converters:MarginConverter x:Key="MarginConverter" />
    <converters:DenomPanelWidthConverter x:Key="DenomPanelWidthConv" />
    <converters:DenomPanelHeightConverter x:Key="DenomPanelHeightConv" />
    <converters:GameIconHeightConverter x:Key="GameIconHeightConv" />
    <converters:GameIconImageHeightConverter x:Key="GameIconImageHeightConv" />
    <converters:GameTabMarginConverter x:Key="GameTabMarginConv" />
    <converters:GridSpacingConverter x:Key="GridSpacingConverter" />
    <converters:LobbyColumnWidthConverter x:Key="ColumnWidthConverter" />
    <converters:LobbyOuterColumnWidthConverter x:Key="LobbyOuterColumnWidthConv" />
    <converters:LobbyColumnMinWidthConverter x:Key="ColumnMinWidthConv" />
    <converters:ProgressiveLabelSizeConverter x:Key="ProgressiveLabelSizeConv" />
    <converters:MultiParameterCommandConverter x:Key="MultiParameterCommandConv" />
    <converters:CharacterToResourceImageConverter x:Key="CharacterToResourceImageConv" />
    <converters:ScaleByResolutionConverter x:Key="ScaleByResolutionConv" />
    <converters:LobbyProgressiveBannerVisibilityConverter x:Key="LobbyProgressiveBannerVisibilityConv" />
    <converters:GameIconJackpotTextVisibilityConverter x:Key="GameIconJackpotTextVisibilityConv" />
    <converters:CommaMarginConverter x:Key="CommaMarginConv" />
    
    <sys:Boolean x:Key="FalseValue">False</sys:Boolean>
    <sys:Boolean x:Key="TrueValue">True</sys:Boolean>
    <sys:Double x:Key="ExtraLargeGameIconDenomIconWidth">110</sys:Double>
    <sys:Double x:Key="ExtraLargeGameIconDenomIconHeight">70</sys:Double>
    <sys:Double x:Key="ExtraLargeGameIconDenomPanelMaxWidth">620</sys:Double>
    <sys:Double x:Key="ExtraLargeGameIconDenomPanelMaxHeight">150</sys:Double>
    <Thickness x:Key="ExtraLargeGameIconDenomIconMargin"
               Left="15"
               Top="30"
               Right="15"
               Bottom="30" />
    <Thickness x:Key="LightningLinkGrandCommaShiftMargin"
               Left="0"
               Top="20"
               Right="0"
               Bottom="0" />
    <Thickness x:Key="LightningLinkMajorCommaShiftMargin"
               Left="0"
               Top="9"
               Right="0"
               Bottom="0" />
    
    <Storyboard x:Key="AttractHaloFadeInStoryboard"
                AutomationProperties.AutomationId="Storyboard0_ADFE09ED">
        <DoubleAnimation Storyboard.TargetProperty="Opacity"
                         From="0.0"
                         To="1.0"
                         Duration="0:0:2.0"
                         BeginTime="0:0:0"
                         AutoReverse="False"
                         AutomationProperties.AutomationId="DoubleAnimation1_ADFE09ED">
            <DoubleAnimation.EasingFunction>
                <QuadraticEase EasingMode="EaseInOut"
                               AutomationProperties.AutomationId="QuadraticEase2_ADFE09ED" />
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>
    </Storyboard>
    
    <Storyboard x:Key="AttractHaloFadeOutStoryboard"
                AutomationProperties.AutomationId="Storyboard3_ADFE09ED">
        <DoubleAnimation Storyboard.TargetProperty="Opacity"
                         From="1.0"
                         To="0.0"
                         Duration="0:0:2.0"
                         BeginTime="0:0:0"
                         AutoReverse="False"
                         AutomationProperties.AutomationId="DoubleAnimation4_ADFE09ED">
            <DoubleAnimation.EasingFunction>
                <QuadraticEase EasingMode="EaseInOut"
                               AutomationProperties.AutomationId="QuadraticEase5_ADFE09ED" />
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>
    </Storyboard>
    
    <Storyboard x:Key="AttractFadeInStoryboard"
                AutomationProperties.AutomationId="Storyboard6_ADFE09ED">
        <DoubleAnimation Storyboard.TargetProperty="Opacity"
                         From="0.0"
                         To="1.0"
                         Duration="0:0:0.1"
                         BeginTime="0:0:0"
                         AutoReverse="False"
                         AutomationProperties.AutomationId="DoubleAnimation7_ADFE09ED">
            <DoubleAnimation.EasingFunction>
                <QuadraticEase EasingMode="EaseInOut"
                               AutomationProperties.AutomationId="QuadraticEase8_ADFE09ED" />
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>
    </Storyboard>
    
    <Storyboard x:Key="AttractFadeOutStoryboard"
                AutomationProperties.AutomationId="Storyboard9_ADFE09ED">
        <DoubleAnimation Storyboard.TargetProperty="Opacity"
                         From="1.0"
                         To="0.0"
                         Duration="0:0:0.1"
                         BeginTime="0:0:0"
                         AutoReverse="False"
                         AutomationProperties.AutomationId="DoubleAnimation10_ADFE09ED">
            <DoubleAnimation.EasingFunction>
                <QuadraticEase EasingMode="EaseInOut"
                               AutomationProperties.AutomationId="QuadraticEase11_ADFE09ED" />
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>
    </Storyboard>
    
    <LinearGradientBrush x:Key="JackpotTextGradient"
                         StartPoint=".5,0"
                         EndPoint=".5,1"
                         Opacity="1"
                         AutomationProperties.AutomationId="LinearGradientBrush12_ADFE09ED">
        <GradientStop Color="#FFF100"
                      Offset=".5"
                      AutomationProperties.AutomationId="GradientStop13_ADFE09ED" />
        <GradientStop Color="#FFB000"
                      Offset=".1"
                      AutomationProperties.AutomationId="GradientStop14_ADFE09ED" />
        <GradientStop Color="#FFFF00"
                      Offset=".45"
                      AutomationProperties.AutomationId="GradientStop15_ADFE09ED" />
        <GradientStop Color="#FFFF00"
                      Offset=".65"
                      AutomationProperties.AutomationId="GradientStop16_ADFE09ED" />
        <GradientStop Color="#FFB300"
                      Offset=".70"
                      AutomationProperties.AutomationId="GradientStop17_ADFE09ED" />
        <GradientStop Color="#FFD600"
                      Offset=".95"
                      AutomationProperties.AutomationId="GradientStop18_ADFE09ED" />
    </LinearGradientBrush>
    
    <DataTemplate x:Key="LobbyViewLandscapeTemplate"
                  DataType="{x:Type viewModels:LobbyViewModel}">
        <Grid AutomationProperties.AutomationId="Grid0_1F333AB0">
            <Grid.Background>
                <ImageBrush ImageSource="{DynamicResource BottomBackground}"
                            AutomationProperties.AutomationId="ImageBrush27_ADFE09ED" />
            </Grid.Background>
            <Viewbox AutomationProperties.AutomationId="Viewbox1_1F333AB0">
                <Grid Width="1920"
                      Height="1080"
                      AutomationProperties.AutomationId="Grid2_1F333AB0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height=".04*"
                                       AutomationProperties.AutomationId="RowDefinition3_1F333AB0" />
                        <RowDefinition Height=".77*"
                                       AutomationProperties.AutomationId="RowDefinition4_1F333AB0" />
                        <RowDefinition Height=".06*"
                                       AutomationProperties.AutomationId="RowDefinition5_1F333AB0" />
                        <RowDefinition Height=".13*"
                                       AutomationProperties.AutomationId="RowDefinition6_1F333AB0" />
                    </Grid.RowDefinitions>
                    
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition
                                Width="{Binding IsExtraLargeGameIconTabActive, Converter={StaticResource LobbyOuterColumnWidthConv}}"
                                          AutomationProperties.AutomationId="ColumnDefinition7_1F333AB0">
                            <ColumnDefinition.MinWidth>
                                <MultiBinding Converter="{StaticResource ColumnMinWidthConv}">
                                    <Binding Path="GameCount" />
                                    <Binding Path="PreserveGameLayoutSideMargins" />
                                    <Binding Path="IsExtraLargeGameIconTabActive" />
                                </MultiBinding>
                            </ColumnDefinition.MinWidth>
                        </ColumnDefinition>
                        <ColumnDefinition AutomationProperties.AutomationId="ColumnDefinition8_1F333AB0">
                            <ColumnDefinition.Width>
                                <MultiBinding Converter="{StaticResource ColumnWidthConverter}">
                                    <Binding Path="GameCount" />
                                    <Binding Path="PreserveGameLayoutSideMargins" />
                                    <Binding Path="IsExtraLargeGameIconTabActive" />
                                </MultiBinding>
                            </ColumnDefinition.Width>
                        </ColumnDefinition>
                        <ColumnDefinition
                                Width="{Binding IsExtraLargeGameIconTabActive, Converter={StaticResource LobbyOuterColumnWidthConv}}"
                                          AutomationProperties.AutomationId="ColumnDefinition9_1F333AB0">
                            <ColumnDefinition.MinWidth>
                                <MultiBinding Converter="{StaticResource ColumnMinWidthConv}">
                                    <Binding Path="GameCount" />
                                    <Binding Path="PreserveGameLayoutSideMargins" />
                                    <Binding Path="IsExtraLargeGameIconTabActive" />
                                </MultiBinding>
                            </ColumnDefinition.MinWidth>
                        </ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    
                    <!-- **** HEADER **** -->
                    <Viewbox Grid.Row="0"
                             Grid.Column="1"
                             Visibility="{Binding IsTabView, Converter={StaticResource TrueCollapsedFalseVisible}}"
                             AutomationProperties.AutomationId="Viewbox10_1F333AB0">
                        <Image Stretch="None"
                               Source="{DynamicResource SelectAGame}"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Top"
                               RenderTransformOrigin="0.5 0.5"
                               AutomationProperties.AutomationId="Image11_1F333AB0">
                            <Image.RenderTransform>
                                <TranslateTransform X="0"
                                                    Y="{Binding ChooseGameOffsetY}"
                                                    AutomationProperties.AutomationId="TranslateTransform12_1F333AB0" />
                            </Image.RenderTransform>
                        </Image>
                    </Viewbox>
                    <Button Grid.Row="0"
                            Grid.Column="2"
                            HorizontalAlignment="Right"
                            VerticalAlignment="Top"
                            Margin="0 40 40 0"
                            Style="{StaticResource stlFlatButton}"
                            Command="{Binding Path=ResponsibleGamingDialogOpenCommand}"
                            Visibility="{Binding IsResponsibleGamingInfoVisible, Converter={StaticResource TrueVisibleFalseCollapsed}}"
                            AutomationProperties.AutomationId="Button13_1F333AB0">
                        <Image Source="{DynamicResource ResponsibleGaming}"
                               Stretch="None"
                               AutomationProperties.AutomationId="Image14_1F333AB0" />
                    </Button>
                    
                    <!-- **** Bartop UI **** -->
                    <Grid Grid.Row="0"
                          Grid.RowSpan="3"
                          Grid.Column="0"
                          Grid.ColumnSpan="3"
                          Width="1920"
                          Height="1080"
                          HorizontalAlignment="Stretch"
                          VerticalAlignment="Stretch"
                          Visibility="{Binding IsTabView, Converter={StaticResource TrueVisibleFalseCollapsed}}"
                          AutomationProperties.AutomationId="Grid15_1F333AB0">
                        <commonControls:CustomItemsControl x:Name="TabsControl"
                                                           ItemsSource="{Binding GameTabInfo.Tabs}">
                            <ItemsControl.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <Grid AutomationProperties.AutomationId="Grid16_1F333AB0" />
                                </ItemsPanelTemplate>
                            </ItemsControl.ItemsPanel>
                            <ItemsControl.ItemTemplate>
                                <DataTemplate DataType="{x:Type viewModels:GameTabInfo}">
                                    <commonControls:ImageTransparentPassThrough x:Name="GameTab"
                                                                                common:ImageHelper.SourceResourceKey="{Binding TabResourceKey}">
                                        <commonControls:ImageTransparentPassThrough.Style>
                                            <Style TargetType="{x:Type commonControls:ImageTransparentPassThrough}">
                                                <Setter Property="Panel.ZIndex"
                                                        Value="1" />
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding Enabled}"
                                                                 Value="True"
                                                                 AutomationProperties.AutomationId="DataTrigger17_1F333AB0">
                                                        <Setter Property="Panel.ZIndex"
                                                                Value="2" />
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </commonControls:ImageTransparentPassThrough.Style>
                                        <b:Interaction.Triggers>
                                            <b:EventTrigger EventName="MouseLeftButtonDown"
                                                            AutomationProperties.AutomationId="EventTrigger18_1F333AB0">
                                                <b:InvokeCommandAction
                                                        Command="{Binding DataContext.GameTabPressedCommand, ElementName=TabsControl}"
                                                                       CommandParameter="{Binding}" />
                                            </b:EventTrigger>
                                        </b:Interaction.Triggers>
                                    </commonControls:ImageTransparentPassThrough>
                                </DataTemplate>
                            </ItemsControl.ItemTemplate>
                        </commonControls:CustomItemsControl>
                        <ItemsControl x:Name="TabLabelsControl"
                                      ItemsSource="{Binding GameTabInfo.Tabs}"
                                      VerticalAlignment="Top"
                                      Margin="60 62 60 0"
                                      AutomationProperties.AutomationId="ItemsControl19_1F333AB0">
                            <ItemsControl.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <UniformGrid Columns="6"
                                                 Panel.ZIndex="10"
                                                 AutomationProperties.AutomationId="UniformGrid20_1F333AB0" />
                                </ItemsPanelTemplate>
                            </ItemsControl.ItemsPanel>
                            <ItemsControl.ItemTemplate>
                                <DataTemplate DataType="{x:Type viewModels:GameTabInfo}">
                                    <Viewbox Stretch="Uniform"
                                             StretchDirection="DownOnly"
                                             VerticalAlignment="Center"
                                             HorizontalAlignment="Center"
                                             IsHitTestVisible="False"
                                             Visibility="{Binding Enabled}"
                                             AutomationProperties.AutomationId="Viewbox21_1F333AB0">
                                        <Grid AutomationProperties.AutomationId="Grid22_1F333AB0">
                                            <Image common:ImageHelper.SourceResourceKey="{Binding LabelResourceKey}"
                                                   Margin="{Binding Path=., Converter={StaticResource GameTabMarginConv}}"
                                                   Visibility="{Binding HasAnimatedResource, Converter={StaticResource TrueCollapsedFalseVisible}}"
                                                   AutomationProperties.AutomationId="Image23_1F333AB0" />
                                            <Image
                                                    gif:ImageBehavior.AnimatedSource="{Binding LabelAnimatedResourcePath}"
                                                   Margin="{Binding Path=., Converter={StaticResource GameTabMarginConv}}"
                                                   Visibility="{Binding HasAnimatedResource, Converter={StaticResource TrueVisibleFalseCollapsed}}"
                                                   AutomationProperties.AutomationId="Image24_1F333AB0" />
                                        </Grid>
                                    </Viewbox>
                                </DataTemplate>
                            </ItemsControl.ItemTemplate>
                        </ItemsControl>
                        
                        <ItemsControl Panel.ZIndex="11"
                                      x:Name="SubTabsControl"
                                      ItemsSource="{Binding GameTabInfo.SubTabs}"
                                      HorizontalAlignment="Left"
                                      VerticalAlignment="Top"
                                      Height="69"
                                      Margin="{Binding GameTabInfo.SubTabsMargin}"
                                      AutomationProperties.AutomationId="ItemsControl25_1F333AB0">
                            <ItemsControl.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <StackPanel Orientation="Horizontal"
                                                AutomationProperties.AutomationId="StackPanel26_1F333AB0" />
                                </ItemsPanelTemplate>
                            </ItemsControl.ItemsPanel>
                            <ItemsControl.ItemTemplate>
                                <DataTemplate DataType="{x:Type viewModels:SubTabInfoViewModel}">
                                    <Grid
                                            Visibility="{Binding IsVisible, Converter={StaticResource TrueVisibleFalseCollapsed}}"
                                          AutomationProperties.AutomationId="Grid27_1F333AB0">
                                        <Image Source="{DynamicResource SubTabNormal}"
                                               AutomationProperties.AutomationId="Image28_1F333AB0">
                                            <b:Interaction.Triggers>
                                                <b:EventTrigger EventName="MouseLeftButtonDown"
                                                                AutomationProperties.AutomationId="EventTrigger29_1F333AB0">
                                                    <b:InvokeCommandAction
                                                            Command="{Binding DataContext.SubTabPressedCommand, ElementName=SubTabsControl}"
                                                                           CommandParameter="{Binding}" />
                                                </b:EventTrigger>
                                            </b:Interaction.Triggers>
                                        </Image>
                                        <Image common:ImageHelper.SourceResourceKey="{Binding ResourceKey}"
                                               Height="40"
                                               VerticalAlignment="Top"
                                               HorizontalAlignment="Center"
                                               Margin="0 9 0 0"
                                               IsHitTestVisible="False"
                                               AutomationProperties.AutomationId="Image30_1F333AB0" />
                                    </Grid>
                                </DataTemplate>
                            </ItemsControl.ItemTemplate>
                        </ItemsControl>
                    </Grid>
                    
                    <!-- **** GAME GRID **** -->
                    <ItemsControl x:Name="GameList"
                                  FocusVisualStyle="{x:Null}"
                                  Margin="{Binding Path=MarginInputs, Converter={StaticResource MarginConverter}, ConverterParameter=GameGrid}"
                                  Grid.Row="1"
                                  Grid.RowSpan="2"
                                  Grid.Column="1"
                                  ItemsSource="{Binding Path=DisplayedGameList}"
                                  HorizontalAlignment="Left"
                                  VerticalAlignment="Top"
                                  AutomationProperties.AutomationId="ItemsControl31_1F333AB0">
                        <ItemsControl.ItemsPanel>
                            <!-- Swap out the panel that arranges the items -->
                            <ItemsPanelTemplate>
                                <controls:GameLayoutPanel
                                        Spacing="{Binding MarginInputs, Converter={StaticResource GridSpacingConverter}}"
                                                          IsTabView="{Binding IsTabView}"
                                                          IsExtraLargeGameIconTabActive="{Binding IsExtraLargeGameIconTabActive}" />
                            </ItemsPanelTemplate>
                        </ItemsControl.ItemsPanel>
                        <!-- Specify the UI to render each item -->
                        <ItemsControl.ItemTemplate>
                            <DataTemplate DataType="{x:Type models:GameInfo}">
                                <Grid x:Name="GameTile"
                                      VerticalAlignment="Top"
                                      AutomationProperties.AutomationId="Grid32_1F333AB0">
                                    <Border Width="Auto"
                                            Height="{Binding Path=DataContext.MarginInputs, RelativeSource={RelativeSource AncestorType=ItemsControl}, Converter={StaticResource GameIconHeightConv}}"
                                            AutomationProperties.AutomationId="Border33_1F333AB0">
                                        <Grid HorizontalAlignment="Left"
                                              VerticalAlignment="Top"
                                              Height="{Binding Path=DataContext.MarginInputs, RelativeSource={RelativeSource AncestorType=ItemsControl}, Converter={StaticResource GameIconHeightConv}}"
                                              AutomationProperties.AutomationId="Grid34_1F333AB0">
                                            <Grid HorizontalAlignment="Center"
                                                  VerticalAlignment="Bottom"
                                                  Margin="0 0 0 46"
                                                  AutomationProperties.AutomationId="Grid35_1F333AB0">
                                                <Grid.Visibility>
                                                    <MultiBinding
                                                            Converter="{StaticResource ProgressiveLabelVisibilityConv}">
                                                        <Binding Path="HasProgressiveLabelDisplay" />
                                                        <Binding Path="DataContext.IsExtraLargeGameIconTabActive"
                                                                 RelativeSource="{RelativeSource AncestorType=ItemsControl}" />
                                                    </MultiBinding>
                                                </Grid.Visibility>
                                                <Image Stretch="None"
                                                       Source="{DynamicResource ProgressiveOverlay}"
                                                       RenderOptions.BitmapScalingMode="HighQuality"
                                                       VerticalAlignment="Center"
                                                       HorizontalAlignment="Center"
                                                       AutomationProperties.AutomationId="Image36_1F333AB0" />
                                                <Image Stretch="None"
                                                       Source="{DynamicResource ProgressiveBackground}"
                                                       RenderOptions.BitmapScalingMode="HighQuality"
                                                       Visibility="{Binding IsSelectedWithProgressiveLabel, Converter={StaticResource TrueCollapsedFalseVisible}}"
                                                       VerticalAlignment="Center"
                                                       HorizontalAlignment="Center"
                                                       AutomationProperties.AutomationId="Image37_1F333AB0" />
                                                <Image Stretch="None"
                                                       Source="{DynamicResource ProgressiveSelectedBackground}"
                                                       RenderOptions.BitmapScalingMode="HighQuality"
                                                       Visibility="{Binding IsSelectedWithProgressiveLabel, Converter={StaticResource TrueVisibleFalseCollapsed}}"
                                                       VerticalAlignment="Center"
                                                       HorizontalAlignment="Center"
                                                       AutomationProperties.AutomationId="Image38_1F333AB0" />
                                                <Image Stretch="None"
                                                       Source="{DynamicResource GameSelectedWithProgressivesOverlay}"
                                                       RenderOptions.BitmapScalingMode="HighQuality"
                                                       Visibility="{Binding IsSelectedWithProgressiveLabel, Converter={StaticResource TrueVisibleFalseCollapsed}}"
                                                       VerticalAlignment="Center"
                                                       HorizontalAlignment="Center"
                                                       AutomationProperties.AutomationId="Image39_1F333AB0" />
                                                <TextBlock Text="{Binding ProgressiveIndicatorText}"
                                                           Visibility="{Binding HasProgressiveLabelDisplay, Converter={StaticResource TrueVisibleFalseCollapsed}}"
                                                           FontFamily="Verdana"
                                                           Margin="{Binding IsSelectedWithProgressiveLabel, Converter={StaticResource MarginConverter}, ConverterParameter=ProgressiveOverlayText}"
                                                           HorizontalAlignment="Center"
                                                           VerticalAlignment="Center"
                                                           TextAlignment="Center"
                                                           FontWeight="ExtraBold"
                                                           FontSize="{Binding Path=Text, RelativeSource={RelativeSource Self}, Converter={StaticResource ProgressiveLabelSizeConv}}"
                                                           Foreground="{StaticResource JackpotTextGradient}"
                                                           AutomationProperties.AutomationId="TextBlock40_1F333AB0" />
                                            </Grid>
                                            
                                            <!-- Use DataContext of ItemsControl instead of current GameInfo for SelectCommand -->
                                            <Button x:Name="GameIconButton"
                                                    Style="{StaticResource stlFlatButton}"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Top"
                                                    Tag="{Binding Name}"
                                                    Width="Auto"
                                                    Height="Auto"
                                                    Command="{Binding Path=DataContext.GameSelectCommand, RelativeSource={RelativeSource AncestorType=ItemsControl}}"
                                                    CommandParameter="{Binding}"
                                                    AutomationProperties.AutomationId="Button41_1F333AB0">
                                                <Button.IsEnabled>
                                                    <MultiBinding Converter="{StaticResource FiltersToBoolConv}">
                                                        <Binding Path="DataContext.DenomFilter"
                                                                 RelativeSource="{RelativeSource AncestorType=ItemsControl}" />
                                                        <Binding Path="FilteredDenomination" />
                                                        <Binding Path="Enabled" />
                                                        <Binding Path="DataContext.IsExtraLargeGameIconTabActive"
                                                                 RelativeSource="{RelativeSource AncestorType=ItemsControl}"
                                                                 Converter="{StaticResource InverseBoolConverter}" />
                                                    </MultiBinding>
                                                </Button.IsEnabled>
                                                <Grid VerticalAlignment="Top"
                                                      HorizontalAlignment="Center"
                                                      AutomationProperties.AutomationId="Grid42_1F333AB0">
                                                    <ContentControl x:Name="ButtonImage"
                                                                    HorizontalAlignment="Center"
                                                                    VerticalAlignment="Top"
                                                                    Margin="{Binding Path=DataContext.MarginInputs, RelativeSource={RelativeSource AncestorType=ItemsControl}, Converter={StaticResource MarginConverter}, ConverterParameter=GameIcon}"
                                                                    AutomationProperties.AutomationId="ContentControl43_1F333AB0">
                                                        <ContentControl.Style>
                                                            <Style TargetType="ContentControl">
                                                                <Style.Triggers>
                                                                    <DataTrigger
                                                                            Binding="{Binding ImageIsBink, UpdateSourceTrigger=PropertyChanged}"
                                                                                 Value="True"
                                                                                 AutomationProperties.AutomationId="DataTrigger44_1F333AB0">
                                                                        <Setter Property="Content">
                                                                            <Setter.Value>
                                                                                <bink:BinkViewerControl
                                                                                        Stretch="Uniform"
                                                                                                        Filename="{Binding ImagePath}"
                                                                                                        Width="Auto"
                                                                                                        Height="{Binding Path=DataContext.MarginInputs, RelativeSource={RelativeSource AncestorType=ItemsControl}, Converter={StaticResource GameIconImageHeightConv}}" />
                                                                            </Setter.Value>
                                                                        </Setter>
                                                                    </DataTrigger>
                                                                    <DataTrigger
                                                                            Binding="{Binding ImageIsBink, UpdateSourceTrigger=PropertyChanged}"
                                                                                 Value="False"
                                                                                 AutomationProperties.AutomationId="DataTrigger45_1F333AB0">
                                                                        <Setter Property="Content">
                                                                            <Setter.Value>
                                                                                <Image
                                                                                        RenderOptions.BitmapScalingMode="HighQuality"
                                                                                       Stretch="Uniform"
                                                                                       gif:ImageBehavior.AnimatedSource="{Binding ImagePath}"
                                                                                       Width="Auto"
                                                                                       Height="{Binding Path=DataContext.MarginInputs, RelativeSource={RelativeSource AncestorType=ItemsControl}, Converter={StaticResource GameIconImageHeightConv}}"
                                                                                       AutomationProperties.AutomationId="Image46_1F333AB0" />
                                                                            </Setter.Value>
                                                                        </Setter>
                                                                    </DataTrigger>
                                                                </Style.Triggers>
                                                            </Style>
                                                        </ContentControl.Style>
                                                    </ContentControl>
                                                    
                                                    <Image x:Name="SelectedImage"
                                                           Source="{DynamicResource GameSelectedOverlay}"
                                                           RenderOptions.BitmapScalingMode="HighQuality"
                                                           Margin="0"
                                                           Height="{Binding Path=DataContext.MarginInputs, RelativeSource={RelativeSource AncestorType=ItemsControl}, Converter={StaticResource GameIconImageHeightConv}}"
                                                           HorizontalAlignment="Center"
                                                           VerticalAlignment="Top"
                                                           AutomationProperties.AutomationId="Image47_1F333AB0">
                                                        <Image.Visibility>
                                                            <MultiBinding
                                                                    Converter="{StaticResource LogicalAndTrueVisibleFalseCollapsed}">
                                                                <Binding Path="IsSelected" />
                                                                <Binding
                                                                        Path="DataContext.IsExtraLargeGameIconTabActive"
                                                                         RelativeSource="{RelativeSource AncestorType=ItemsControl}"
                                                                         Converter="{StaticResource InverseBoolConverter}" />
