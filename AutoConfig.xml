	<Protocol>G2S</Protocol>
	
	<!-- Valid values: {true, false} -->
	<NoteAcceptorEnabled>true</NoteAcceptorEnabled>
	
	<!-- Valid values: {Fake, JCM USB GDS, MEI USB GDS} -->
	<NoteAcceptorMake>JCM</NoteAcceptorMake>
	
	<!-- Valid values: {GDS} -->
	<NoteAcceptorProtocol>GDS</NoteAcceptorProtocol>
	
	<!-- Valid values: {USB, COM1, COM2, ...} -->
	<NoteAcceptorPort>USB</NoteAcceptorPort>
	
	<!-- Valid values: {true, false} -->
	<PrinterEnabled>true</PrinterEnabled>
	
	<!-- Valid values: {Fake, FutureLogic Gen2 Universal USB GDS, Nanoptix NextGen USB GDS, JCM Gen5 USB GDS} -->
