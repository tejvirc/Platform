	<!-- Valid values: {GDS} -->
	<PrinterProtocol>GDS</PrinterProtocol>
	
	<!-- Valid values: {USB, COM1, COM2, ...} -->
	<PrinterPort>USB</PrinterPort>
	
	<!-- Valid values: {true, false} -->
	<IdReaderEnabled>true</IdReaderEnabled>
	
	<!-- Valid values: {Fake, UNIFORM USB GDS} -->
