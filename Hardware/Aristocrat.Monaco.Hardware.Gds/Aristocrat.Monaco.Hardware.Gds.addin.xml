<Addin id="GdsDevices" namespace="Client12Addins" version="1.0">

  <Runtime>
    <Import assembly="Aristocrat.Monaco.Hardware.Gds.dll" />
    <Import assembly="Aristocrat.Monaco.Hardware.Contracts.dll" />
  </Runtime>

  <Dependencies>
    <Addin id="Monaco.Hardware" version="1.0" />
  </Dependencies>

  <Extension path="/Hardware/IdReader/IdReaderImplementations">
    <DeviceImplementation ProtocolName="GDS" type="Aristocrat.Monaco.Hardware.Gds.CardReader.CardReaderGds" />
    <DeviceImplementation ProtocolName="Virtual" type="Aristocrat.Monaco.Hardware.Gds.CardReader.CardReaderGds"/>
    <DeviceImplementation ProtocolName="Virtual2" type="Aristocrat.Monaco.Hardware.Gds.CardReader.CardReaderGds"/>
  </Extension>
  <Extension path="/Hardware/Printer/PrinterImplementations">
    <DeviceImplementation ProtocolName="JCM TCL" type="Aristocrat.Monaco.Hardware.Gds.Printer.PrinterGds" />
  </Extension>

  <Extension path="/Hardware/Printer/PrinterImplementations">
    <DeviceImplementation ProtocolName="GDS" type="Aristocrat.Monaco.Hardware.Gds.Printer.PrinterGds" />
  </Extension>

  <Extension path="/Hardware/Printer/PrinterImplementations">
    <DeviceImplementation ProtocolName="Nanoptix TCL" type="Aristocrat.Monaco.Hardware.Gds.Printer.PrinterGds" />
  </Extension>

  <Extension path="/Hardware/Printer/PrinterImplementations">
    <DeviceImplementation ProtocolName="EpicTTL" type="Aristocrat.Monaco.Hardware.Gds.Printer.PrinterGds" />
  </Extension>

  <Extension path="/Hardware/ReelController/ReelControllerImplementations">
    <DeviceImplementation ProtocolName="Harkey" type="Aristocrat.Monaco.Hardware.Gds.Reel.ReelControllerGds" />
  </Extension>

</Addin>