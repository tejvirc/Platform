    loc:Localizer.For="Operator"
    mc:Ignorable="d"
    d:DataContext="{d:DesignInstance {x:Type viewModels:SystemResetPageViewModel}}"
    d:DesignWidth="800"
    d:DesignHeight="600"
    Background="Transparent"
    common:DialogParticipation.Register="{Binding}">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary
                    Source="/Aristocrat.Monaco.UI.Common;component/Skins/OperatorMenu.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid
        Name="MainGrid"
        AutomationProperties.AutomationId="Grid0_ED0F5ED8">
        <Grid.ColumnDefinitions>
            <ColumnDefinition
                Width="*"
                AutomationProperties.AutomationId="ColumnDefinition0_983AE9B" />
            <ColumnDefinition
                Width="*"
                AutomationProperties.AutomationId="ColumnDefinition1_983AE9B" />
        </Grid.ColumnDefinitions>
        <Grid
            Grid.Column="0"
            AutomationProperties.AutomationId="Grid2_983AE9B">
            <Grid.RowDefinitions>
                <RowDefinition
                    Height="Auto"
                    AutomationProperties.AutomationId="RowDefinition1_ED0F5ED8" />
                <RowDefinition
                    Height="*"
                    AutomationProperties.AutomationId="RowDefinition2_ED0F5ED8" />
                <RowDefinition
                    Height="Auto"
                    AutomationProperties.AutomationId="RowDefinition3_ED0F5ED8" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition
                    Width="Auto"
                    AutomationProperties.AutomationId="ColumnDefinition4_ED0F5ED8" />
                <ColumnDefinition
                    Width="Auto"
                    AutomationProperties.AutomationId="ColumnDefinition5_ED0F5ED8" />
            </Grid.ColumnDefinitions>
            <StackPanel
                Grid.Row="0"
                Grid.Column="0"
                Name="PartialResetText"
                HorizontalAlignment="Left"
                AutomationProperties.AutomationId="StackPanel6_ED0F5ED8">
                <Label
                    Content="{lex:Loc Key={x:Static loc:ResourceKeys.PartialClearHeadingText}}"
                    Height="Auto"
                    FontSize="{StaticResource Heading3FontSize}"
                    Margin="0,0,0,8"
                    AutomationProperties.AutomationId="Label7_ED0F5ED8" />
                <TextBlock
                    Text="{lex:Loc Key={x:Static loc:ResourceKeys.PartialClearSummaryText}}"
                    Height="Auto"
                    FontSize="{StaticResource ContentFontSize}"
                    Margin="5,0,0,32"
                    TextWrapping="Wrap"
                    AutomationProperties.AutomationId="TextBlock8_ED0F5ED8" />
                <Label
                    Content="{lex:Loc Key={x:Static loc:ResourceKeys.PartialClearDetailsText1}}"
                    Height="Auto"
                    FontSize="{StaticResource Heading4FontSize}"
                    Margin="0,0,0,8"
                    AutomationProperties.AutomationId="Label9_ED0F5ED8" />
                <TextBlock
                    Text="{lex:Loc Key={x:Static loc:ResourceKeys.PartialClearDetailsText2}}"
                    Margin="5,0,0,32"
                    FontSize="{StaticResource ContentFontSize}"
                    Height="Auto"
                    TextWrapping="Wrap"
                    AutomationProperties.AutomationId="TextBlock10_ED0F5ED8" />
                <Label
                    Content="{lex:Loc Key={x:Static loc:ResourceKeys.PartialClearDetailsText3}}"
                    Height="Auto"
                    FontSize="{StaticResource Heading4FontSize}"
                    Margin="0,0,0,8"
                    AutomationProperties.AutomationId="Label11_ED0F5ED8" />
                <TextBlock
                    Text="{lex:Loc Key={x:Static loc:ResourceKeys.PartialClearDetailsText4}}"
                    Margin="5,0,0,8"
                    FontSize="{StaticResource ContentFontSize}"
                    Height="Auto"
                    TextWrapping="Wrap"
                    AutomationProperties.AutomationId="TextBlock12_ED0F5ED8" />
            </StackPanel>
            <Button
                Grid.Row="1"
                Grid.Column="0"
                Name="PartialResetButton"
                Content="{lex:Loc Key={x:Static loc:ResourceKeys.PartialClearButtonContent}}"
                Command="{Binding PartialResetButtonClickCommand}"
                IsEnabled="{Binding InputEnabled}"
                Margin="5,40,0,0"
                Width="180"
                VerticalAlignment="Top"
                HorizontalAlignment="Left"
                AutomationProperties.AutomationId="Button13_ED0F5ED8" />
        </Grid>
        <Grid
            Grid.Column="1"
            AutomationProperties.AutomationId="Grid3_983AE9B">
            <Grid.RowDefinitions>
                <RowDefinition
                    Height="Auto"
                    AutomationProperties.AutomationId="RowDefinition4_983AE9B" />
                <RowDefinition
                    Height="*"
                    AutomationProperties.AutomationId="RowDefinition5_983AE9B" />
                <RowDefinition
                    Height="Auto"
                    AutomationProperties.AutomationId="RowDefinition6_983AE9B" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition
                    Width="Auto"
                    AutomationProperties.AutomationId="ColumnDefinition7_983AE9B" />
                <ColumnDefinition
                    Width="Auto"
                    AutomationProperties.AutomationId="ColumnDefinition8_983AE9B" />
            </Grid.ColumnDefinitions>
            <StackPanel
                Grid.Row="0"
                Grid.Column="0"
                Name="FullResetText"
                HorizontalAlignment="Left"
                AutomationProperties.AutomationId="StackPanel9_983AE9B">
                <Label
                    Content="{lex:Loc Key={x:Static loc:ResourceKeys.FullClearHeadingText}}"
                    Height="Auto"
                    FontSize="{StaticResource Heading3FontSize}"
                    Margin="0,0,0,8"
                    AutomationProperties.AutomationId="Label10_983AE9B" />
                <TextBlock
                    Text="{lex:Loc Key={x:Static loc:ResourceKeys.FullClearSummaryText}}"
                    Height="Auto"
                    FontSize="{StaticResource ContentFontSize}"
                    Margin="5,0,0,32"
                    TextWrapping="Wrap"
                    AutomationProperties.AutomationId="TextBlock11_983AE9B" />
                <Label
                    Content="{lex:Loc Key={x:Static loc:ResourceKeys.FullClearDetailsText1}}"
                    Height="Auto"
                    FontSize="{StaticResource Heading4FontSize}"
                    Margin="0,0,0,8"
                    AutomationProperties.AutomationId="Label12_983AE9B" />
                <TextBlock
                    Text="{lex:Loc Key={x:Static loc:ResourceKeys.FullClearDetailsText2}}"
                    Margin="5,0,0,32"
                    FontSize="{StaticResource ContentFontSize}"
                    Height="Auto"
                    TextWrapping="Wrap"
                    AutomationProperties.AutomationId="TextBlock13_983AE9B" />
                <Label
                    Content="{lex:Loc Key={x:Static loc:ResourceKeys.FullClearDetailsText3}}"
                    Height="Auto"
                    FontSize="{StaticResource Heading4FontSize}"
                    Margin="0,0,0,8"
                    AutomationProperties.AutomationId="Label14_983AE9B" />
                <TextBlock
                    Text="{lex:Loc Key={x:Static loc:ResourceKeys.FullClearDetailsText4}}"
                    Margin="5,0,0,8"
                    FontSize="{StaticResource ContentFontSize}"
                    Height="Auto"
                    TextWrapping="Wrap"
                    AutomationProperties.AutomationId="TextBlock15_983AE9B" />
            </StackPanel>
            <Button
                Grid.Row="1"
                Grid.Column="0"
                Name="FullResetButton"
                Content="{lex:Loc Key={x:Static loc:ResourceKeys.FullClearButtonContent}}"
                Command="{Binding FullResetButtonClickCommand}"
                IsEnabled="{Binding InputEnabled}"
                Margin="5,40,0,0"
                Width="180"
                VerticalAlignment="Top"
                HorizontalAlignment="Left"
                AutomationProperties.AutomationId="Button16_983AE9B" />
        </Grid>
    </Grid>
</operatorMenu:OperatorMenuPage>