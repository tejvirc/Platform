                    <Image x:Key="StripImage"
                           Source="../Resources/Operator_Menu_Background_Strip.png"
                           AutomationProperties.AutomationId="Image25_F088786C">
                        <Image.OpacityMask>
                            <LinearGradientBrush StartPoint=".5,0"
                                                 EndPoint=".5,1"
                                                 AutomationProperties.AutomationId="LinearGradientBrush26_F088786C">
                                <GradientStop Offset="0.0"
                                              Color="#001C3053"
                                              AutomationProperties.AutomationId="GradientStop27_F088786C" />
                                <GradientStop Offset="1"
                                              Color="#FF1C3053"
                                              AutomationProperties.AutomationId="GradientStop28_F088786C" />
                            </LinearGradientBrush>
                        </Image.OpacityMask>
                    </Image>
                    <VisualBrush x:Key="StripBrush"
                                 Visual="{StaticResource StripImage}"
                                 AutomationProperties.AutomationId="VisualBrush29_F088786C" />
                </Grid.Resources>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" AutomationProperties.AutomationId="RowDefinition30_F088786C" />
                    <RowDefinition Height="5" AutomationProperties.AutomationId="RowDefinition31_F088786C" />
                    <RowDefinition Height="*" AutomationProperties.AutomationId="RowDefinition32_F088786C" />
                    <RowDefinition Height="Auto" AutomationProperties.AutomationId="RowDefinition33_F088786C" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" AutomationProperties.AutomationId="ColumnDefinition34_F088786C" />
                    <ColumnDefinition Width="*" AutomationProperties.AutomationId="ColumnDefinition35_F088786C" />
                </Grid.ColumnDefinitions>
                <Border Grid.Row="0"
                        Grid.Column="0"
                        Background="{DynamicResource OperatorMenuMedium}"
                        BorderBrush="{DynamicResource OperatorMenuDark}"
                        Height="100"
                        AutomationProperties.AutomationId="Border36_F088786C">
                    <Image Source="../Resources/Aristocrat_Logo_White_Large.png"
                           Stretch="Uniform"
                           VerticalAlignment="Center"
                           Margin="12"
                           AutomationProperties.AutomationId="Image37_F088786C" />
                </Border>
                <Border Grid.Row="1"
                        Grid.Column="0"
                        Background="{DynamicResource OperatorMenuDropShadowBrush}"
                        AutomationProperties.AutomationId="Border38_F088786C" />
                <Grid Grid.Row="2"
                      Grid.Column="0"
                      AutomationProperties.AutomationId="Grid39_F088786C">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*" AutomationProperties.AutomationId="RowDefinition40_F088786C" />
                        <RowDefinition Height="Auto" AutomationProperties.AutomationId="RowDefinition41_F088786C" />
                        <RowDefinition Height="Auto" AutomationProperties.AutomationId="RowDefinition42_F088786C" />
                        <RowDefinition Height="Auto" AutomationProperties.AutomationId="RowDefinition43_F088786C" />
                        <RowDefinition Height="Auto" AutomationProperties.AutomationId="RowDefinition44_F088786C" />
                    </Grid.RowDefinitions>
                    <common:TouchListBox Name="Views"
