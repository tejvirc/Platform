                            <ColumnDefinition Width="Auto" AutomationProperties.AutomationId="ColumnDefinition77_F088786C" />
                            <ColumnDefinition Width="*" AutomationProperties.AutomationId="ColumnDefinition78_F088786C" />
                            <ColumnDefinition Width="Auto" AutomationProperties.AutomationId="ColumnDefinition79_F088786C" />
                            <ColumnDefinition Width="55" AutomationProperties.AutomationId="ColumnDefinition80_F088786C" />
                        </Grid.ColumnDefinitions>
                        <Image Source="../Resources/Audit.png"
