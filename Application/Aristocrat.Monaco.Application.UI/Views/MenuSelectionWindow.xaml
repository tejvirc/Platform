                    <EventTrigger RoutedEvent="Loaded" AutomationProperties.AutomationId="EventTrigger3_F088786C">
                        <EventTrigger.Actions>
                            <BeginStoryboard AutomationProperties.AutomationId="BeginStoryboard4_F088786C">
                                <Storyboard AutomationProperties.AutomationId="Storyboard5_F088786C">
                                    <DoubleAnimation Storyboard.TargetProperty="(TextBlock.Opacity)"
                                                     From="1"
                                                     To="0"
                                                     Duration="00:00:02"
                                                     AutoReverse="True"
                                                     RepeatBehavior="Forever"
                                                     AutomationProperties.AutomationId="DoubleAnimation6_F088786C"/>
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger.Actions>
                    </EventTrigger>
                </Style.Triggers>
            </Style>
            <DataTemplate x:Key="SinglePageTemplate" DataType="{x:Type ContentControl}">
                <GroupBox Header="{Binding DataContext.SelectedItem.PageName, ElementName=OperatorMenuWindow}"
                          Margin="20"
                          loc:Localizer.LocalizationAware="True"
                          AutomationProperties.AutomationId="GroupBox7_F088786C">
                    <Grid AutomationProperties.AutomationId="Grid8_F088786C">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" AutomationProperties.AutomationId="RowDefinition9_F088786C" />
                            <RowDefinition Height="Auto" AutomationProperties.AutomationId="RowDefinition10_F088786C" />
                        </Grid.RowDefinitions>
                        <ContentControl Grid.Row="0"
                                        Content="{Binding DataContext.SelectedItem.Page, ElementName=OperatorMenuWindow}"
                                        AutomationProperties.AutomationId="ContentControl11_F088786C" />
                        <TextBlock Grid.Row="1"
                                   HorizontalAlignment="Right"
                                   VerticalAlignment="Bottom"
                                   TextWrapping="Wrap"
                                   Style="{StaticResource MonacoWarningTextBlock}"
                                   Text="{Binding DataContext.WarningMessageText, ElementName=OperatorMenuWindow, UpdateSourceTrigger=PropertyChanged}"
                                   AutomationProperties.AutomationId="TextBlock12_F088786C" />
                    </Grid>
                </GroupBox>
            </DataTemplate>
            <GridLength x:Key="GoldenRatioGridLength">1.618*</GridLength>
        </ResourceDictionary>
    </common:BaseWindow.Resources>
    <i:Interaction.Behaviors>
        <behaviors:TouchWindowBehavior />
        <behaviors:HighDpiWindowBehavior />
    </i:Interaction.Behaviors>
    <Grid AutomationProperties.AutomationId="Grid13_F088786C">
        <Grid.RowDefinitions>
            <RowDefinition Height="0" AutomationProperties.AutomationId="RowDefinition14_F088786C" />
            <RowDefinition Height="1*" AutomationProperties.AutomationId="RowDefinition15_F088786C" />
        </Grid.RowDefinitions>
        <i:Interaction.Triggers>
            <events:RoutedEventTrigger RoutedEvent="{x:Static FrameworkElement.SizeChangedEvent}" Target="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType=Window}}">
                <common:AdaptToOrientationGridAction FirstRowHeightWhenPortrait="{StaticResource GoldenRatioGridLength}" />
            </events:RoutedEventTrigger>
            <events:RoutedEventTrigger RoutedEvent="{x:Static FrameworkElement.LoadedEvent}">
                <common:AdaptToOrientationGridAction FirstRowHeightWhenPortrait="{StaticResource GoldenRatioGridLength}" />
            </events:RoutedEventTrigger>
        </i:Interaction.Triggers>
        <Viewbox Grid.Row="0"
                 AutomationProperties.AutomationId="Viewbox16_F088786C">
            <Viewbox.Style>
                <Style TargetType="{x:Type Viewbox}">
                    <Style.Triggers>
                        <DataTrigger
                            AutomationProperties.AutomationId="DataTrigger17_F088786C"
                            Binding="{Binding ShouldDisplaySafetyMessage}"
                            Value="False">
                            <Setter Property="HorizontalAlignment" Value="Center" />
                            <Setter Property="VerticalAlignment" Value="Center" />
                            <Setter Property="Stretch" Value="UniformToFill" />
                        </DataTrigger>
                        <DataTrigger
                            AutomationProperties.AutomationId="DataTrigger18_F088786C"
                            Binding="{Binding ShouldDisplaySafetyMessage}"
                            Value="True">
                            <Setter Property="HorizontalAlignment" Value="Left" />
                            <Setter Property="VerticalAlignment" Value="Top" />
                            <Setter Property="Stretch" Value="UniformToFill" />
                        </DataTrigger>
                    </Style.Triggers>
                </Style>
            </Viewbox.Style>
            <Image AutomationProperties.AutomationId="Image19_F088786C">
                <Image.Style>
                    <Style TargetType="{x:Type Image}">
                        <Style.Triggers>
                            <DataTrigger
                                AutomationProperties.AutomationId="DataTrigger20_F088786C"
                                Binding="{Binding ShouldDisplaySafetyMessage}"
                                Value="False">
                                <Setter Property="Source" Value="../Resources/AristocratLogoPortrait.png" />
                            </DataTrigger>
                            <DataTrigger
                                AutomationProperties.AutomationId="DataTrigger21_F088786C"
                                Binding="{Binding ShouldDisplaySafetyMessage}"
                                Value="True">
                                <Setter Property="Source" Value="{DynamicResource SafetyImage}" />
                            </DataTrigger>
                        </Style.Triggers>
                    </Style>
                </Image.Style>
            </Image>
        </Viewbox>
