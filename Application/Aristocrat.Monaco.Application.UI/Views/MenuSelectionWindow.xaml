                                    Height="45"
                                    HorizontalAlignment="Center"
                                    IsEnabled="{Binding CancelButtonEnabled}"
                                    Command="{Binding PrintButtonCommand}"
                                    Style="{StaticResource MonacoButton1}"
                                    Visibility="{Binding ShowCancelPrintButton, Converter={StaticResource TrueVisibleFalseCollapsed}}"
                                    AutomationProperties.AutomationId="Button70_F088786C">
                                <Grid AutomationProperties.AutomationId="Grid71_F088786C">
                                    <iconPacks:PackIconModern Kind="Cancel"
                                                              Width="25"
                                                              Height="25" />
                                </Grid>
                            </Button>
                        </Grid>
                    </Grid>
                    <Border Grid.Row="3"
