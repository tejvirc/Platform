                                                     AutomationProperties.AutomationId="SolidColorBrush47_F088786C" />
                                    <SolidColorBrush x:Key="{x:Static SystemColors.ControlBrushKey}"
                                                     Color="{StaticResource HighlightColor}"
                                                     AutomationProperties.AutomationId="SolidColorBrush48_F088786C" />
                                </Style.Resources>
                                <Setter Property="Padding" Value="0" />
                                <Setter Property="Background" Value="Transparent" />
                                <Setter Property="FocusVisualStyle" Value="{x:Null}" />
                                <Setter Property="IsEnabled" Value="False" />
                                <Setter Property="Template">
                                    <Setter.Value>
                                        <ControlTemplate TargetType="{x:Type ListBoxItem}">
                                            <Border x:Name="Bd"
                                                    BorderBrush="{TemplateBinding BorderBrush}"
                                                    BorderThickness="{TemplateBinding BorderThickness}"
                                                    Background="{TemplateBinding Background}"
                                                    Padding="{TemplateBinding Padding}"
                                                    SnapsToDevicePixels="True"
                                                    AutomationProperties.AutomationId="Border49_F088786C">
                                                <ContentPresenter ContentTemplate="{TemplateBinding ContentTemplate}"
                                                                  Content="{TemplateBinding Content}"
                                                                  ContentStringFormat="{TemplateBinding ContentStringFormat}"
                                                                  HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                                  SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                                                  VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                                                  AutomationProperties.AutomationId="ContentPresenter50_F088786C" />
                                            </Border>
                                            <ControlTemplate.Triggers>
                                                <DataTrigger Binding="{Binding IsEnabled}"
                                                             Value="True"
                                                             AutomationProperties.AutomationId="DataTrigger51_F088786C">
                                                    <DataTrigger.Setters>
                                                        <Setter Property="IsEnabled" Value="True" />
                                                    </DataTrigger.Setters>
                                                </DataTrigger>
                                                <MultiTrigger AutomationProperties.AutomationId="MultiTrigger52_F088786C">
                                                    <MultiTrigger.Conditions>
                                                        <Condition Property="IsSelected" Value="False" />
                                                    </MultiTrigger.Conditions>
                                                    <Setter Property="Background"
                                                            TargetName="Bd"
                                                            Value="{x:Null}" />
                                                    <Setter Property="BorderBrush"
                                                            TargetName="Bd"
                                                            Value="{x:Null}" />
                                                    <Setter Property="Foreground" Value="{DynamicResource OperatorMenuLabelText}" />
                                                </MultiTrigger>
                                                <MultiTrigger AutomationProperties.AutomationId="MultiTrigger53_F088786C">
                                                    <MultiTrigger.Conditions>
                                                        <Condition Property="IsSelected" Value="True" />
                                                    </MultiTrigger.Conditions>
                                                    <Setter Property="Background"
                                                            TargetName="Bd"
                                                            Value="{DynamicResource MenuHighlightBrush}" />
                                                    <Setter Property="BorderBrush"
                                                            TargetName="Bd"
                                                            Value="{x:Null}" />
                                                    <Setter Property="Foreground" Value="{DynamicResource OperatorMenuDark}" />
                                                </MultiTrigger>
                                                <MultiTrigger AutomationProperties.AutomationId="MultiTrigger54_F088786C">
                                                    <MultiTrigger.Conditions>
                                                        <Condition Property="IsEnabled" Value="False" />
                                                    </MultiTrigger.Conditions>
                                                    <Setter Property="Background"
                                                            TargetName="Bd"
                                                            Value="{x:Null}" />
                                                    <Setter Property="BorderBrush"
                                                            TargetName="Bd"
                                                            Value="{x:Null}" />
                                                    <Setter Property="Foreground" Value="{DynamicResource OperatorMenuDisabledText}" />
                                                </MultiTrigger>
                                            </ControlTemplate.Triggers>
                                        </ControlTemplate>
                                    </Setter.Value>
                                </Setter>
                            </Style>
                        </common:TouchListBox.ItemContainerStyle>
                    </common:TouchListBox>
                    <Button Name="ToggleLanguageButton"
                            Margin="0 20 0 0"
                            TabIndex="100"
                            Grid.Row="1"
                            Width="196"
                            Height="45"
                            Command="{Binding LanguageChangedCommand}"
                            Content="{Binding ToggleLanguageButtonText}"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            Visibility="{Binding ShowToggleLanguageButton, Converter={StaticResource TrueVisibleFalseCollapsed}}"
                            AutomationProperties.AutomationId="Button55_F088786C"/>
                    <Grid Grid.Row="2"
                          Margin="0 20 0 0"
                          AutomationProperties.AutomationId="Grid56_F088786C">
                        <Grid Visibility="{Binding ShowExitButton, Converter={StaticResource TrueVisibleFalseCollapsed}}" AutomationProperties.AutomationId="Grid57_F088786C">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" AutomationProperties.AutomationId="ColumnDefinition58_F088786C" />
                                <ColumnDefinition Width="*" AutomationProperties.AutomationId="ColumnDefinition59_F088786C" />
                            </Grid.ColumnDefinitions>
                            <Button Name="ExitButton"
                                    TabIndex="100"
                                    Grid.Column="0"
                                    Width="65"
                                    Height="45"
                                    Style="{StaticResource MonacoButton1}"
                                    Command="{Binding ExitButtonCommand}"
                                    Margin="30 0 0 0"
                                    HorizontalAlignment="Center"
                                    AutomationProperties.AutomationId="Button60_F088786C">
                                <Grid AutomationProperties.AutomationId="Grid61_F088786C">
                                    <iconPacks:PackIconMaterial Kind="ExitToApp"
                                                                Flip="Horizontal"
                                                                Width="25"
                                                                Height="25" />
                                </Grid>
                            </Button>
                            <Button IsEnabled="{Binding PrintButtonEnabled}"
                                    Grid.Column="1"
                                    Width="65"
                                    Height="45"
                                    HorizontalAlignment="Center"
                                    Margin="0 0 30 0"
                                    Command="{Binding PrintButtonCommand}"
                                    Style="{StaticResource MonacoButton1}"
                                    Visibility="{Binding ShowCancelPrintButton, Converter={StaticResource TrueCollapsedFalseVisible}}"
                                    AutomationProperties.AutomationId="Button62_F088786C">
                                <Grid AutomationProperties.AutomationId="Grid63_F088786C">
                                    <iconPacks:PackIconFontAwesome Kind="Print"
                                                                   Width="25"
                                                                   Height="25" />
                                </Grid>
                            </Button>
                            <Button Grid.Column="1"
                                    Width="65"
                                    Height="45"
                                    HorizontalAlignment="Center"
                                    Margin="0 0 30 0"
