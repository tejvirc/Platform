                    </Border>
                </Grid>
                <Border Background="{DynamicResource OperatorMenuDark}"
                        Grid.Row="0"
                        Grid.Column="1"
                        AutomationProperties.AutomationId="Border74_F088786C">
                    <Grid AutomationProperties.AutomationId="Grid75_F088786C">
