                       Margin="{StaticResource LabelMargin}"
                       Content="{lex:Loc Key={x:Static loc:ResourceKeys.PositionText}}"
                       Visibility="{Binding Position.IsVisible, Converter={StaticResource TrueVisibleFalseCollapsed}}"
                       AutomationProperties.AutomationId="Label38_D3C5ECAC" />
                <ContentControl Grid.Row="5"
                                Grid.Column="1"
                                IsTabStop="False"
                                ContentTemplate="{StaticResource LiveStringSettingTextBoxEditor}"
                                Content="{Binding Position}"
                                AutomationProperties.AutomationId="ContentControl39_D3C5ECAC" />
                <!-- Location Section -->
                <Label Grid.Row="6"
                       Grid.Column="0"
                       Margin="{StaticResource LabelMargin}"
                       Content="{lex:Loc Key={x:Static loc:ResourceKeys.Location}}"
                       Visibility="{Binding Location.IsVisible, Converter={StaticResource TrueVisibleFalseCollapsed}}"
                       AutomationProperties.AutomationId="Label40_D3C5ECAC" />
                <ContentControl Grid.Row="6"
                                Grid.Column="1"
                                IsTabStop="False"
                                ContentTemplate="{StaticResource LiveStringSettingTextBoxEditor}"
                                Content="{Binding Location}"
                                AutomationProperties.AutomationId="ContentControl41_D3C5ECAC" />
                <!-- DeviceName Section -->
                <Label Grid.Row="7"
                       Grid.Column="0"
                       Margin="{StaticResource LabelMargin}"
                       Content="{lex:Loc Key={x:Static loc:ResourceKeys.DeviceName}}"
                       Visibility="{Binding DeviceName.IsVisible, Converter={StaticResource TrueVisibleFalseCollapsed}}"
                       AutomationProperties.AutomationId="Label42_D3C5ECAC" />
                <ContentControl Grid.Row="7"
                                Grid.Column="1"
                                IsTabStop="False"
                                ContentTemplate="{StaticResource LiveStringSettingTextBoxEditor}"
                                Content="{Binding DeviceName}"
                                AutomationProperties.AutomationId="ContentControl43_D3C5ECAC" />
            </Grid>
            <Grid Grid.Row="1"
                  Margin="0,20,0,0"
                  AutomationProperties.AutomationId="Grid44_D3C5ECAC">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"
                                   AutomationProperties.AutomationId="RowDefinition45_D3C5ECAC" />
                    <RowDefinition Height="Auto"
                                   AutomationProperties.AutomationId="RowDefinition46_D3C5ECAC" />
                    <RowDefinition Height="Auto"
                                   AutomationProperties.AutomationId="RowDefinition47_D3C5ECAC" />
                    <RowDefinition Height="Auto"
                                   AutomationProperties.AutomationId="RowDefinition48_D3C5ECAC" />
                    <RowDefinition Height="Auto"
                                   AutomationProperties.AutomationId="RowDefinition49_D3C5ECAC" />
                    <RowDefinition Height="Auto"
                                   AutomationProperties.AutomationId="RowDefinition50_D3C5ECAC" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"
                                      AutomationProperties.AutomationId="ColumnDefinition51_D3C5ECAC" />
                    <ColumnDefinition Width="*"
                                      MinWidth="300"
                                      AutomationProperties.AutomationId="ColumnDefinition52_D3C5ECAC" />
                </Grid.ColumnDefinitions>
                <!-- Hard Boot Time Section -->
                <Label x:Name="HardBootLabel"
                       Grid.Row="0"
                       Grid.Column="0"
                       Content="{lex:Loc Key={x:Static loc:ResourceKeys.HardBootTimeLabel}}"
                       AutomationProperties.AutomationId="Label53_D3C5ECAC" />
                <TextBlock x:Name="HardBootTextBlock"
                           Grid.Row="0"
                           Grid.Column="1"
                           Text="{Binding HardBootTime,                              Mode=OneWay,                              UpdateSourceTrigger=PropertyChanged,                             Converter={StaticResource DateTimeFormatConverter}}"
                           AutomationProperties.AutomationId="TextBlock54_D3C5ECAC" />
                <!-- Soft Boot Time Section -->
                <Label x:Name="SoftBootLabel"
                       Grid.Row="1"
                       Grid.Column="0"
                       Content="{lex:Loc Key={x:Static loc:ResourceKeys.SoftBootTimeLabel}}"
                       AutomationProperties.AutomationId="Label55_D3C5ECAC" />
                <TextBlock x:Name="SoftBootTextBlock"
