                       Grid.Column="1"
                       Text="{Binding ButtonDeck, Mode=OneWay}"
                       AutomationProperties.AutomationId="TextBlock90_D3C5ECAC" />
            <!-- Displays Section -->
            <Label x:Name="DisplaysLabel"
