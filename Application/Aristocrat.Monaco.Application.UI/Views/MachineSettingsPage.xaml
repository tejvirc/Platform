                           Grid.Column="1"
                           Text="{Binding SoftBootTime,                              Mode=OneWay,                              UpdateSourceTrigger=PropertyChanged,                             Converter={StaticResource DateTimeFormatConverter}}"
                           AutomationProperties.AutomationId="TextBlock56_D3C5ECAC" />
