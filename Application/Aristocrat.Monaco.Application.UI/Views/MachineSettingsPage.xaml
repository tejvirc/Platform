                   Grid.Column="0"
                   loc:Localizer.For="Operator"
                   Content="{lex:Loc Key={x:Static loc:ResourceKeys.Displays}}"
                   AutomationProperties.AutomationId="Label91_D3C5ECAC" />
            <TextBlock x:Name="DisplaysTextBlock"
                       Grid.Row="4"
                       Grid.Column="1"
                       Text="{Binding Displays, Mode=OneWay}"
                       AutomationProperties.AutomationId="TextBlock92_D3C5ECAC" />
            <!-- Touch Screen Section -->
            <Label x:Name="TouchScreenLabel"
                   Grid.Row="5"
