                    <Style TargetType="TextBlock"
                           BasedOn="{StaticResource {x:Type TextBlock}}">
                        <Style.Triggers>
                            <DataTrigger Binding="{Binding NoteAcceptorModel, Mode=OneWay, TargetNullValue=''}"
                                         Value=""
                                         AutomationProperties.AutomationId="DataTrigger99_D3C5ECAC">
                                <Setter Property="Text"
                                        Value="{lex:BLoc Key={x:Static loc:ResourceKeys.NotAvailable}}" />
                            </DataTrigger>
                        </Style.Triggers>
                        <Setter Property="Text"
                                Value="{Binding NoteAcceptorModel, Mode=OneWay}" />
