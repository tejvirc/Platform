                <Label x:Name="IpAddressLabel"
                       Grid.Row="2"
                       Grid.Column="0"
                       Content="{lex:Loc Key={x:Static loc:ResourceKeys.IPAddressesLabel}}"
                       AutomationProperties.AutomationId="Label57_D3C5ECAC" />
                <TextBlock x:Name="IpAddressTextBlock"
                           Grid.Row="2"
                           Grid.Column="1"
                           Text="{Binding IpAddress, Mode=OneWay}"
                           AutomationProperties.AutomationId="TextBlock58_D3C5ECAC" />
                <!-- Physical Address Section -->
                <Label x:Name="PhysicalAddressLabel"
                       Grid.Row="3"
                       Grid.Column="0"
                       Content="{lex:Loc Key={x:Static loc:ResourceKeys.MacAddressLabel}}"
                       AutomationProperties.AutomationId="Label59_D3C5ECAC" />
                <TextBlock x:Name="PhysicalAddressTextBlock"
                           Grid.Row="3"
                           Grid.Column="1"
                           Text="{Binding PhysicalAddress, Mode=OneWay}"
                           AutomationProperties.AutomationId="TextBlock60_D3C5ECAC" />
                <!-- Jurisdiction Section -->
                <Label x:Name="JurisdictionLabel"
                       Grid.Row="4"
                       Grid.Column="0"
                       Visibility="{Binding IsVisibleForInspection, Converter={StaticResource TrueVisibleFalseCollapsed}}"
                       Content="{lex:Loc Key={x:Static loc:ResourceKeys.JurisdictionLabel}}"
                       Margin="0,2.222,9.556,13.555"
                       AutomationProperties.AutomationId="Label61_D3C5ECAC" />
                <TextBlock x:Name="JurisdictionTextBlock"
                           Grid.Row="4"
                           Grid.Column="1"
                           Visibility="{Binding IsVisibleForInspection, Converter={StaticResource TrueVisibleFalseCollapsed}}"
                           Text="{Binding Jurisdiction, Mode=OneWay}"
                           Margin="0.444,0.222,-0.444,9.555"
                           AutomationProperties.AutomationId="TextBlock62_D3C5ECAC" />
                <!-- Currency Section -->
                <Label x:Name="CurrencyLabel"
                       Grid.Row="5"
                       Grid.Column="0"
                       Visibility="{Binding IsVisibleForInspection, Converter={StaticResource TrueVisibleFalseCollapsed}}"
                       Content="{lex:Loc Key={x:Static loc:ResourceKeys.Currency}}"
                       Margin="0,2.222,9.556,13.555"
                       AutomationProperties.AutomationId="Label63_D3C5ECAC" />
                <TextBlock x:Name="CurrencyTextBlock"
                           Grid.Row="5"
                           Grid.Column="1"
                           Visibility="{Binding IsVisibleForInspection, Converter={StaticResource TrueVisibleFalseCollapsed}}"
                           Text="{Binding CurrencySample, Mode=OneWay}"
                           Margin="0.444,0.222,-0.444,9.555"
                           AutomationProperties.AutomationId="TextBlock64_D3C5ECAC" />
            </Grid>
        </Grid>
        <Grid x:Name="RightSideGrid"
              Grid.Row="0"
              Grid.Column="1"
              HorizontalAlignment="Left"
              AutomationProperties.AutomationId="Grid65_D3C5ECAC">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"
                               AutomationProperties.AutomationId="RowDefinition66_D3C5ECAC" />
                <RowDefinition Height="Auto"
                               AutomationProperties.AutomationId="RowDefinition67_D3C5ECAC" />
                <RowDefinition Height="Auto"
