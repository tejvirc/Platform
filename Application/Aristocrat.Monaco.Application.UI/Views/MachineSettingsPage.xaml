                <RowDefinition Height="Auto"
                               AutomationProperties.AutomationId="RowDefinition72_D3C5ECAC" />
                <RowDefinition Height="Auto"
                               AutomationProperties.AutomationId="RowDefinition73_D3C5ECAC" />
                <RowDefinition Height="Auto"
                               AutomationProperties.AutomationId="RowDefinition74_D3C5ECAC" />
                <RowDefinition Height="Auto"
                               AutomationProperties.AutomationId="RowDefinition75_D3C5ECAC" />
                <RowDefinition Height="Auto"
                               AutomationProperties.AutomationId="RowDefinition76_D3C5ECAC" />
                <RowDefinition Height="Auto"
                               AutomationProperties.AutomationId="RowDefinition77_D3C5ECAC" />
                <RowDefinition Height="Auto"
                               AutomationProperties.AutomationId="RowDefinition78_D3C5ECAC" />
                <RowDefinition Height="Auto"
                               AutomationProperties.AutomationId="RowDefinition79_D3C5ECAC" />
                <RowDefinition Height="Auto"
                               AutomationProperties.AutomationId="RowDefinition80_D3C5ECAC" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"
                                  AutomationProperties.AutomationId="ColumnDefinition81_D3C5ECAC" />
                <ColumnDefinition Width="*"
                                  AutomationProperties.AutomationId="ColumnDefinition82_D3C5ECAC" />
            </Grid.ColumnDefinitions>
            <!-- Model Section -->
            <Label x:Name="ModelLabel"
                   Grid.Row="0"
                   Grid.Column="0"
                   Content="{lex:Loc Key={x:Static loc:ResourceKeys.ModelLabel}}"
                   AutomationProperties.AutomationId="Label83_D3C5ECAC" />
            <TextBlock x:Name="ModelTextBlock"
                       Grid.Row="0"
                       Grid.Column="1"
                       Text="{Binding ModelText, Mode=OneWay}"
                       AutomationProperties.AutomationId="TextBlock84_D3C5ECAC" />
            <!-- Electronics Section -->
            <Label x:Name="Electronics"
                   Grid.Row="1"
                   Grid.Column="0"
                   loc:Localizer.For="Operator"
                   Content="{lex:Loc Key={x:Static loc:ResourceKeys.Electronics}}"
                   AutomationProperties.AutomationId="Label85_D3C5ECAC" />
            <TextBlock x:Name="ElectronicsTextBlock"
                       Grid.Row="1"
                       Grid.Column="1"
                       Text="{Binding Electronics, Mode=OneWay}"
                       AutomationProperties.AutomationId="TextBlock86_D3C5ECAC" />
            <!-- Graphics Card Section -->
            <Label x:Name="GraphicsCardLabel"
                   Grid.Row="2"
                   Grid.Column="0"
                   loc:Localizer.For="Operator"
                   Content="{lex:Loc Key={x:Static loc:ResourceKeys.GraphicsCard}}"
                   AutomationProperties.AutomationId="Label87_D3C5ECAC" />
            <TextBlock x:Name="GraphicsCardTextBlock"
                       Grid.Row="2"
                       Grid.Column="1"
                       Text="{Binding GraphicsCard, Mode=OneWay}"
                       AutomationProperties.AutomationId="TextBlock88_D3C5ECAC" />
            <!-- Button Deck Section -->
            <Label x:Name="ButtonDeckLabel"
                   Grid.Row="3"
                   Grid.Column="0"
                   loc:Localizer.For="Operator"
                   Content="{lex:Loc Key={x:Static loc:ResourceKeys.ButtonDeck}}"
                   AutomationProperties.AutomationId="Label89_D3C5ECAC" />
            <TextBlock x:Name="ButtonDeckTextBlock"
