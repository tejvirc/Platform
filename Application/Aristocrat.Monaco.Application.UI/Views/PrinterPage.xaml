                        Command="{Binding FormFeedButtonCommand}"
                        IsEnabled="{Binding PrinterButtonsEnabled}"
                        Width="Auto"
                        AutomationProperties.AutomationId="Button81_5E78AF2B" />
                <Button Content="{lex:Loc PrintDiagnosticButton}"
