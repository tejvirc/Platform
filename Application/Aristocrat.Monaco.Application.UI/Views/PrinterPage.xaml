                                      IsEnabled="{Binding PrintLanguageOverrideIsEnabled}"
                                      Visibility="{Binding ShowOperatorOverrideCheckBox, Converter={StaticResource TrueVisibleFalseCollapsed}}"
                                      AutomationProperties.AutomationId="CheckBox77_5E78AF2B" />
