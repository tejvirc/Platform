        <TextBlock Grid.Row="5"
                   Grid.Column="1"
                   Text="{Binding WorkingSet64}"
                   AutomationProperties.AutomationId="TextBlock25_9F96CDD5" />
        <Label Grid.Row="6"
               Grid.Column="0"
               Content="{lex:Loc Key={x:Static loc:ResourceKeys.HeaderPeakWorkingSet}}"
               AutomationProperties.AutomationId="Label26_9F96CDD5" />
        <TextBlock Grid.Row="6"
                   Grid.Column="1"
                   Text="{Binding PeakWorkingSet64}"
                   AutomationProperties.AutomationId="TextBlock27_9F96CDD5" />
        <Label Grid.Row="7"
               Grid.Column="0"
               Content="{lex:Loc Key={x:Static loc:ResourceKeys.HeaderPagedSystemMemorySize}}"
               AutomationProperties.AutomationId="Label28_9F96CDD5" />
        <TextBlock Grid.Row="7"
                   Grid.Column="1"
                   Text="{Binding PagedSystemMemorySize64}"
                   AutomationProperties.AutomationId="TextBlock29_9F96CDD5" />
        <Label Grid.Row="8"
               Grid.Column="0"
               Content="{lex:Loc Key={x:Static loc:ResourceKeys.HeaderNonpagedSystemMemorySize}}"
               AutomationProperties.AutomationId="Label30_9F96CDD5" />
        <TextBlock Grid.Row="8"
                   Grid.Column="1"
                   Text="{Binding NonPagedSystemMemorySize64}"
                   AutomationProperties.AutomationId="TextBlock31_9F96CDD5" />
        <Label Grid.Row="9"
               Grid.Column="0"
               Content="{lex:Loc Key={x:Static loc:ResourceKeys.HeaderThreadCount}}"
               AutomationProperties.AutomationId="Label32_9F96CDD5" />
        <TextBlock Grid.Row="9"
                   Grid.Column="1"
                   Text="{Binding ThreadCount}"
                   AutomationProperties.AutomationId="TextBlock33_9F96CDD5" />
        <Label Grid.Row="10"
               Grid.Column="0"
               Content="{lex:Loc Key={x:Static loc:ResourceKeys.HeaderHandleCount}}"
               AutomationProperties.AutomationId="Label34_9F96CDD5" />
        <TextBlock Grid.Row="10"
                   Grid.Column="1"
                   Text="{Binding HandleCount}"
                   AutomationProperties.AutomationId="TextBlock35_9F96CDD5" />
    </Grid>
</operatorMenu:OperatorMenuPage>